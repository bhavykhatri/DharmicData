    "chapter": 72,
    "verse": 8
  },
  "02072009a": {
    "text": {
      "ascii": "buddhau kaluSabhUtAyAM vinAze pratyupasthite\n",
      "ud": "बुद्धौ कलुषभूतायां विनाशे प्रत्युपस्थिते\n",

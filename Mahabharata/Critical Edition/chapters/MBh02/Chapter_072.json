    "book": 2,
    "chapter": 72,
    "verse": 9
  },
  "02072009c": {
    "text": {
      "ascii": "anayo nayasaMkAzo hRdayAn nApasarpati\n",
      "ud": "अनयो नयसंकाशो हृदयान्नापसर्पति\n",
      "ur": "anayo nayasaṁkāśo hr̥dayān nāpasarpati\n"

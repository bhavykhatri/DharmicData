    "chapter": 72,
    "verse": 5
  },
  "02072005c": {
    "text": {
      "ascii": "vinAzaH sarvalokasya sAnubandho bhaviSyati\n",
      "ud": "विनाशः सर्वलोकस्य सानुबन्धो भविष्यति\n",

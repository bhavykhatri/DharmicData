    "book": 2,
    "chapter": 24,
    "verse": 4
  },
  "02024005a": {
    "text": {
      "ascii": "mRdaGgavaranAdena rathanemisvanena ca\n",
      "ud": "मृदङ्गवरनादेन रथनेमिस्वनेन च\n",
      "ur": "mr̥daṅgavaranādena rathanemisvanena ca\n"

    "chapter": 24,
    "verse": 18
  },
  "02024019a": {
    "text": {
      "ascii": "tataH siMhapuraM ramyaM citrAyudhasurakSitam\n",
      "ud": "ततः सिंहपुरं रम्यं चित्रायुधसुरक्षितम्\n",

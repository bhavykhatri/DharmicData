    "book": 2,
    "chapter": 24,
    "verse": 4
  },
  "02024004c": {
    "text": {
      "ascii": "kulUtavAsinaM rAjan bRhantam upajagmivAn\n",
      "ud": "कुलूतवासिनं राजन्बृहन्तमुपजग्मिवान्\n",

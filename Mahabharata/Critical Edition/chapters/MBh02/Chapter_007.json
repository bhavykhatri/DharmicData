    "book": 2,
    "chapter": 7,
    "verse": 6
  },
  "02007006c": {
    "text": {
      "ascii": "marutaH sarvato rAjan sarve ca gRhamedhinaH\n",
      "ud": "मरुतः सर्वतो राजन्सर्वे च गृहमेधिनः\n",
      "ur": "marutaḥ sarvato rājan sarve ca gr̥hamedhinaḥ\n"
    },
    "book": 2,
    "chapter": 7,
    "verse": 6
  },
  "02007006e": {
    "text": {
      "ascii": "siddhA devarSayaz caiva sAdhyA devagaNAs tathA\n",
      "ud": "सिद्धा देवर्षयश्चैव साध्या देवगणास्तथा\n",
      "ur": "siddhā devarṣayaś caiva sādhyā devagaṇās tathā\n"
    },
    "book": 2,
    "chapter": 7,
    "verse": 6
  },
  "02007007a": {
    "text": {
      "ascii": "ete sAnucarAH sarve divyarUpAH svalaMkRtAH\n",
      "ud": "एते सानुचराः सर्वे दिव्यरूपाः स्वलंकृताः\n",
      "ur": "ete sānucarāḥ sarve divyarūpāḥ svalaṁkr̥tāḥ\n"

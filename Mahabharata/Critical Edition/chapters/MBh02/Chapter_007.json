    "chapter": 7,
    "verse": 8
  },
  "02007008c": {
    "text": {
      "ascii": "amalA dhUtapApmAno dIpyamAnA ivAgnayaH\n",
      "ud": "अमला धूतपाप्मानो दीप्यमाना इवाग्नयः\n",

    "book": 2,
    "chapter": 7,
    "verse": 7
  },
  "02007007c": {
    "text": {
      "ascii": "upAsate mahAtmAnaM devarAjam ariMdamam\n",
      "ud": "उपासते महात्मानं देवराजमरिंदमम्\n",
      "ur": "upāsate mahātmānaṁ devarājam ariṁdamam\n"
    },
    "book": 2,
    "chapter": 7,
    "verse": 7
  },
  "02007008a": {

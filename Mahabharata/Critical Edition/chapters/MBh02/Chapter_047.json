    "chapter": 47,
    "verse": 23
  },
  "02047023c": {
    "text": {
      "ascii": "zlakSNaM vastram akArpAsam AvikaM mRdu cAjinam\n",
      "ud": "श्लक्ष्णं वस्त्रमकार्पासमाविकं मृदु चाजिनम्\n",
      "ur": "ślakṣṇaṁ vastram akārpāsam āvikaṁ mr̥du cājinam\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 23
  },
  "02047024a": {
    "text": {
      "ascii": "nizitAMz caiva dIrghAsIn RSTizaktiparazvadhAn\n",
      "ud": "निशितांश्चैव दीर्घासीनृष्टिशक्तिपरश्वधान्\n",
      "ur": "niśitāṁś caiva dīrghāsīn r̥ṣṭiśaktiparaśvadhān\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 24
  },
  "02047024c": {
    "text": {
      "ascii": "aparAntasamudbhUtAMs tathaiva parazUJ zitAn\n",
      "ud": "अपरान्तसमुद्भूतांस्तथैव परशूञ्शितान्\n",
      "ur": "aparāntasamudbhūtāṁs tathaiva paraśūñ śitān\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 24
  },
  "02047025a": {
    "text": {
      "ascii": "rasAn gandhAMz ca vividhAn ratnAni ca sahasrazaH\n",
      "ud": "रसान्गन्धांश्च विविधान्रत्नानि च सहस्रशः\n",
      "ur": "rasān gandhāṁś ca vividhān ratnāni ca sahasraśaḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 25
  },
  "02047025c": {
    "text": {
      "ascii": "baliM ca kRtsnam AdAya dvAri tiSThanti vAritAH\n",
      "ud": "बलिं च कृत्स्नमादाय द्वारि तिष्ठन्ति वारिताः\n",
      "ur": "baliṁ ca kr̥tsnam ādāya dvāri tiṣṭhanti vāritāḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 25
  },
  "02047026a": {
    "text": {
      "ascii": "zakAs tukhArAH kaGkAz ca romazAH zRGgiNo narAH\n",
      "ud": "शकास्तुखाराः कङ्काश्च रोमशाः शृङ्गिणो नराः\n",
      "ur": "śakās tukhārāḥ kaṅkāś ca romaśāḥ śr̥ṅgiṇo narāḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 26
  },
  "02047026c": {
    "text": {
      "ascii": "mahAgamAn dUragamAn gaNitAn arbudaM hayAn\n",
      "ud": "महागमान्दूरगमान्गणितानर्बुदं हयान्\n",
      "ur": "mahāgamān dūragamān gaṇitān arbudaṁ hayān\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 26
  },
  "02047027a": {
    "text": {
      "ascii": "koTizaz caiva bahuzaH suvarNaM padmasaMmitam\n",
      "ud": "कोटिशश्चैव बहुशः सुवर्णं पद्मसंमितम्\n",
      "ur": "koṭiśaś caiva bahuśaḥ suvarṇaṁ padmasaṁmitam\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 27
  },
  "02047027c": {
    "text": {
      "ascii": "balim AdAya vividhaM dvAri tiSThanti vAritAH\n",
      "ud": "बलिमादाय विविधं द्वारि तिष्ठन्ति वारिताः\n",
      "ur": "balim ādāya vividhaṁ dvāri tiṣṭhanti vāritāḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 27
  },
  "02047028a": {
    "text": {
      "ascii": "AsanAni mahArhANi yAnAni zayanAni ca\n",
      "ud": "आसनानि महार्हाणि यानानि शयनानि च\n",
      "ur": "āsanāni mahārhāṇi yānāni śayanāni ca\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 28
  },
  "02047028c": {
    "text": {
      "ascii": "maNikAJcanacitrANi gajadantamayAni ca\n",
      "ud": "मणिकाञ्चनचित्राणि गजदन्तमयानि च\n",
      "ur": "maṇikāñcanacitrāṇi gajadantamayāni ca\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 28
  },
  "02047029a": {
    "text": {
      "ascii": "rathAMz ca vividhAkArAJ jAtarUpapariSkRtAn\n",
      "ud": "रथांश्च विविधाकाराञ्जातरूपपरिष्कृतान्\n",
      "ur": "rathāṁś ca vividhākārāñ jātarūpapariṣkr̥tān\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 29
  },
  "02047029c": {
    "text": {
      "ascii": "hayair vinItaiH saMpannAn vaiyAghraparivAraNAn\n",
      "ud": "हयैर्विनीतैः संपन्नान्वैयाघ्रपरिवारणान्\n",
      "ur": "hayair vinītaiḥ saṁpannān vaiyāghraparivāraṇān\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 29
  },
  "02047030a": {
    "text": {
      "ascii": "vicitrAMz ca paristomAn ratnAni ca sahasrazaH\n",
      "ud": "विचित्रांश्च परिस्तोमान्रत्नानि च सहस्रशः\n",
      "ur": "vicitrāṁś ca paristomān ratnāni ca sahasraśaḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 30
  },
  "02047030c": {
    "text": {
      "ascii": "nArAcAn ardhanArAcAJ zastrANi vividhAni ca\n",
      "ud": "नाराचानर्धनाराचाञ्शस्त्राणि विविधानि च\n",
      "ur": "nārācān ardhanārācāñ śastrāṇi vividhāni ca\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 30
  },
  "02047031a": {
    "text": {
      "ascii": "etad dattvA mahad dravyaM pUrvadezAdhipo nRpaH\n",
      "ud": "एतद्दत्त्वा महद्द्रव्यं पूर्वदेशाधिपो नृपः\n",
      "ur": "etad dattvā mahad dravyaṁ pūrvadeśādhipo nr̥paḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 31
  },
  "02047031c": {
    "text": {
      "ascii": "praviSTo yajJasadanaM pANDavasya mahAtmanaH\n",
      "ud": "प्रविष्टो यज्ञसदनं पाण्डवस्य महात्मनः\n",
      "ur": "praviṣṭo yajñasadanaṁ pāṇḍavasya mahātmanaḥ\n"
    },
    "book": 2,
    "chapter": 47,
    "verse": 31
  }
}
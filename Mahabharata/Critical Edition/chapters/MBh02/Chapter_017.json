    "book": 2,
    "chapter": 17,
    "verse": 5
  },
  "02017005c": {
    "text": {
      "ascii": "AjJApayac ca rAkSasyA mAgadheSu mahotsavam\n",
      "ud": "आज्ञापयच्च राक्षस्या मागधेषु महोत्सवम्\n",
      "ur": "ājñāpayac ca rākṣasyā māgadheṣu mahotsavam\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 5
  },
  "02017006a": {
    "text": {
      "ascii": "tasya nAmAkarot tatra prajApatisamaH pitA\n",
      "ud": "तस्य नामाकरोत्तत्र प्रजापतिसमः पिता\n",
      "ur": "tasya nāmākarot tatra prajāpatisamaḥ pitā\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 6
  },
  "02017006c": {
    "text": {
      "ascii": "jarayA saMdhito yasmAj jarAsaMdhas tato 'bhavat\n",
      "ud": "जरया संधितो यस्माज्जरासंधस्ततोऽभवत्\n",
      "ur": "jarayā saṁdhito yasmāj jarāsaṁdhas tato ’bhavat\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 6
  },
  "02017007a": {
    "text": {
      "ascii": "so 'vardhata mahAtejA magadhAdhipateH sutaH\n",
      "ud": "सोऽवर्धत महातेजा मगधाधिपतेः सुतः\n",
      "ur": "so ’vardhata mahātejā magadhādhipateḥ sutaḥ\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 7
  },
  "02017007c": {
    "text": {
      "ascii": "pramANabalasaMpanno hutAhutir ivAnalaH\n",
      "ud": "प्रमाणबलसंपन्नो हुताहुतिरिवानलः\n",
      "ur": "pramāṇabalasaṁpanno hutāhutir ivānalaḥ\n"

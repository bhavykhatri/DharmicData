    "book": 2,
    "chapter": 17,
    "verse": 8
  },
  "02017008c": {
    "text": {
      "ascii": "magadhAn upacakrAma bhagavAMz caNDakauzikaH\n",
      "ud": "मगधानुपचक्राम भगवांश्चण्डकौशिकः\n",
      "ur": "magadhān upacakrāma bhagavāṁś caṇḍakauśikaḥ\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 8
  },
  "02017009a": {
    "text": {
      "ascii": "tasyAgamanasaMhRSTaH sAmAtyaH sapuraHsaraH\n",
      "ud": "तस्यागमनसंहृष्टः सामात्यः सपुरःसरः\n",
      "ur": "tasyāgamanasaṁhr̥ṣṭaḥ sāmātyaḥ sapuraḥsaraḥ\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 9
  },
  "02017009c": {
    "text": {
      "ascii": "sabhAryaH saha putreNa nirjagAma bRhadrathaH\n",
      "ud": "सभार्यः सह पुत्रेण निर्जगाम बृहद्रथः\n",
      "ur": "sabhāryaḥ saha putreṇa nirjagāma br̥hadrathaḥ\n"

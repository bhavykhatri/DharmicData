    "chapter": 17,
    "verse": 3
  },
  "02017004": {
    "text": {
      "ascii": "kRSNa uvAca\n",
      "ud": "कृष्ण उवाच\n",
      "ur": "kr̥ṣṇa uvāca\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 4
  },
  "02017004a": {
    "text": {
      "ascii": "evam uktvA tu sA rAjaMs tatraivAntaradhIyata\n",
      "ud": "एवमुक्त्वा तु सा राजंस्तत्रैवान्तरधीयत\n",
      "ur": "evam uktvā tu sā rājaṁs tatraivāntaradhīyata\n"
    },
    "book": 2,
    "chapter": 17,
    "verse": 4
  },
  "02017004c": {
    "text": {
      "ascii": "sa gRhya ca kumAraM taM prAvizat svagRhaM nRpaH\n",
      "ud": "स गृह्य च कुमारं तं प्राविशत्स्वगृहं नृपः\n",
      "ur": "sa gr̥hya ca kumāraṁ taṁ prāviśat svagr̥haṁ nr̥paḥ\n"

    "book": 2,
    "chapter": 31,
    "verse": 1
  },
  "02031001c": {
    "text": {
      "ascii": "bhISmam AmantrayAm Asa dhRtarASTraM ca pANDavaH\n",
      "ud": "भीष्ममामन्त्रयामास धृतराष्ट्रं च पाण्डवः\n",
      "ur": "bhīṣmam āmantrayām āsa dhr̥tarāṣṭraṁ ca pāṇḍavaḥ\n"
    },
    "book": 2,
    "chapter": 31,
    "verse": 1
  },
  "02031002a": {
    "text": {
      "ascii": "prayayuH prItamanaso yajJaM brahmapuraHsarAH\n",
      "ud": "प्रययुः प्रीतमनसो यज्ञं ब्रह्मपुरःसराः\n",
      "ur": "prayayuḥ prītamanaso yajñaṁ brahmapuraḥsarāḥ\n"

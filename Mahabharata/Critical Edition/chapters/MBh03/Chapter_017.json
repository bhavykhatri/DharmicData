    },
    "book": 3,
    "chapter": 17,
    "verse": 25
  },
  "03017025c": {
    "text": {
      "ascii": "abhimantrya mahAstreNa saMdadhe zatrunAzanam\n",
      "ud": "अभिमन्त्र्य महास्त्रेण संदधे शत्रुनाशनम्\n",
      "ur": "abhimantrya mahāstreṇa saṁdadhe śatrunāśanam\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 25
  },
  "03017026a": {
    "text": {
      "ascii": "sa vivindhyAya sakrodhaH samAhUya mahArathaH\n",
      "ud": "स विविन्ध्याय सक्रोधः समाहूय महारथः\n",
      "ur": "sa vivindhyāya sakrodhaḥ samāhūya mahārathaḥ\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 26
  },
  "03017026c": {
    "text": {
      "ascii": "cikSepa me suto rAjan sa gatAsur athApatat\n",
      "ud": "चिक्षेप मे सुतो राजन्स गतासुरथापतत्\n",
      "ur": "cikṣepa me suto rājan sa gatāsur athāpatat\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 26
  },
  "03017027a": {
    "text": {
      "ascii": "vivindhyaM nihataM dRSTvA tAM ca vikSobhitAM camUm\n",
      "ud": "विविन्ध्यं निहतं दृष्ट्वा तां च विक्षोभितां चमूम्\n",
      "ur": "vivindhyaṁ nihataṁ dr̥ṣṭvā tāṁ ca vikṣobhitāṁ camūm\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 27
  },
  "03017027c": {
    "text": {
      "ascii": "kAmagena sa saubhena zAlvaH punar upAgamat\n",
      "ud": "कामगेन स सौभेन शाल्वः पुनरुपागमत्\n",
      "ur": "kāmagena sa saubhena śālvaḥ punar upāgamat\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 27
  },
  "03017028a": {
    "text": {
      "ascii": "tato vyAkulitaM sarvaM dvArakAvAsi tad balam\n",
      "ud": "ततो व्याकुलितं सर्वं द्वारकावासि तद्बलम्\n",
      "ur": "tato vyākulitaṁ sarvaṁ dvārakāvāsi tad balam\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 28
  },
  "03017028c": {
    "text": {
      "ascii": "dRSTvA zAlvaM mahAbAho saubhasthaM pRthivIgatam\n",
      "ud": "दृष्ट्वा शाल्वं महाबाहो सौभस्थं पृथिवीगतम्\n",
      "ur": "dr̥ṣṭvā śālvaṁ mahābāho saubhasthaṁ pr̥thivīgatam\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 28
  },
  "03017029a": {
    "text": {
      "ascii": "tato niryAya kaunteya vyavasthApya ca tad balam\n",
      "ud": "ततो निर्याय कौन्तेय व्यवस्थाप्य च तद्बलम्\n",
      "ur": "tato niryāya kaunteya vyavasthāpya ca tad balam\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 29
  },
  "03017029c": {
    "text": {
      "ascii": "AnartAnAM mahArAja pradyumno vAkyam abravIt\n",
      "ud": "आनर्तानां महाराज प्रद्युम्नो वाक्यमब्रवीत्\n",
      "ur": "ānartānāṁ mahārāja pradyumno vākyam abravīt\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 29
  },
  "03017030a": {
    "text": {
      "ascii": "sarve bhavantas tiSThantu sarve pazyantu mAM yudhi\n",
      "ud": "सर्वे भवन्तस्तिष्ठन्तु सर्वे पश्यन्तु मां युधि\n",
      "ur": "sarve bhavantas tiṣṭhantu sarve paśyantu māṁ yudhi\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 30
  },
  "03017030c": {
    "text": {
      "ascii": "nivArayantaM saMgrAme balAt saubhaM sarAjakam\n",
      "ud": "निवारयन्तं संग्रामे बलात्सौभं सराजकम्\n",
      "ur": "nivārayantaṁ saṁgrāme balāt saubhaṁ sarājakam\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 30
  },
  "03017031a": {
    "text": {
      "ascii": "ahaM saubhapateH senAm Ayasair bhujagair iva\n",
      "ud": "अहं सौभपतेः सेनामायसैर्भुजगैरिव\n",
      "ur": "ahaṁ saubhapateḥ senām āyasair bhujagair iva\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 31
  },
  "03017031c": {
    "text": {
      "ascii": "dhanurbhujavinirmuktair nAzayAmy adya yAdavAH\n",
      "ud": "धनुर्भुजविनिर्मुक्तैर्नाशयाम्यद्य यादवाः\n",
      "ur": "dhanurbhujavinirmuktair nāśayāmy adya yādavāḥ\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 31
  },
  "03017032a": {
    "text": {
      "ascii": "AzvasadhvaM na bhIH kAryA saubharAD adya nazyati\n",
      "ud": "आश्वसध्वं न भीः कार्या सौभराडद्य नश्यति\n",
      "ur": "āśvasadhvaṁ na bhīḥ kāryā saubharāḍ adya naśyati\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 32
  },
  "03017032c": {
    "text": {
      "ascii": "mayAbhipanno duSTAtmA sasaubho vinaziSyati\n",
      "ud": "मयाभिपन्नो दुष्टात्मा ससौभो विनशिष्यति\n",
      "ur": "mayābhipanno duṣṭātmā sasaubho vinaśiṣyati\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 32
  },
  "03017033a": {
    "text": {
      "ascii": "evaM bruvati saMhRSTe pradyumne pANDunandana\n",
      "ud": "एवं ब्रुवति संहृष्टे प्रद्युम्ने पाण्डुनन्दन\n",
      "ur": "evaṁ bruvati saṁhr̥ṣṭe pradyumne pāṇḍunandana\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 33
  },
  "03017033c": {
    "text": {
      "ascii": "viSThitaM tad balaM vIra yuyudhe ca yathAsukham\n",
      "ud": "विष्ठितं तद्बलं वीर युयुधे च यथासुखम्\n",
      "ur": "viṣṭhitaṁ tad balaṁ vīra yuyudhe ca yathāsukham\n"
    },
    "book": 3,
    "chapter": 17,
    "verse": 33
  }
}
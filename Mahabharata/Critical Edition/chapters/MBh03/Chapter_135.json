    },
    "book": 3,
    "chapter": 135,
    "verse": 23
  },
  "03135023c": {
    "text": {
      "ascii": "bhUya evAkarod yatnaM tapasy amitavikrama\n",
      "ud": "भूय एवाकरोद्यत्नं तपस्यमितविक्रम\n",
      "ur": "bhūya evākarod yatnaṁ tapasy amitavikrama\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 23
  },
  "03135024a": {
    "text": {
      "ascii": "ghoreNa tapasA rAjaMs tapyamAno mahAtapAH\n",
      "ud": "घोरेण तपसा राजंस्तप्यमानो महातपाः\n",
      "ur": "ghoreṇa tapasā rājaṁs tapyamāno mahātapāḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 24
  },
  "03135024c": {
    "text": {
      "ascii": "saMtApayAm Asa bhRzaM devendram iti naH zrutam\n",
      "ud": "संतापयामास भृशं देवेन्द्रमिति नः श्रुतम्\n",
      "ur": "saṁtāpayām āsa bhr̥śaṁ devendram iti naḥ śrutam\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 24
  },
  "03135025a": {
    "text": {
      "ascii": "taM tathA tapyamAnaM tu tapas tIvraM mahAmunim\n",
      "ud": "तं तथा तप्यमानं तु तपस्तीव्रं महामुनिम्\n",
      "ur": "taṁ tathā tapyamānaṁ tu tapas tīvraṁ mahāmunim\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 25
  },
  "03135025c": {
    "text": {
      "ascii": "upetya balabhid devo vArayAm Asa vai punaH\n",
      "ud": "उपेत्य बलभिद्देवो वारयामास वै पुनः\n",
      "ur": "upetya balabhid devo vārayām āsa vai punaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 25
  },
  "03135026a": {
    "text": {
      "ascii": "azakyo 'rthaH samArabdho naitad buddhikRtaM tava\n",
      "ud": "अशक्योऽर्थः समारब्धो नैतद्बुद्धिकृतं तव\n",
      "ur": "aśakyo ’rthaḥ samārabdho naitad buddhikr̥taṁ tava\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 26
  },
  "03135026c": {
    "text": {
      "ascii": "pratibhAsyanti vai vedAs tava caiva pituz ca te\n",
      "ud": "प्रतिभास्यन्ति वै वेदास्तव चैव पितुश्च ते\n",
      "ur": "pratibhāsyanti vai vedās tava caiva pituś ca te\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 26
  },
  "03135027": {
    "text": {
      "ascii": "yavakrIr uvAca\n",
      "ud": "यवक्रीरुवाच\n",
      "ur": "yavakrīr uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 27
  },
  "03135027a": {
    "text": {
      "ascii": "na caitad evaM kriyate devarAja mamepsitam\n",
      "ud": "न चैतदेवं क्रियते देवराज ममेप्सितम्\n",
      "ur": "na caitad evaṁ kriyate devarāja mamepsitam\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 27
  },
  "03135027c": {
    "text": {
      "ascii": "mahatA niyamenAhaM tapsye ghorataraM tapaH\n",
      "ud": "महता नियमेनाहं तप्स्ये घोरतरं तपः\n",
      "ur": "mahatā niyamenāhaṁ tapsye ghorataraṁ tapaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 27
  },
  "03135028a": {
    "text": {
      "ascii": "samiddhe 'gnAv upakRtyAGgam aGgaM; hoSyAmi vA maghavaMs tan nibodha\n",
      "ud": "समिद्धेऽग्नावुपकृत्याङ्गमङ्गं; होष्यामि वा मघवंस्तन्निबोध\n",
      "ur": "samiddhe ’gnāv upakr̥tyāṅgam aṅgaṁ; hoṣyāmi vā maghavaṁs tan nibodha\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 28
  },
  "03135028c": {
    "text": {
      "ascii": "yady etad evaM na karoSi kAmaM; mamepsitaM devarAjeha sarvam\n",
      "ud": "यद्येतदेवं न करोषि कामं; ममेप्सितं देवराजेह सर्वम्\n",
      "ur": "yady etad evaṁ na karoṣi kāmaṁ; mamepsitaṁ devarājeha sarvam\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 28
  },
  "03135029": {
    "text": {
      "ascii": "lomaza uvAca\n",
      "ud": "लोमश उवाच\n",
      "ur": "lomaśa uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 29
  },
  "03135029a": {
    "text": {
      "ascii": "nizcayaM tam abhijJAya munes tasya mahAtmanaH\n",
      "ud": "निश्चयं तमभिज्ञाय मुनेस्तस्य महात्मनः\n",
      "ur": "niścayaṁ tam abhijñāya munes tasya mahātmanaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 29
  },
  "03135029c": {
    "text": {
      "ascii": "prativAraNahetvarthaM buddhyA saMcintya buddhimAn\n",
      "ud": "प्रतिवारणहेत्वर्थं बुद्ध्या संचिन्त्य बुद्धिमान्\n",
      "ur": "prativāraṇahetvarthaṁ buddhyā saṁcintya buddhimān\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 29
  },
  "03135030a": {
    "text": {
      "ascii": "tata indro 'karod rUpaM brAhmaNasya tapasvinaH\n",
      "ud": "तत इन्द्रोऽकरोद्रूपं ब्राह्मणस्य तपस्विनः\n",
      "ur": "tata indro ’karod rūpaṁ brāhmaṇasya tapasvinaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 30
  },
  "03135030c": {
    "text": {
      "ascii": "anekazatavarSasya durbalasya sayakSmaNaH\n",
      "ud": "अनेकशतवर्षस्य दुर्बलस्य सयक्ष्मणः\n",
      "ur": "anekaśatavarṣasya durbalasya sayakṣmaṇaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 30
  },
  "03135031a": {
    "text": {
      "ascii": "yavakrItasya yat tIrtham ucitaM zaucakarmaNi\n",
      "ud": "यवक्रीतस्य यत्तीर्थमुचितं शौचकर्मणि\n",
      "ur": "yavakrītasya yat tīrtham ucitaṁ śaucakarmaṇi\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 31
  },
  "03135031c": {
    "text": {
      "ascii": "bhAgIrathyAM tatra setuM vAlukAbhiz cakAra saH\n",
      "ud": "भागीरथ्यां तत्र सेतुं वालुकाभिश्चकार सः\n",
      "ur": "bhāgīrathyāṁ tatra setuṁ vālukābhiś cakāra saḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 31
  },
  "03135032a": {
    "text": {
      "ascii": "yadAsya vadato vAkyaM na sa cakre dvijottamaH\n",
      "ud": "यदास्य वदतो वाक्यं न स चक्रे द्विजोत्तमः\n",
      "ur": "yadāsya vadato vākyaṁ na sa cakre dvijottamaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 32
  },
  "03135032c": {
    "text": {
      "ascii": "vAlukAbhis tataH zakro gaGgAM samabhipUrayan\n",
      "ud": "वालुकाभिस्ततः शक्रो गङ्गां समभिपूरयन्\n",
      "ur": "vālukābhis tataḥ śakro gaṅgāṁ samabhipūrayan\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 32
  },
  "03135033a": {
    "text": {
      "ascii": "vAlukAmuSTim anizaM bhAgIrathyAM vyasarjayat\n",
      "ud": "वालुकामुष्टिमनिशं भागीरथ्यां व्यसर्जयत्\n",
      "ur": "vālukāmuṣṭim aniśaṁ bhāgīrathyāṁ vyasarjayat\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 33
  },
  "03135033c": {
    "text": {
      "ascii": "setum abhyArabhac chakro yavakrItaM nidarzayan\n",
      "ud": "सेतुमभ्यारभच्छक्रो यवक्रीतं निदर्शयन्\n",
      "ur": "setum abhyārabhac chakro yavakrītaṁ nidarśayan\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 33
  },
  "03135034a": {
    "text": {
      "ascii": "taM dadarza yavakrIs tu yatnavantaM nibandhane\n",
      "ud": "तं ददर्श यवक्रीस्तु यत्नवन्तं निबन्धने\n",
      "ur": "taṁ dadarśa yavakrīs tu yatnavantaṁ nibandhane\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 34
  },
  "03135034c": {
    "text": {
      "ascii": "prahasaMz cAbravId vAkyam idaM sa munipuMgavaH\n",
      "ud": "प्रहसंश्चाब्रवीद्वाक्यमिदं स मुनिपुंगवः\n",
      "ur": "prahasaṁś cābravīd vākyam idaṁ sa munipuṁgavaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 34
  },
  "03135035a": {
    "text": {
      "ascii": "kim idaM vartate brahman kiM ca te ha cikIrSitam\n",
      "ud": "किमिदं वर्तते ब्रह्मन्किं च ते ह चिकीर्षितम्\n",
      "ur": "kim idaṁ vartate brahman kiṁ ca te ha cikīrṣitam\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 35
  },
  "03135035c": {
    "text": {
      "ascii": "atIva hi mahAn yatnaH kriyate 'yaM nirarthakaH\n",
      "ud": "अतीव हि महान्यत्नः क्रियतेऽयं निरर्थकः\n",
      "ur": "atīva hi mahān yatnaḥ kriyate ’yaṁ nirarthakaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 35
  },
  "03135036": {
    "text": {
      "ascii": "indra uvAca\n",
      "ud": "इन्द्र उवाच\n",
      "ur": "indra uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 36
  },
  "03135036a": {
    "text": {
      "ascii": "bandhiSye setunA gaGgAM sukhaH panthA bhaviSyati\n",
      "ud": "बन्धिष्ये सेतुना गङ्गां सुखः पन्था भविष्यति\n",
      "ur": "bandhiṣye setunā gaṅgāṁ sukhaḥ panthā bhaviṣyati\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 36
  },
  "03135036c": {
    "text": {
      "ascii": "klizyate hi janas tAta taramANaH punaH punaH\n",
      "ud": "क्लिश्यते हि जनस्तात तरमाणः पुनः पुनः\n",
      "ur": "kliśyate hi janas tāta taramāṇaḥ punaḥ punaḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 36
  },
  "03135037": {
    "text": {
      "ascii": "yavakrIr uvAca\n",
      "ud": "यवक्रीरुवाच\n",
      "ur": "yavakrīr uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 37
  },
  "03135037a": {
    "text": {
      "ascii": "nAyaM zakyas tvayA baddhuM mahAn oghaH kathaM cana\n",
      "ud": "नायं शक्यस्त्वया बद्धुं महानोघः कथंचन\n",
      "ur": "nāyaṁ śakyas tvayā baddhuṁ mahān oghaḥ kathaṁ cana\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 37
  },
  "03135037c": {
    "text": {
      "ascii": "azakyAd vinivartasva zakyam arthaM samArabha\n",
      "ud": "अशक्याद्विनिवर्तस्व शक्यमर्थं समारभ\n",
      "ur": "aśakyād vinivartasva śakyam arthaṁ samārabha\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 37
  },
  "03135038": {
    "text": {
      "ascii": "indra uvAca\n",
      "ud": "इन्द्र उवाच\n",
      "ur": "indra uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 38
  },
  "03135038a": {
    "text": {
      "ascii": "yathaiva bhavatA cedaM tapo vedArtham udyatam\n",
      "ud": "यथैव भवता चेदं तपो वेदार्थमुद्यतम्\n",
      "ur": "yathaiva bhavatā cedaṁ tapo vedārtham udyatam\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 38
  },
  "03135038c": {
    "text": {
      "ascii": "azakyaM tadvad asmAbhir ayaM bhAraH samudyataH\n",
      "ud": "अशक्यं तद्वदस्माभिरयं भारः समुद्यतः\n",
      "ur": "aśakyaṁ tadvad asmābhir ayaṁ bhāraḥ samudyataḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 38
  },
  "03135039": {
    "text": {
      "ascii": "yavakrIr uvAca\n",
      "ud": "यवक्रीरुवाच\n",
      "ur": "yavakrīr uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 39
  },
  "03135039a": {
    "text": {
      "ascii": "yathA tava nirartho 'yam Arambhas tridazezvara\n",
      "ud": "यथा तव निरर्थोऽयमारम्भस्त्रिदशेश्वर\n",
      "ur": "yathā tava nirartho ’yam ārambhas tridaśeśvara\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 39
  },
  "03135039c": {
    "text": {
      "ascii": "tathA yadi mamApIdaM manyase pAkazAsana\n",
      "ud": "तथा यदि ममापीदं मन्यसे पाकशासन\n",
      "ur": "tathā yadi mamāpīdaṁ manyase pākaśāsana\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 39
  },
  "03135040a": {
    "text": {
      "ascii": "kriyatAM yad bhavec chakyaM mayA suragaNezvara\n",
      "ud": "क्रियतां यद्भवेच्छक्यं मया सुरगणेश्वर\n",
      "ur": "kriyatāṁ yad bhavec chakyaṁ mayā suragaṇeśvara\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 40
  },
  "03135040c": {
    "text": {
      "ascii": "varAMz ca me prayacchAnyAn yair anyAn bhavitAsmy ati\n",
      "ud": "वरांश्च मे प्रयच्छान्यान्यैरन्यान्भवितास्म्यति\n",
      "ur": "varāṁś ca me prayacchānyān yair anyān bhavitāsmy ati\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 40
  },
  "03135041": {
    "text": {
      "ascii": "lomaza uvAca\n",
      "ud": "लोमश उवाच\n",
      "ur": "lomaśa uvāca\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 41
  },
  "03135041a": {
    "text": {
      "ascii": "tasmai prAdAd varAn indra uktavAn yAn mahAtapAH\n",
      "ud": "तस्मै प्रादाद्वरानिन्द्र उक्तवान्यान्महातपाः\n",
      "ur": "tasmai prādād varān indra uktavān yān mahātapāḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 41
  },
  "03135041c": {
    "text": {
      "ascii": "pratibhAsyanti te vedAH pitrA saha yathepsitAH\n",
      "ud": "प्रतिभास्यन्ति ते वेदाः पित्रा सह यथेप्सिताः\n",
      "ur": "pratibhāsyanti te vedāḥ pitrā saha yathepsitāḥ\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 41
  },
  "03135042a": {
    "text": {
      "ascii": "yac cAnyat kAGkSase kAmaM yavakrIr gamyatAm iti\n",
      "ud": "यच्चान्यत्काङ्क्षसे कामं यवक्रीर्गम्यतामिति\n",
      "ur": "yac cānyat kāṅkṣase kāmaṁ yavakrīr gamyatām iti\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 42
  },
  "03135042c": {
    "text": {
      "ascii": "sa labdhakAmaH pitaram upetyAtha tato 'bravIt\n",
      "ud": "स लब्धकामः पितरमुपेत्याथ ततोऽब्रवीत्\n",
      "ur": "sa labdhakāmaḥ pitaram upetyātha tato ’bravīt\n"
    },
    "book": 3,
    "chapter": 135,
    "verse": 42
  }
}
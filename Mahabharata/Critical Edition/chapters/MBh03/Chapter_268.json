    "book": 3,
    "chapter": 268,
    "verse": 10
  },
  "03268011a": {
    "text": {
      "ascii": "akRtAtmAnam AsAdya rAjAnam anaye ratam\n",
      "ud": "अकृतात्मानमासाद्य राजानमनये रतम्\n",

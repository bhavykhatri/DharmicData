    },
    "book": 3,
    "chapter": 268,
    "verse": 24
  },
  "03268025a": {
    "text": {
      "ascii": "karabhAruNagAtrANAM harINAM yuddhazAlinAm\n",
      "ud": "करभारुणगात्राणां हरीणां युद्धशालिनाम्\n",
      "ur": "karabhāruṇagātrāṇāṁ harīṇāṁ yuddhaśālinām\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 25
  },
  "03268025c": {
    "text": {
      "ascii": "koTIzatasahasreNa laGkAm abhyapatat tadA\n",
      "ud": "कोटीशतसहस्रेण लङ्कामभ्यपतत्तदा\n",
      "ur": "koṭīśatasahasreṇa laṅkām abhyapatat tadā\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 25
  },
  "03268026a": {
    "text": {
      "ascii": "utpatadbhiH patadbhiz ca nipatadbhiz ca vAnaraiH\n",
      "ud": "उत्पतद्भिः पतद्भिश्च निपतद्भिश्च वानरैः\n",
      "ur": "utpatadbhiḥ patadbhiś ca nipatadbhiś ca vānaraiḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 26
  },
  "03268026c": {
    "text": {
      "ascii": "nAdRzyata tadA sUryo rajasA nAzitaprabhaH\n",
      "ud": "नादृश्यत तदा सूर्यो रजसा नाशितप्रभः\n",
      "ur": "nādr̥śyata tadā sūryo rajasā nāśitaprabhaḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 26
  },
  "03268027a": {
    "text": {
      "ascii": "zAliprasUnasadRzaiH zirISakusumaprabhaiH\n",
      "ud": "शालिप्रसूनसदृशैः शिरीषकुसुमप्रभैः\n",
      "ur": "śāliprasūnasadr̥śaiḥ śirīṣakusumaprabhaiḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 27
  },
  "03268027c": {
    "text": {
      "ascii": "taruNAdityasadRzaiH zaragauraiz ca vAnaraiH\n",
      "ud": "तरुणादित्यसदृशैः शरगौरैश्च वानरैः\n",
      "ur": "taruṇādityasadr̥śaiḥ śaragauraiś ca vānaraiḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 27
  },
  "03268028a": {
    "text": {
      "ascii": "prAkAraM dadRzus te tu samantAt kapilIkRtam\n",
      "ud": "प्राकारं ददृशुस्ते तु समन्तात्कपिलीकृतम्\n",
      "ur": "prākāraṁ dadr̥śus te tu samantāt kapilīkr̥tam\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 28
  },
  "03268028c": {
    "text": {
      "ascii": "rAkSasA vismitA rAjan sastrIvRddhAH samantataH\n",
      "ud": "राक्षसा विस्मिता राजन्सस्त्रीवृद्धाः समन्ततः\n",
      "ur": "rākṣasā vismitā rājan sastrīvr̥ddhāḥ samantataḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 28
  },
  "03268029a": {
    "text": {
      "ascii": "bibhidus te maNistambhAn karNATTazikharANi ca\n",
      "ud": "बिभिदुस्ते मणिस्तम्भान्कर्णाट्टशिखराणि च\n",
      "ur": "bibhidus te maṇistambhān karṇāṭṭaśikharāṇi ca\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 29
  },
  "03268029c": {
    "text": {
      "ascii": "bhagnonmathitavegAni yantrANi ca vicikSipuH\n",
      "ud": "भग्नोन्मथितवेगानि यन्त्राणि च विचिक्षिपुः\n",
      "ur": "bhagnonmathitavegāni yantrāṇi ca vicikṣipuḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 29
  },
  "03268030a": {
    "text": {
      "ascii": "parigRhya zataghnIz ca sacakrAH sahuDopalAH\n",
      "ud": "परिगृह्य शतघ्नीश्च सचक्राः सहुडोपलाः\n",
      "ur": "parigr̥hya śataghnīś ca sacakrāḥ sahuḍopalāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 30
  },
  "03268030c": {
    "text": {
      "ascii": "cikSipur bhujavegena laGkAmadhye mahAbalAH\n",
      "ud": "चिक्षिपुर्भुजवेगेन लङ्कामध्ये महाबलाः\n",
      "ur": "cikṣipur bhujavegena laṅkāmadhye mahābalāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 30
  },
  "03268031a": {
    "text": {
      "ascii": "prAkArasthAz ca ye ke cin nizAcaragaNAs tadA\n",
      "ud": "प्राकारस्थाश्च ये केचिन्निशाचरगणास्तदा\n",
      "ur": "prākārasthāś ca ye ke cin niśācaragaṇās tadā\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 31
  },
  "03268031c": {
    "text": {
      "ascii": "pradudruvus te zatazaH kapibhiH samabhidrutAH\n",
      "ud": "प्रदुद्रुवुस्ते शतशः कपिभिः समभिद्रुताः\n",
      "ur": "pradudruvus te śataśaḥ kapibhiḥ samabhidrutāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 31
  },
  "03268032a": {
    "text": {
      "ascii": "tatas tu rAjavacanAd rAkSasAH kAmarUpiNaH\n",
      "ud": "ततस्तु राजवचनाद्राक्षसाः कामरूपिणः\n",
      "ur": "tatas tu rājavacanād rākṣasāḥ kāmarūpiṇaḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 32
  },
  "03268032c": {
    "text": {
      "ascii": "niryayur vikRtAkArAH sahasrazatasaMghazaH\n",
      "ud": "निर्ययुर्विकृताकाराः सहस्रशतसंघशः\n",
      "ur": "niryayur vikr̥tākārāḥ sahasraśatasaṁghaśaḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 32
  },
  "03268033a": {
    "text": {
      "ascii": "zastravarSANi varSanto drAvayanto vanaukasaH\n",
      "ud": "शस्त्रवर्षाणि वर्षन्तो द्रावयन्तो वनौकसः\n",
      "ur": "śastravarṣāṇi varṣanto drāvayanto vanaukasaḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 33
  },
  "03268033c": {
    "text": {
      "ascii": "prAkAraM zodhayantas te paraM vikramam AsthitAH\n",
      "ud": "प्राकारं शोधयन्तस्ते परं विक्रममास्थिताः\n",
      "ur": "prākāraṁ śodhayantas te paraṁ vikramam āsthitāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 33
  },
  "03268034a": {
    "text": {
      "ascii": "sa mASarAzisadRzair babhUva kSaNadAcaraiH\n",
      "ud": "स माषराशिसदृशैर्बभूव क्षणदाचरैः\n",
      "ur": "sa māṣarāśisadr̥śair babhūva kṣaṇadācaraiḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 34
  },
  "03268034c": {
    "text": {
      "ascii": "kRto nirvAnaro bhUyaH prAkAro bhImadarzanaiH\n",
      "ud": "कृतो निर्वानरो भूयः प्राकारो भीमदर्शनैः\n",
      "ur": "kr̥to nirvānaro bhūyaḥ prākāro bhīmadarśanaiḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 34
  },
  "03268035a": {
    "text": {
      "ascii": "petuH zUlavibhinnAGgA bahavo vAnararSabhAH\n",
      "ud": "पेतुः शूलविभिन्नाङ्गा बहवो वानरर्षभाः\n",
      "ur": "petuḥ śūlavibhinnāṅgā bahavo vānararṣabhāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 35
  },
  "03268035c": {
    "text": {
      "ascii": "stambhatoraNabhagnAz ca petus tatra nizAcarAH\n",
      "ud": "स्तम्भतोरणभग्नाश्च पेतुस्तत्र निशाचराः\n",
      "ur": "stambhatoraṇabhagnāś ca petus tatra niśācarāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 35
  },
  "03268036a": {
    "text": {
      "ascii": "kezAkezy abhavad yuddhaM rakSasAM vAnaraiH saha\n",
      "ud": "केशाकेश्यभवद्युद्धं रक्षसां वानरैः सह\n",
      "ur": "keśākeśy abhavad yuddhaṁ rakṣasāṁ vānaraiḥ saha\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 36
  },
  "03268036c": {
    "text": {
      "ascii": "nakhair dantaiz ca vIrANAM khAdatAM vai parasparam\n",
      "ud": "नखैर्दन्तैश्च वीराणां खादतां वै परस्परम्\n",
      "ur": "nakhair dantaiś ca vīrāṇāṁ khādatāṁ vai parasparam\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 36
  },
  "03268037a": {
    "text": {
      "ascii": "niSTananto hy ubhayatas tatra vAnararAkSasAH\n",
      "ud": "निष्टनन्तो ह्युभयतस्तत्र वानरराक्षसाः\n",
      "ur": "niṣṭananto hy ubhayatas tatra vānararākṣasāḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 37
  },
  "03268037c": {
    "text": {
      "ascii": "hatA nipatitA bhUmau na muJcanti parasparam\n",
      "ud": "हता निपतिता भूमौ न मुञ्चन्ति परस्परम्\n",
      "ur": "hatā nipatitā bhūmau na muñcanti parasparam\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 37
  },
  "03268038a": {
    "text": {
      "ascii": "rAmas tu zarajAlAni vavarSa jalado yathA\n",
      "ud": "रामस्तु शरजालानि ववर्ष जलदो यथा\n",
      "ur": "rāmas tu śarajālāni vavarṣa jalado yathā\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 38
  },
  "03268038c": {
    "text": {
      "ascii": "tAni laGkAM samAsAdya jaghnus tAn rajanIcarAn\n",
      "ud": "तानि लङ्कां समासाद्य जघ्नुस्तान्रजनीचरान्\n",
      "ur": "tāni laṅkāṁ samāsādya jaghnus tān rajanīcarān\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 38
  },
  "03268039a": {
    "text": {
      "ascii": "saumitrir api nArAcair dRDhadhanvA jitaklamaH\n",
      "ud": "सौमित्रिरपि नाराचैर्दृढधन्वा जितक्लमः\n",
      "ur": "saumitrir api nārācair dr̥ḍhadhanvā jitaklamaḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 39
  },
  "03268039c": {
    "text": {
      "ascii": "AdizyAdizya durgasthAn pAtayAm Asa rAkSasAn\n",
      "ud": "आदिश्यादिश्य दुर्गस्थान्पातयामास राक्षसान्\n",
      "ur": "ādiśyādiśya durgasthān pātayām āsa rākṣasān\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 39
  },
  "03268040a": {
    "text": {
      "ascii": "tataH pratyavahAro 'bhUt sainyAnAM rAghavAjJayA\n",
      "ud": "ततः प्रत्यवहारोऽभूत्सैन्यानां राघवाज्ञया\n",
      "ur": "tataḥ pratyavahāro ’bhūt sainyānāṁ rāghavājñayā\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 40
  },
  "03268040c": {
    "text": {
      "ascii": "kRte vimarde laGkAyAM labdhalakSo jayottaraH\n",
      "ud": "कृते विमर्दे लङ्कायां लब्धलक्षो जयोत्तरः\n",
      "ur": "kr̥te vimarde laṅkāyāṁ labdhalakṣo jayottaraḥ\n"
    },
    "book": 3,
    "chapter": 268,
    "verse": 40
  }
}
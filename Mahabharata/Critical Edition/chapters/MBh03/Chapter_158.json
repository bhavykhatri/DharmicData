    "book": 3,
    "chapter": 158,
    "verse": 14
  },
  "03158015a": {
    "text": {
      "ascii": "te javena mahAvegAH prApya vaizravaNAlayam\n",
      "ud": "ते जवेन महावेगाः प्राप्य वैश्रवणालयम्\n",
      "ur": "te javena mahāvegāḥ prāpya vaiśravaṇālayam\n"
    },
    "book": 3,
    "chapter": 158,
    "verse": 15
  },
  "03158015c": {
    "text": {

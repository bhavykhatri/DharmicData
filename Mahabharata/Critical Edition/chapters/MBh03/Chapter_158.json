    "book": 3,
    "chapter": 158,
    "verse": 14
  },
  "03158014c": {
    "text": {
      "ascii": "sahitAH pratyapadyanta kuberasadanaM prati\n",
      "ud": "सहिताः प्रत्यपद्यन्त कुबेरसदनं प्रति\n",
      "ur": "sahitāḥ pratyapadyanta kuberasadanaṁ prati\n"

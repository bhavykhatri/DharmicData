    "book": 3,
    "chapter": 88,
    "verse": 28
  },
  "03088028c": {
    "text": {
      "ascii": "kIrtitAni narazreSTha tIrthAny AyatanAni ca\n",
      "ud": "कीर्तितानि नरश्रेष्ठ तीर्थान्यायतनानि च\n",
      "ur": "kīrtitāni naraśreṣṭha tīrthāny āyatanāni ca\n"
    },
    "book": 3,
    "chapter": 88,
    "verse": 28
  },
  "03088029a": {
    "text": {
      "ascii": "etAni vasubhiH sAdhyair Adityair marudazvibhiH\n",
      "ud": "एतानि वसुभिः साध्यैरादित्यैर्मरुदश्विभिः\n",
      "ur": "etāni vasubhiḥ sādhyair ādityair marudaśvibhiḥ\n"
    },
    "book": 3,
    "chapter": 88,
    "verse": 29
  },
  "03088029c": {
    "text": {
      "ascii": "RSibhir brahmakalpaiz ca sevitAni mahAtmabhiH\n",
      "ud": "ऋषिभिर्ब्रह्मकल्पैश्च सेवितानि महात्मभिः\n",
      "ur": "r̥ṣibhir brahmakalpaiś ca sevitāni mahātmabhiḥ\n"

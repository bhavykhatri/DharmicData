    "book": 3,
    "chapter": 156,
    "verse": 15
  },
  "03156015c": {
    "text": {
      "ascii": "juSante parvatazreSTham RSayaH parvasaMdhiSu\n",
      "ud": "जुषन्ते पर्वतश्रेष्ठमृषयः पर्वसंधिषु\n",
      "ur": "juṣante parvataśreṣṭham r̥ṣayaḥ parvasaṁdhiṣu\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 15
  },
  "03156016a": {
    "text": {
      "ascii": "kAminaH saha kAntAbhiH parasparam anuvratAH\n",
      "ud": "कामिनः सह कान्ताभिः परस्परमनुव्रताः\n",
      "ur": "kāminaḥ saha kāntābhiḥ parasparam anuvratāḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 16
  },
  "03156016c": {
    "text": {
      "ascii": "dRzyante zailazRGgasthAs tathA kiMpuruSA nRpa\n",
      "ud": "दृश्यन्ते शैलशृङ्गस्थास्तथा किंपुरुषा नृप\n",
      "ur": "dr̥śyante śailaśr̥ṅgasthās tathā kiṁpuruṣā nr̥pa\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 16
  },
  "03156017a": {
    "text": {
      "ascii": "arajAMsi ca vAsAMsi vasAnAH kauzikAni ca\n",
      "ud": "अरजांसि च वासांसि वसानाः कौशिकानि च\n",
      "ur": "arajāṁsi ca vāsāṁsi vasānāḥ kauśikāni ca\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 17
  },
  "03156017c": {
    "text": {
      "ascii": "dRzyante bahavaH pArtha gandharvApsarasAM gaNAH\n",
      "ud": "दृश्यन्ते बहवः पार्थ गन्धर्वाप्सरसां गणाः\n",
      "ur": "dr̥śyante bahavaḥ pārtha gandharvāpsarasāṁ gaṇāḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 17
  },
  "03156018a": {
    "text": {
      "ascii": "vidyAdharagaNAz caiva sragviNaH priyadarzanAH\n",
      "ud": "विद्याधरगणाश्चैव स्रग्विणः प्रियदर्शनाः\n",
      "ur": "vidyādharagaṇāś caiva sragviṇaḥ priyadarśanāḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 18
  },
  "03156018c": {
    "text": {
      "ascii": "mahoragagaNAz caiva suparNAz coragAdayaH\n",
      "ud": "महोरगगणाश्चैव सुपर्णाश्चोरगादयः\n",
      "ur": "mahoragagaṇāś caiva suparṇāś coragādayaḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 18
  },
  "03156019a": {
    "text": {
      "ascii": "asya copari zailasya zrUyate parvasaMdhiSu\n",
      "ud": "अस्य चोपरि शैलस्य श्रूयते पर्वसंधिषु\n",
      "ur": "asya copari śailasya śrūyate parvasaṁdhiṣu\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 19
  },
  "03156019c": {
    "text": {
      "ascii": "bherIpaNavazaGkhAnAM mRdaGgAnAM ca nisvanaH\n",
      "ud": "भेरीपणवशङ्खानां मृदङ्गानां च निस्वनः\n",
      "ur": "bherīpaṇavaśaṅkhānāṁ mr̥daṅgānāṁ ca nisvanaḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 19
  },
  "03156020a": {
    "text": {
      "ascii": "ihasthair eva tat sarvaM zrotavyaM bharatarSabhAH\n",
      "ud": "इहस्थैरेव तत्सर्वं श्रोतव्यं भरतर्षभाः\n",
      "ur": "ihasthair eva tat sarvaṁ śrotavyaṁ bharatarṣabhāḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 20
  },
  "03156020c": {
    "text": {
      "ascii": "na kAryA vaH kathaM cit syAt tatrAbhisaraNe matiH\n",
      "ud": "न कार्या वः कथंचित्स्यात्तत्राभिसरणे मतिः\n",
      "ur": "na kāryā vaḥ kathaṁ cit syāt tatrābhisaraṇe matiḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 20
  },
  "03156021a": {
    "text": {
      "ascii": "na cApy ataH paraM zakyaM gantuM bharatasattamAH\n",
      "ud": "न चाप्यतः परं शक्यं गन्तुं भरतसत्तमाः\n",

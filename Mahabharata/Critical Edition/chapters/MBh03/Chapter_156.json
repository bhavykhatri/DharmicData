    },
    "book": 3,
    "chapter": 156,
    "verse": 22
  },
  "03156022c": {
    "text": {
      "ascii": "dviSanti sarvabhUtAni tADayanti ca rAkSasAH\n",
      "ud": "द्विषन्ति सर्वभूतानि ताडयन्ति च राक्षसाः\n",
      "ur": "dviṣanti sarvabhūtāni tāḍayanti ca rākṣasāḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 22
  },
  "03156023a": {
    "text": {
      "ascii": "abhyatikramya zikharaM zailasyAsya yudhiSThira\n",
      "ud": "अभ्यतिक्रम्य शिखरं शैलस्यास्य युधिष्ठिर\n",
      "ur": "abhyatikramya śikharaṁ śailasyāsya yudhiṣṭhira\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 23
  },
  "03156023c": {
    "text": {
      "ascii": "gatiH paramasiddhAnAM devarSINAM prakAzate\n",
      "ud": "गतिः परमसिद्धानां देवर्षीणां प्रकाशते\n",
      "ur": "gatiḥ paramasiddhānāṁ devarṣīṇāṁ prakāśate\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 23
  },
  "03156024a": {
    "text": {
      "ascii": "cApalAd iha gacchantaM pArtha yAnam ataH param\n",
      "ud": "चापलादिह गच्छन्तं पार्थ यानमतः परम्\n",
      "ur": "cāpalād iha gacchantaṁ pārtha yānam ataḥ param\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 24
  },
  "03156024c": {
    "text": {
      "ascii": "ayaHzUlAdibhir ghnanti rAkSasAH zatrusUdana\n",
      "ud": "अयःशूलादिभिर्घ्नन्ति राक्षसाः शत्रुसूदन\n",
      "ur": "ayaḥśūlādibhir ghnanti rākṣasāḥ śatrusūdana\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 24
  },
  "03156025a": {
    "text": {
      "ascii": "apsarobhiH parivRtaH samRddhyA naravAhanaH\n",
      "ud": "अप्सरोभिः परिवृतः समृद्ध्या नरवाहनः\n",
      "ur": "apsarobhiḥ parivr̥taḥ samr̥ddhyā naravāhanaḥ\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 25
  },
  "03156025c": {
    "text": {
      "ascii": "iha vaizravaNas tAta parvasaMdhiSu dRzyate\n",
      "ud": "इह वैश्रवणस्तात पर्वसंधिषु दृश्यते\n",
      "ur": "iha vaiśravaṇas tāta parvasaṁdhiṣu dr̥śyate\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 25
  },
  "03156026a": {
    "text": {
      "ascii": "zikhare taM samAsInam adhipaM sarvarakSasAm\n",
      "ud": "शिखरे तं समासीनमधिपं सर्वरक्षसाम्\n",
      "ur": "śikhare taṁ samāsīnam adhipaṁ sarvarakṣasām\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 26
  },
  "03156026c": {
    "text": {
      "ascii": "prekSante sarvabhUtAni bhAnumantam ivoditam\n",
      "ud": "प्रेक्षन्ते सर्वभूतानि भानुमन्तमिवोदितम्\n",
      "ur": "prekṣante sarvabhūtāni bhānumantam ivoditam\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 26
  },
  "03156027a": {
    "text": {
      "ascii": "devadAnavasiddhAnAM tathA vaizravaNasya ca\n",
      "ud": "देवदानवसिद्धानां तथा वैश्रवणस्य च\n",
      "ur": "devadānavasiddhānāṁ tathā vaiśravaṇasya ca\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 27
  },
  "03156027c": {
    "text": {
      "ascii": "gireH zikharam udyAnam idaM bharatasattama\n",
      "ud": "गिरेः शिखरमुद्यानमिदं भरतसत्तम\n",
      "ur": "gireḥ śikharam udyānam idaṁ bharatasattama\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 27
  },
  "03156028a": {
    "text": {
      "ascii": "upAsInasya dhanadaM tumburoH parvasaMdhiSu\n",
      "ud": "उपासीनस्य धनदं तुम्बुरोः पर्वसंधिषु\n",
      "ur": "upāsīnasya dhanadaṁ tumburoḥ parvasaṁdhiṣu\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 28
  },
  "03156028c": {
    "text": {
      "ascii": "gItasAmasvanas tAta zrUyate gandhamAdane\n",
      "ud": "गीतसामस्वनस्तात श्रूयते गन्धमादने\n",
      "ur": "gītasāmasvanas tāta śrūyate gandhamādane\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 28
  },
  "03156029a": {
    "text": {
      "ascii": "etad evaMvidhaM citram iha tAta yudhiSThira\n",
      "ud": "एतदेवंविधं चित्रमिह तात युधिष्ठिर\n",
      "ur": "etad evaṁvidhaṁ citram iha tāta yudhiṣṭhira\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 29
  },
  "03156029c": {
    "text": {
      "ascii": "prekSante sarvabhUtAni bahuzaH parvasaMdhiSu\n",
      "ud": "प्रेक्षन्ते सर्वभूतानि बहुशः पर्वसंधिषु\n",
      "ur": "prekṣante sarvabhūtāni bahuśaḥ parvasaṁdhiṣu\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 29
  },
  "03156030a": {
    "text": {
      "ascii": "bhuJjAnAH sarvabhojyAni rasavanti phalAni ca\n",
      "ud": "भुञ्जानाः सर्वभोज्यानि रसवन्ति फलानि च\n",
      "ur": "bhuñjānāḥ sarvabhojyāni rasavanti phalāni ca\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 30
  },
  "03156030c": {
    "text": {
      "ascii": "vasadhvaM pANDavazreSThA yAvad arjunadarzanam\n",
      "ud": "वसध्वं पाण्डवश्रेष्ठा यावदर्जुनदर्शनम्\n",
      "ur": "vasadhvaṁ pāṇḍavaśreṣṭhā yāvad arjunadarśanam\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 30
  },
  "03156031a": {
    "text": {
      "ascii": "na tAta capalair bhAvyam iha prAptaiH kathaM cana\n",
      "ud": "न तात चपलैर्भाव्यमिह प्राप्तैः कथंचन\n",
      "ur": "na tāta capalair bhāvyam iha prāptaiḥ kathaṁ cana\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 31
  },
  "03156031c": {
    "text": {
      "ascii": "uSitveha yathAkAmaM yathAzraddhaM vihRtya ca\n",
      "ud": "उषित्वेह यथाकामं यथाश्रद्धं विहृत्य च\n",
      "ur": "uṣitveha yathākāmaṁ yathāśraddhaṁ vihr̥tya ca\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 31
  },
  "03156031e": {
    "text": {
      "ascii": "tataH zastrabhRtAM zreSTha pRthivIM pAlayiSyasi\n",
      "ud": "ततः शस्त्रभृतां श्रेष्ठ पृथिवीं पालयिष्यसि\n",
      "ur": "tataḥ śastrabhr̥tāṁ śreṣṭha pr̥thivīṁ pālayiṣyasi\n"
    },
    "book": 3,
    "chapter": 156,
    "verse": 31
  }
}
{
  "03143001": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 1
  },
  "03143001a": {
    "text": {
      "ascii": "te zUrAs tatadhanvAnas tUNavantaH samArgaNAH\n",
      "ud": "ते शूरास्ततधन्वानस्तूणवन्तः समार्गणाः\n",
      "ur": "te śūrās tatadhanvānas tūṇavantaḥ samārgaṇāḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 1
  },
  "03143001c": {
    "text": {
      "ascii": "baddhagodhAGgulitrANAH khaDgavanto 'mitaujasaH\n",
      "ud": "बद्धगोधाङ्गुलित्राणाः खड्गवन्तोऽमितौजसः\n",
      "ur": "baddhagodhāṅgulitrāṇāḥ khaḍgavanto ’mitaujasaḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 1
  },
  "03143002a": {
    "text": {
      "ascii": "parigRhya dvijazreSThAJ zreSThAH sarvadhanuSmatAm\n",
      "ud": "परिगृह्य द्विजश्रेष्ठाञ्श्रेष्ठाः सर्वधनुष्मताम्\n",
      "ur": "parigr̥hya dvijaśreṣṭhāñ śreṣṭhāḥ sarvadhanuṣmatām\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 2
  },
  "03143002c": {
    "text": {
      "ascii": "pAJcAlIsahitA rAjan prayayur gandhamAdanam\n",
      "ud": "पाञ्चालीसहिता राजन्प्रययुर्गन्धमादनम्\n",
      "ur": "pāñcālīsahitā rājan prayayur gandhamādanam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 2
  },
  "03143003a": {
    "text": {
      "ascii": "sarAMsi saritaz caiva parvatAMz ca vanAni ca\n",
      "ud": "सरांसि सरितश्चैव पर्वतांश्च वनानि च\n",
      "ur": "sarāṁsi saritaś caiva parvatāṁś ca vanāni ca\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 3
  },
  "03143003c": {
    "text": {
      "ascii": "vRkSAMz ca bahulacchAyAn dadRzur girimUrdhani\n",
      "ud": "वृक्षांश्च बहुलच्छायान्ददृशुर्गिरिमूर्धनि\n",
      "ur": "vr̥kṣāṁś ca bahulacchāyān dadr̥śur girimūrdhani\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 3
  },
  "03143003e": {
    "text": {
      "ascii": "nityapuSpaphalAn dezAn devarSigaNasevitAn\n",
      "ud": "नित्यपुष्पफलान्देशान्देवर्षिगणसेवितान्\n",
      "ur": "nityapuṣpaphalān deśān devarṣigaṇasevitān\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 3
  },
  "03143004a": {
    "text": {
      "ascii": "Atmany AtmAnam AdhAya vIrA mUlaphalAzanAH\n",
      "ud": "आत्मन्यात्मानमाधाय वीरा मूलफलाशनाः\n",
      "ur": "ātmany ātmānam ādhāya vīrā mūlaphalāśanāḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 4
  },
  "03143004c": {
    "text": {
      "ascii": "cerur uccAvacAkArAn dezAn viSamasaMkaTAn\n",
      "ud": "चेरुरुच्चावचाकारान्देशान्विषमसंकटान्\n",
      "ur": "cerur uccāvacākārān deśān viṣamasaṁkaṭān\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 4
  },
  "03143004e": {
    "text": {
      "ascii": "pazyanto mRgajAtAni bahUni vividhAni ca\n",
      "ud": "पश्यन्तो मृगजातानि बहूनि विविधानि च\n",
      "ur": "paśyanto mr̥gajātāni bahūni vividhāni ca\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 4
  },
  "03143005a": {
    "text": {
      "ascii": "RSisiddhAmarayutaM gandharvApsarasAM priyam\n",
      "ud": "ऋषिसिद्धामरयुतं गन्धर्वाप्सरसां प्रियम्\n",
      "ur": "r̥ṣisiddhāmarayutaṁ gandharvāpsarasāṁ priyam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 5
  },
  "03143005c": {
    "text": {
      "ascii": "vivizus te mahAtmAnaH kiMnarAcaritaM girim\n",
      "ud": "विविशुस्ते महात्मानः किंनराचरितं गिरिम्\n",
      "ur": "viviśus te mahātmānaḥ kiṁnarācaritaṁ girim\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 5
  },
  "03143006a": {
    "text": {
      "ascii": "pravizatsv atha vIreSu parvataM gandhamAdanam\n",
      "ud": "प्रविशत्स्वथ वीरेषु पर्वतं गन्धमादनम्\n",
      "ur": "praviśatsv atha vīreṣu parvataṁ gandhamādanam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 6
  },
  "03143006c": {
    "text": {
      "ascii": "caNDavAtaM mahad varSaM prAdurAsId vizAM pate\n",
      "ud": "चण्डवातं महद्वर्षं प्रादुरासीद्विशां पते\n",
      "ur": "caṇḍavātaṁ mahad varṣaṁ prādurāsīd viśāṁ pate\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 6
  },
  "03143007a": {
    "text": {
      "ascii": "tato reNuH samudbhUtaH sapatrabahulo mahAn\n",
      "ud": "ततो रेणुः समुद्भूतः सपत्रबहुलो महान्\n",
      "ur": "tato reṇuḥ samudbhūtaḥ sapatrabahulo mahān\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 7
  },
  "03143007c": {
    "text": {
      "ascii": "pRthivIM cAntarikSaM ca dyAM caiva tamasAvRNot\n",
      "ud": "पृथिवीं चान्तरिक्षं च द्यां चैव तमसावृणोत्\n",
      "ur": "pr̥thivīṁ cāntarikṣaṁ ca dyāṁ caiva tamasāvr̥ṇot\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 7
  },
  "03143008a": {
    "text": {
      "ascii": "na sma prajJAyate kiM cid AvRte vyomni reNunA\n",
      "ud": "न स्म प्रज्ञायते किंचिदावृते व्योम्नि रेणुना\n",
      "ur": "na sma prajñāyate kiṁ cid āvr̥te vyomni reṇunā\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 8
  },
  "03143008c": {
    "text": {
      "ascii": "na cApi zekus te kartum anyonyasyAbhibhASaNam\n",
      "ud": "न चापि शेकुस्ते कर्तुमन्योन्यस्याभिभाषणम्\n",
      "ur": "na cāpi śekus te kartum anyonyasyābhibhāṣaṇam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 8
  },
  "03143009a": {
    "text": {
      "ascii": "na cApazyanta te 'nyonyaM tamasA hatacakSuSaH\n",
      "ud": "न चापश्यन्त तेऽन्योन्यं तमसा हतचक्षुषः\n",
      "ur": "na cāpaśyanta te ’nyonyaṁ tamasā hatacakṣuṣaḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 9
  },
  "03143009c": {
    "text": {
      "ascii": "AkRSyamANA vAtena sAzmacUrNena bhArata\n",
      "ud": "आकृष्यमाणा वातेन साश्मचूर्णेन भारत\n",
      "ur": "ākr̥ṣyamāṇā vātena sāśmacūrṇena bhārata\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 9
  },
  "03143010a": {
    "text": {
      "ascii": "drumANAM vAtabhagnAnAM patatAM bhUtale bhRzam\n",
      "ud": "द्रुमाणां वातभग्नानां पततां भूतले भृशम्\n",
      "ur": "drumāṇāṁ vātabhagnānāṁ patatāṁ bhūtale bhr̥śam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 10
  },
  "03143010c": {
    "text": {
      "ascii": "anyeSAM ca mahIjAnAM zabdaH samabhavan mahAn\n",
      "ud": "अन्येषां च महीजानां शब्दः समभवन्महान्\n",
      "ur": "anyeṣāṁ ca mahījānāṁ śabdaḥ samabhavan mahān\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 10
  },
  "03143011a": {
    "text": {
      "ascii": "dyauH svit patati kiM bhUmau dIryante parvatA nu kim\n",
      "ud": "द्यौः स्वित्पतति किं भूमौ दीर्यन्ते पर्वता नु किम्\n",
      "ur": "dyauḥ svit patati kiṁ bhūmau dīryante parvatā nu kim\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 11
  },
  "03143011c": {
    "text": {
      "ascii": "iti te menire sarve pavanena vimohitAH\n",
      "ud": "इति ते मेनिरे सर्वे पवनेन विमोहिताः\n",
      "ur": "iti te menire sarve pavanena vimohitāḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 11
  },
  "03143012a": {
    "text": {
      "ascii": "te yathAnantarAn vRkSAn valmIkAn viSamANi ca\n",
      "ud": "ते यथानन्तरान्वृक्षान्वल्मीकान्विषमाणि च\n",
      "ur": "te yathānantarān vr̥kṣān valmīkān viṣamāṇi ca\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 12
  },
  "03143012c": {
    "text": {
      "ascii": "pANibhiH parimArganto bhItA vAyor nililyire\n",
      "ud": "पाणिभिः परिमार्गन्तो भीता वायोर्निलिल्यिरे\n",
      "ur": "pāṇibhiḥ parimārganto bhītā vāyor nililyire\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 12
  },
  "03143013a": {
    "text": {
      "ascii": "tataH kArmukam udyamya bhImaseno mahAbalaH\n",
      "ud": "ततः कार्मुकमुद्यम्य भीमसेनो महाबलः\n",
      "ur": "tataḥ kārmukam udyamya bhīmaseno mahābalaḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 13
  },
  "03143013c": {
    "text": {
      "ascii": "kRSNAm AdAya saMgatyA tasthAv Azritya pAdapam\n",
      "ud": "कृष्णामादाय संगत्या तस्थावाश्रित्य पादपम्\n",
      "ur": "kr̥ṣṇām ādāya saṁgatyā tasthāv āśritya pādapam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 13
  },
  "03143014a": {
    "text": {
      "ascii": "dharmarAjaz ca dhaumyaz ca nililyAte mahAvane\n",
      "ud": "धर्मराजश्च धौम्यश्च निलिल्याते महावने\n",
      "ur": "dharmarājaś ca dhaumyaś ca nililyāte mahāvane\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 14
  },
  "03143014c": {
    "text": {
      "ascii": "agnihotrANy upAdAya sahadevas tu parvate\n",
      "ud": "अग्निहोत्राण्युपादाय सहदेवस्तु पर्वते\n",
      "ur": "agnihotrāṇy upādāya sahadevas tu parvate\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 14
  },
  "03143015a": {
    "text": {
      "ascii": "nakulo brAhmaNAz cAnye lomazaz ca mahAtapAH\n",
      "ud": "नकुलो ब्राह्मणाश्चान्ये लोमशश्च महातपाः\n",
      "ur": "nakulo brāhmaṇāś cānye lomaśaś ca mahātapāḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 15
  },
  "03143015c": {
    "text": {
      "ascii": "vRkSAn AsAdya saMtrastAs tatra tatra nililyire\n",
      "ud": "वृक्षानासाद्य संत्रस्तास्तत्र तत्र निलिल्यिरे\n",
      "ur": "vr̥kṣān āsādya saṁtrastās tatra tatra nililyire\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 15
  },
  "03143016a": {
    "text": {
      "ascii": "mandIbhUte ca pavane tasmin rajasi zAmyati\n",
      "ud": "मन्दीभूते च पवने तस्मिन्रजसि शाम्यति\n",
      "ur": "mandībhūte ca pavane tasmin rajasi śāmyati\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 16
  },
  "03143016c": {
    "text": {
      "ascii": "mahadbhiH pRSatais tUrNaM varSam abhyAjagAma ha\n",
      "ud": "महद्भिः पृषतैस्तूर्णं वर्षमभ्याजगाम ह\n",
      "ur": "mahadbhiḥ pr̥ṣatais tūrṇaṁ varṣam abhyājagāma ha\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 16
  },
  "03143017a": {
    "text": {
      "ascii": "tato 'zmasahitA dhArAH saMvRNvantyaH samantataH\n",
      "ud": "ततोऽश्मसहिता धाराः संवृण्वन्त्यः समन्ततः\n",
      "ur": "tato ’śmasahitā dhārāḥ saṁvr̥ṇvantyaḥ samantataḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 17
  },
  "03143017c": {
    "text": {
      "ascii": "prapetur anizaM tatra zIghravAtasamIritAH\n",
      "ud": "प्रपेतुरनिशं तत्र शीघ्रवातसमीरिताः\n",
      "ur": "prapetur aniśaṁ tatra śīghravātasamīritāḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 17
  },
  "03143018a": {
    "text": {
      "ascii": "tataH sAgaragA ApaH kIryamANAH samantataH\n",
      "ud": "ततः सागरगा आपः कीर्यमाणाः समन्ततः\n",
      "ur": "tataḥ sāgaragā āpaḥ kīryamāṇāḥ samantataḥ\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 18
  },
  "03143018c": {
    "text": {
      "ascii": "prAdurAsan sakaluSAH phenavatyo vizAM pate\n",
      "ud": "प्रादुरासन्सकलुषाः फेनवत्यो विशां पते\n",
      "ur": "prādurāsan sakaluṣāḥ phenavatyo viśāṁ pate\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 18
  },
  "03143019a": {
    "text": {
      "ascii": "vahantyo vAri bahulaM phenoDupapariplutam\n",
      "ud": "वहन्त्यो वारि बहुलं फेनोडुपपरिप्लुतम्\n",
      "ur": "vahantyo vāri bahulaṁ phenoḍupapariplutam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 19
  },
  "03143019c": {
    "text": {
      "ascii": "parisasrur mahAzabdAH prakarSantyo mahIruhAn\n",
      "ud": "परिसस्रुर्महाशब्दाः प्रकर्षन्त्यो महीरुहान्\n",
      "ur": "parisasrur mahāśabdāḥ prakarṣantyo mahīruhān\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 19
  },
  "03143020a": {
    "text": {
      "ascii": "tasminn uparate varSe vAte ca samatAM gate\n",
      "ud": "तस्मिन्नुपरते वर्षे वाते च समतां गते\n",
      "ur": "tasminn uparate varṣe vāte ca samatāṁ gate\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 20
  },
  "03143020c": {
    "text": {
      "ascii": "gate hy ambhasi nimnAni prAdurbhUte divAkare\n",
      "ud": "गते ह्यम्भसि निम्नानि प्रादुर्भूते दिवाकरे\n",
      "ur": "gate hy ambhasi nimnāni prādurbhūte divākare\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 20
  },
  "03143021a": {
    "text": {
      "ascii": "nirjagmus te zanaiH sarve samAjagmuz ca bhArata\n",
      "ud": "निर्जग्मुस्ते शनैः सर्वे समाजग्मुश्च भारत\n",
      "ur": "nirjagmus te śanaiḥ sarve samājagmuś ca bhārata\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 21
  },
  "03143021c": {
    "text": {
      "ascii": "pratasthuz ca punar vIrAH parvataM gandhamAdanam\n",
      "ud": "प्रतस्थुश्च पुनर्वीराः पर्वतं गन्धमादनम्\n",
      "ur": "pratasthuś ca punar vīrāḥ parvataṁ gandhamādanam\n"
    },
    "book": 3,
    "chapter": 143,
    "verse": 21
  }
}
    },
    "book": 3,
    "chapter": 146,
    "verse": 24
  },
  "03146024c": {
    "text": {
      "ascii": "vimalair dhAtuvicchedaiH kAJcanAJjanarAjataiH\n",
      "ud": "विमलैर्धातुविच्छेदैः काञ्चनाञ्जनराजतैः\n",
      "ur": "vimalair dhātuvicchedaiḥ kāñcanāñjanarājataiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 24
  },
  "03146025a": {
    "text": {
      "ascii": "sapakSam iva nRtyantaM pArzvalagnaiH payodharaiH\n",
      "ud": "सपक्षमिव नृत्यन्तं पार्श्वलग्नैः पयोधरैः\n",
      "ur": "sapakṣam iva nr̥tyantaṁ pārśvalagnaiḥ payodharaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 25
  },
  "03146025c": {
    "text": {
      "ascii": "muktAhArair iva citaM cyutaiH prasravaNodakaiH\n",
      "ud": "मुक्ताहारैरिव चितं च्युतैः प्रस्रवणोदकैः\n",
      "ur": "muktāhārair iva citaṁ cyutaiḥ prasravaṇodakaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 25
  },
  "03146026a": {
    "text": {
      "ascii": "abhirAmanadIkuJjanirjharodarakandaram\n",
      "ud": "अभिरामनदीकुञ्जनिर्झरोदरकन्दरम्\n",
      "ur": "abhirāmanadīkuñjanirjharodarakandaram\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 26
  },
  "03146026c": {
    "text": {
      "ascii": "apsaronUpuraravaiH pranRttabahubarhiNam\n",
      "ud": "अप्सरोनूपुररवैः प्रनृत्तबहुबर्हिणम्\n",
      "ur": "apsaronūpuraravaiḥ pranr̥ttabahubarhiṇam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 26
  },
  "03146027a": {
    "text": {
      "ascii": "digvAraNaviSANAgrair ghRSTopalazilAtalam\n",
      "ud": "दिग्वारणविषाणाग्रैर्घृष्टोपलशिलातलम्\n",
      "ur": "digvāraṇaviṣāṇāgrair ghr̥ṣṭopalaśilātalam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 27
  },
  "03146027c": {
    "text": {
      "ascii": "srastAMzukam ivAkSobhyair nimnagAniHsRtair jalaiH\n",
      "ud": "स्रस्तांशुकमिवाक्षोभ्यैर्निम्नगानिःसृतैर्जलैः\n",
      "ur": "srastāṁśukam ivākṣobhyair nimnagāniḥsr̥tair jalaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 27
  },
  "03146028a": {
    "text": {
      "ascii": "sazaSpakavalaiH svasthair adUraparivartibhiH\n",
      "ud": "सशष्पकवलैः स्वस्थैरदूरपरिवर्तिभिः\n",
      "ur": "saśaṣpakavalaiḥ svasthair adūraparivartibhiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 28
  },
  "03146028c": {
    "text": {
      "ascii": "bhayasyAjJaiz ca hariNaiH kautUhalanirIkSitaH\n",
      "ud": "भयस्याज्ञैश्च हरिणैः कौतूहलनिरीक्षितः\n",
      "ur": "bhayasyājñaiś ca hariṇaiḥ kautūhalanirīkṣitaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 28
  },
  "03146029a": {
    "text": {
      "ascii": "cAlayann Uruvegena latAjAlAny anekazaH\n",
      "ud": "चालयन्नूरुवेगेन लताजालान्यनेकशः\n",
      "ur": "cālayann ūruvegena latājālāny anekaśaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 29
  },
  "03146029c": {
    "text": {
      "ascii": "AkrIDamAnaH kaunteyaH zrImAn vAyusuto yayau\n",
      "ud": "आक्रीडमानः कौन्तेयः श्रीमान्वायुसुतो ययौ\n",
      "ur": "ākrīḍamānaḥ kaunteyaḥ śrīmān vāyusuto yayau\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 29
  },
  "03146030a": {
    "text": {
      "ascii": "priyAmanorathaM kartum udyataz cArulocanaH\n",
      "ud": "प्रियामनोरथं कर्तुमुद्यतश्चारुलोचनः\n",
      "ur": "priyāmanorathaṁ kartum udyataś cārulocanaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 30
  },
  "03146030c": {
    "text": {
      "ascii": "prAMzuH kanakatAlAbhaH siMhasaMhanano yuvA\n",
      "ud": "प्रांशुः कनकतालाभः सिंहसंहननो युवा\n",
      "ur": "prāṁśuḥ kanakatālābhaḥ siṁhasaṁhanano yuvā\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 30
  },
  "03146031a": {
    "text": {
      "ascii": "mattavAraNavikrAnto mattavAraNavegavAn\n",
      "ud": "मत्तवारणविक्रान्तो मत्तवारणवेगवान्\n",
      "ur": "mattavāraṇavikrānto mattavāraṇavegavān\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 31
  },
  "03146031c": {
    "text": {
      "ascii": "mattavAraNatAmrAkSo mattavAraNavAraNaH\n",
      "ud": "मत्तवारणताम्राक्षो मत्तवारणवारणः\n",
      "ur": "mattavāraṇatāmrākṣo mattavāraṇavāraṇaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 31
  },
  "03146032a": {
    "text": {
      "ascii": "priyapArzvopaviSTAbhir vyAvRttAbhir viceSTitaiH\n",
      "ud": "प्रियपार्श्वोपविष्टाभिर्व्यावृत्ताभिर्विचेष्टितैः\n",
      "ur": "priyapārśvopaviṣṭābhir vyāvr̥ttābhir viceṣṭitaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 32
  },
  "03146032c": {
    "text": {
      "ascii": "yakSagandharvayoSAbhir adRzyAbhir nirIkSitaH\n",
      "ud": "यक्षगन्धर्वयोषाभिरदृश्याभिर्निरीक्षितः\n",
      "ur": "yakṣagandharvayoṣābhir adr̥śyābhir nirīkṣitaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 32
  },
  "03146033a": {
    "text": {
      "ascii": "navAvatAraM rUpasya vikrINann iva pANDavaH\n",
      "ud": "नवावतारं रूपस्य विक्रीणन्निव पाण्डवः\n",
      "ur": "navāvatāraṁ rūpasya vikrīṇann iva pāṇḍavaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 33
  },
  "03146033c": {
    "text": {
      "ascii": "cacAra ramaNIyeSu gandhamAdanasAnuSu\n",
      "ud": "चचार रमणीयेषु गन्धमादनसानुषु\n",
      "ur": "cacāra ramaṇīyeṣu gandhamādanasānuṣu\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 33
  },
  "03146034a": {
    "text": {
      "ascii": "saMsmaran vividhAn klezAn duryodhanakRtAn bahUn\n",
      "ud": "संस्मरन्विविधान्क्लेशान्दुर्योधनकृतान्बहून्\n",
      "ur": "saṁsmaran vividhān kleśān duryodhanakr̥tān bahūn\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 34
  },
  "03146034c": {
    "text": {
      "ascii": "draupadyA vanavAsinyAH priyaM kartuM samudyataH\n",
      "ud": "द्रौपद्या वनवासिन्याः प्रियं कर्तुं समुद्यतः\n",
      "ur": "draupadyā vanavāsinyāḥ priyaṁ kartuṁ samudyataḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 34
  },
  "03146035a": {
    "text": {
      "ascii": "so 'cintayad gate svargam arjune mayi cAgate\n",
      "ud": "सोऽचिन्तयद्गते स्वर्गमर्जुने मयि चागते\n",
      "ur": "so ’cintayad gate svargam arjune mayi cāgate\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 35
  },
  "03146035c": {
    "text": {
      "ascii": "puSpahetoH kathaM nv AryaH kariSyati yudhiSThiraH\n",
      "ud": "पुष्पहेतोः कथं न्वार्यः करिष्यति युधिष्ठिरः\n",
      "ur": "puṣpahetoḥ kathaṁ nv āryaḥ kariṣyati yudhiṣṭhiraḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 35
  },
  "03146036a": {
    "text": {
      "ascii": "snehAn naravaro nUnam avizvAsAd vanasya ca\n",
      "ud": "स्नेहान्नरवरो नूनमविश्वासाद्वनस्य च\n",
      "ur": "snehān naravaro nūnam aviśvāsād vanasya ca\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 36
  },
  "03146036c": {
    "text": {
      "ascii": "nakulaM sahadevaM ca na mokSyati yudhiSThiraH\n",
      "ud": "नकुलं सहदेवं च न मोक्ष्यति युधिष्ठिरः\n",
      "ur": "nakulaṁ sahadevaṁ ca na mokṣyati yudhiṣṭhiraḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 36
  },
  "03146037a": {
    "text": {
      "ascii": "kathaM nu kusumAvAptiH syAc chIghram iti cintayan\n",
      "ud": "कथं नु कुसुमावाप्तिः स्याच्छीघ्रमिति चिन्तयन्\n",
      "ur": "kathaṁ nu kusumāvāptiḥ syāc chīghram iti cintayan\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 37
  },
  "03146037c": {
    "text": {
      "ascii": "pratasthe narazArdUlaH pakSirAD iva vegitaH\n",
      "ud": "प्रतस्थे नरशार्दूलः पक्षिराडिव वेगितः\n",
      "ur": "pratasthe naraśārdūlaḥ pakṣirāḍ iva vegitaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 37
  },
  "03146038a": {
    "text": {
      "ascii": "kampayan medinIM padbhyAM nirghAta iva parvasu\n",
      "ud": "कम्पयन्मेदिनीं पद्भ्यां निर्घात इव पर्वसु\n",
      "ur": "kampayan medinīṁ padbhyāṁ nirghāta iva parvasu\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 38
  },
  "03146038c": {
    "text": {
      "ascii": "trAsayan gajayUthAni vAtaraMhA vRkodaraH\n",
      "ud": "त्रासयन्गजयूथानि वातरंहा वृकोदरः\n",
      "ur": "trāsayan gajayūthāni vātaraṁhā vr̥kodaraḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 38
  },
  "03146039a": {
    "text": {
      "ascii": "siMhavyAghragaNAMz caiva mardamAno mahAbalaH\n",
      "ud": "सिंहव्याघ्रगणांश्चैव मर्दमानो महाबलः\n",
      "ur": "siṁhavyāghragaṇāṁś caiva mardamāno mahābalaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 39
  },
  "03146039c": {
    "text": {
      "ascii": "unmUlayan mahAvRkSAn pothayaMz corasA balI\n",
      "ud": "उन्मूलयन्महावृक्षान्पोथयंश्चोरसा बली\n",
      "ur": "unmūlayan mahāvr̥kṣān pothayaṁś corasā balī\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 39
  },
  "03146040a": {
    "text": {
      "ascii": "latAvallIz ca vegena vikarSan pANDunandanaH\n",
      "ud": "लतावल्लीश्च वेगेन विकर्षन्पाण्डुनन्दनः\n",
      "ur": "latāvallīś ca vegena vikarṣan pāṇḍunandanaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 40
  },
  "03146040c": {
    "text": {
      "ascii": "upary upari zailAgram ArurukSur iva dvipaH\n",
      "ud": "उपर्युपरि शैलाग्रमारुरुक्षुरिव द्विपः\n",
      "ur": "upary upari śailāgram ārurukṣur iva dvipaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 40
  },
  "03146040e": {
    "text": {
      "ascii": "vinardamAno 'tibhRzaM savidyud iva toyadaH\n",
      "ud": "विनर्दमानोऽतिभृशं सविद्युदिव तोयदः\n",
      "ur": "vinardamāno ’tibhr̥śaṁ savidyud iva toyadaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 40
  },
  "03146041a": {
    "text": {
      "ascii": "tasya zabdena ghoreNa dhanurghoSeNa cAbhibho\n",
      "ud": "तस्य शब्देन घोरेण धनुर्घोषेण चाभिभो\n",
      "ur": "tasya śabdena ghoreṇa dhanurghoṣeṇa cābhibho\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 41
  },
  "03146041c": {
    "text": {
      "ascii": "trastAni mRgayUthAni samantAd vipradudruvuH\n",
      "ud": "त्रस्तानि मृगयूथानि समन्ताद्विप्रदुद्रुवुः\n",
      "ur": "trastāni mr̥gayūthāni samantād vipradudruvuḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 41
  },
  "03146042a": {
    "text": {
      "ascii": "athApazyan mahAbAhur gandhamAdanasAnuSu\n",
      "ud": "अथापश्यन्महाबाहुर्गन्धमादनसानुषु\n",
      "ur": "athāpaśyan mahābāhur gandhamādanasānuṣu\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 42
  },
  "03146042c": {
    "text": {
      "ascii": "suramyaM kadalISaNDaM bahuyojanavistRtam\n",
      "ud": "सुरम्यं कदलीषण्डं बहुयोजनविस्तृतम्\n",
      "ur": "suramyaṁ kadalīṣaṇḍaṁ bahuyojanavistr̥tam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 42
  },
  "03146043a": {
    "text": {
      "ascii": "tam abhyagacchad vegena kSobhayiSyan mahAbalaH\n",
      "ud": "तमभ्यगच्छद्वेगेन क्षोभयिष्यन्महाबलः\n",
      "ur": "tam abhyagacchad vegena kṣobhayiṣyan mahābalaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 43
  },
  "03146043c": {
    "text": {
      "ascii": "mahAgaja ivAsrAvI prabhaJjan vividhAn drumAn\n",
      "ud": "महागज इवास्रावी प्रभञ्जन्विविधान्द्रुमान्\n",
      "ur": "mahāgaja ivāsrāvī prabhañjan vividhān drumān\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 43
  },
  "03146044a": {
    "text": {
      "ascii": "utpATya kadalIskandhAn bahutAlasamucchrayAn\n",
      "ud": "उत्पाट्य कदलीस्कन्धान्बहुतालसमुच्छ्रयान्\n",
      "ur": "utpāṭya kadalīskandhān bahutālasamucchrayān\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 44
  },
  "03146044c": {
    "text": {
      "ascii": "cikSepa tarasA bhImaH samantAd balinAM varaH\n",
      "ud": "चिक्षेप तरसा भीमः समन्ताद्बलिनां वरः\n",
      "ur": "cikṣepa tarasā bhīmaḥ samantād balināṁ varaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 44
  },
  "03146045a": {
    "text": {
      "ascii": "tataH sattvAny upAkrAman bahUni ca mahAnti ca\n",
      "ud": "ततः सत्त्वान्युपाक्रामन्बहूनि च महान्ति च\n",
      "ur": "tataḥ sattvāny upākrāman bahūni ca mahānti ca\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 45
  },
  "03146045c": {
    "text": {
      "ascii": "ruruvAraNasaMghAz ca mahiSAz ca jalAzrayAH\n",
      "ud": "रुरुवारणसंघाश्च महिषाश्च जलाश्रयाः\n",
      "ur": "ruruvāraṇasaṁghāś ca mahiṣāś ca jalāśrayāḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 45
  },
  "03146046a": {
    "text": {
      "ascii": "siMhavyAghrAz ca saMkruddhA bhImasenam abhidravan\n",
      "ud": "सिंहव्याघ्राश्च संक्रुद्धा भीमसेनमभिद्रवन्\n",
      "ur": "siṁhavyāghrāś ca saṁkruddhā bhīmasenam abhidravan\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 46
  },
  "03146046c": {
    "text": {
      "ascii": "vyAditAsyA mahAraudrA vinadanto 'tibhISaNAH\n",
      "ud": "व्यादितास्या महारौद्रा विनदन्तोऽतिभीषणाः\n",
      "ur": "vyāditāsyā mahāraudrā vinadanto ’tibhīṣaṇāḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 46
  },
  "03146047a": {
    "text": {
      "ascii": "tato vAyusutaH krodhAt svabAhubalam AzritaH\n",
      "ud": "ततो वायुसुतः क्रोधात्स्वबाहुबलमाश्रितः\n",
      "ur": "tato vāyusutaḥ krodhāt svabāhubalam āśritaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 47
  },
  "03146047c": {
    "text": {
      "ascii": "gajenAghnan gajaM bhImaH siMhaM siMhena cAbhibhUH\n",
      "ud": "गजेनाघ्नन्गजं भीमः सिंहं सिंहेन चाभिभूः\n",
      "ur": "gajenāghnan gajaṁ bhīmaḥ siṁhaṁ siṁhena cābhibhūḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 47
  },
  "03146047e": {
    "text": {
      "ascii": "talaprahArair anyAMz ca vyahanat pANDavo balI\n",
      "ud": "तलप्रहारैरन्यांश्च व्यहनत्पाण्डवो बली\n",
      "ur": "talaprahārair anyāṁś ca vyahanat pāṇḍavo balī\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 47
  },
  "03146048a": {
    "text": {
      "ascii": "te hanyamAnA bhImena siMhavyAghratarakSavaH\n",
      "ud": "ते हन्यमाना भीमेन सिंहव्याघ्रतरक्षवः\n",
      "ur": "te hanyamānā bhīmena siṁhavyāghratarakṣavaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 48
  },
  "03146048c": {
    "text": {
      "ascii": "bhayAd visasRpuH sarve zakRnmUtraM ca susruvuH\n",
      "ud": "भयाद्विससृपुः सर्वे शकृन्मूत्रं च सुस्रुवुः\n",
      "ur": "bhayād visasr̥puḥ sarve śakr̥nmūtraṁ ca susruvuḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 48
  },
  "03146049a": {
    "text": {
      "ascii": "praviveza tataH kSipraM tAn apAsya mahAbalaH\n",
      "ud": "प्रविवेश ततः क्षिप्रं तानपास्य महाबलः\n",
      "ur": "praviveśa tataḥ kṣipraṁ tān apāsya mahābalaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 49
  },
  "03146049c": {
    "text": {
      "ascii": "vanaM pANDusutaH zrImAJ zabdenApUrayan dizaH\n",
      "ud": "वनं पाण्डुसुतः श्रीमाञ्शब्देनापूरयन्दिशः\n",
      "ur": "vanaṁ pāṇḍusutaḥ śrīmāñ śabdenāpūrayan diśaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 49
  },
  "03146050a": {
    "text": {
      "ascii": "tena zabdena cogreNa bhImasenaraveNa ca\n",
      "ud": "तेन शब्देन चोग्रेण भीमसेनरवेण च\n",
      "ur": "tena śabdena cogreṇa bhīmasenaraveṇa ca\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 50
  },
  "03146050c": {
    "text": {
      "ascii": "vanAntaragatAH sarve vitresur mRgapakSiNaH\n",
      "ud": "वनान्तरगताः सर्वे वित्रेसुर्मृगपक्षिणः\n",
      "ur": "vanāntaragatāḥ sarve vitresur mr̥gapakṣiṇaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 50
  },
  "03146051a": {
    "text": {
      "ascii": "taM zabdaM sahasA zrutvA mRgapakSisamIritam\n",
      "ud": "तं शब्दं सहसा श्रुत्वा मृगपक्षिसमीरितम्\n",
      "ur": "taṁ śabdaṁ sahasā śrutvā mr̥gapakṣisamīritam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 51
  },
  "03146051c": {
    "text": {
      "ascii": "jalArdrapakSA vihagAH samutpetuH sahasrazaH\n",
      "ud": "जलार्द्रपक्षा विहगाः समुत्पेतुः सहस्रशः\n",
      "ur": "jalārdrapakṣā vihagāḥ samutpetuḥ sahasraśaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 51
  },
  "03146052a": {
    "text": {
      "ascii": "tAn audakAn pakSigaNAn nirIkSya bharatarSabhaH\n",
      "ud": "तानौदकान्पक्षिगणान्निरीक्ष्य भरतर्षभः\n",
      "ur": "tān audakān pakṣigaṇān nirīkṣya bharatarṣabhaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 52
  },
  "03146052c": {
    "text": {
      "ascii": "tAn evAnusaran ramyaM dadarza sumahat saraH\n",
      "ud": "तानेवानुसरन्रम्यं ददर्श सुमहत्सरः\n",
      "ur": "tān evānusaran ramyaṁ dadarśa sumahat saraḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 52
  },
  "03146053a": {
    "text": {
      "ascii": "kAJcanaiH kadalISaNDair mandamArutakampitaiH\n",
      "ud": "काञ्चनैः कदलीषण्डैर्मन्दमारुतकम्पितैः\n",
      "ur": "kāñcanaiḥ kadalīṣaṇḍair mandamārutakampitaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 53
  },
  "03146053c": {
    "text": {
      "ascii": "vIjyamAnam ivAkSobhyaM tIrAntaravisarpibhiH\n",
      "ud": "वीज्यमानमिवाक्षोभ्यं तीरान्तरविसर्पिभिः\n",
      "ur": "vījyamānam ivākṣobhyaṁ tīrāntaravisarpibhiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 53
  },
  "03146054a": {
    "text": {
      "ascii": "tat saro 'thAvatIryAzu prabhUtakamalotpalam\n",
      "ud": "तत्सरोऽथावतीर्याशु प्रभूतकमलोत्पलम्\n",
      "ur": "tat saro ’thāvatīryāśu prabhūtakamalotpalam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 54
  },
  "03146054c": {
    "text": {
      "ascii": "mahAgaja ivoddAmaz cikrIDa balavad balI\n",
      "ud": "महागज इवोद्दामश्चिक्रीड बलवद्बली\n",
      "ur": "mahāgaja ivoddāmaś cikrīḍa balavad balī\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 54
  },
  "03146054e": {
    "text": {
      "ascii": "vikrIDya tasmin suciram uttatArAmitadyutiH\n",
      "ud": "विक्रीड्य तस्मिन्सुचिरमुत्ततारामितद्युतिः\n",
      "ur": "vikrīḍya tasmin suciram uttatārāmitadyutiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 54
  },
  "03146055a": {
    "text": {
      "ascii": "tato 'vagAhya vegena tad vanaM bahupAdapam\n",
      "ud": "ततोऽवगाह्य वेगेन तद्वनं बहुपादपम्\n",
      "ur": "tato ’vagāhya vegena tad vanaṁ bahupādapam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 55
  },
  "03146055c": {
    "text": {
      "ascii": "dadhmau ca zaGkhaM svanavat sarvaprANena pANDavaH\n",
      "ud": "दध्मौ च शङ्खं स्वनवत्सर्वप्राणेन पाण्डवः\n",
      "ur": "dadhmau ca śaṅkhaṁ svanavat sarvaprāṇena pāṇḍavaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 55
  },
  "03146056a": {
    "text": {
      "ascii": "tasya zaGkhasya zabdena bhImasenaraveNa ca\n",
      "ud": "तस्य शङ्खस्य शब्देन भीमसेनरवेण च\n",
      "ur": "tasya śaṅkhasya śabdena bhīmasenaraveṇa ca\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 56
  },
  "03146056c": {
    "text": {
      "ascii": "bAhuzabdena cogreNa nardantIva girer guhAH\n",
      "ud": "बाहुशब्देन चोग्रेण नर्दन्तीव गिरेर्गुहाः\n",
      "ur": "bāhuśabdena cogreṇa nardantīva girer guhāḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 56
  },
  "03146057a": {
    "text": {
      "ascii": "taM vajraniSpeSasamam AsphoTitaravaM bhRzam\n",
      "ud": "तं वज्रनिष्पेषसममास्फोटितरवं भृशम्\n",
      "ur": "taṁ vajraniṣpeṣasamam āsphoṭitaravaṁ bhr̥śam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 57
  },
  "03146057c": {
    "text": {
      "ascii": "zrutvA zailaguhAsuptaiH siMhair mukto mahAsvanaH\n",
      "ud": "श्रुत्वा शैलगुहासुप्तैः सिंहैर्मुक्तो महास्वनः\n",
      "ur": "śrutvā śailaguhāsuptaiḥ siṁhair mukto mahāsvanaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 57
  },
  "03146058a": {
    "text": {
      "ascii": "siMhanAdabhayatrastaiH kuJjarair api bhArata\n",
      "ud": "सिंहनादभयत्रस्तैः कुञ्जरैरपि भारत\n",
      "ur": "siṁhanādabhayatrastaiḥ kuñjarair api bhārata\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 58
  },
  "03146058c": {
    "text": {
      "ascii": "mukto virAvaH sumahAn parvato yena pUritaH\n",
      "ud": "मुक्तो विरावः सुमहान्पर्वतो येन पूरितः\n",
      "ur": "mukto virāvaḥ sumahān parvato yena pūritaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 58
  },
  "03146059a": {
    "text": {
      "ascii": "taM tu nAdaM tataH zrutvA supto vAnarapuMgavaH\n",
      "ud": "तं तु नादं ततः श्रुत्वा सुप्तो वानरपुंगवः\n",
      "ur": "taṁ tu nādaṁ tataḥ śrutvā supto vānarapuṁgavaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 59
  },
  "03146059c": {
    "text": {
      "ascii": "prAjRmbhata mahAkAyo hanUmAn nAma vAnaraH\n",
      "ud": "प्राजृम्भत महाकायो हनूमान्नाम वानरः\n",
      "ur": "prājr̥mbhata mahākāyo hanūmān nāma vānaraḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 59
  },
  "03146060a": {
    "text": {
      "ascii": "kadalISaNDamadhyastho nidrAvazagatas tadA\n",
      "ud": "कदलीषण्डमध्यस्थो निद्रावशगतस्तदा\n",
      "ur": "kadalīṣaṇḍamadhyastho nidrāvaśagatas tadā\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 60
  },
  "03146060c": {
    "text": {
      "ascii": "jRmbhamANaH suvipulaM zakradhvajam ivocchritam\n",
      "ud": "जृम्भमाणः सुविपुलं शक्रध्वजमिवोच्छ्रितम्\n",
      "ur": "jr̥mbhamāṇaḥ suvipulaṁ śakradhvajam ivocchritam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 60
  },
  "03146060e": {
    "text": {
      "ascii": "AsphoTayata lAGgUlam indrAzanisamasvanam\n",
      "ud": "आस्फोटयत लाङ्गूलमिन्द्राशनिसमस्वनम्\n",
      "ur": "āsphoṭayata lāṅgūlam indrāśanisamasvanam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 60
  },
  "03146061a": {
    "text": {
      "ascii": "tasya lAGgUlaninadaM parvataH sa guhAmukhaiH\n",
      "ud": "तस्य लाङ्गूलनिनदं पर्वतः स गुहामुखैः\n",
      "ur": "tasya lāṅgūlaninadaṁ parvataḥ sa guhāmukhaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 61
  },
  "03146061c": {
    "text": {
      "ascii": "udgAram iva gaur nardam utsasarja samantataH\n",
      "ud": "उद्गारमिव गौर्नर्दमुत्ससर्ज समन्ततः\n",
      "ur": "udgāram iva gaur nardam utsasarja samantataḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 61
  },
  "03146062a": {
    "text": {
      "ascii": "sa lAGgUlaravas tasya mattavAraNanisvanam\n",
      "ud": "स लाङ्गूलरवस्तस्य मत्तवारणनिस्वनम्\n",
      "ur": "sa lāṅgūlaravas tasya mattavāraṇanisvanam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 62
  },
  "03146062c": {
    "text": {
      "ascii": "antardhAya vicitreSu cacAra girisAnuSu\n",
      "ud": "अन्तर्धाय विचित्रेषु चचार गिरिसानुषु\n",
      "ur": "antardhāya vicitreṣu cacāra girisānuṣu\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 62
  },
  "03146063a": {
    "text": {
      "ascii": "sa bhImasenas taM zrutvA saMprahRSTatanUruhaH\n",
      "ud": "स भीमसेनस्तं श्रुत्वा संप्रहृष्टतनूरुहः\n",
      "ur": "sa bhīmasenas taṁ śrutvā saṁprahr̥ṣṭatanūruhaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 63
  },
  "03146063c": {
    "text": {
      "ascii": "zabdaprabhavam anvicchaMz cacAra kadalIvanam\n",
      "ud": "शब्दप्रभवमन्विच्छंश्चचार कदलीवनम्\n",
      "ur": "śabdaprabhavam anvicchaṁś cacāra kadalīvanam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 63
  },
  "03146064a": {
    "text": {
      "ascii": "kadalIvanamadhyastham atha pIne zilAtale\n",
      "ud": "कदलीवनमध्यस्थमथ पीने शिलातले\n",
      "ur": "kadalīvanamadhyastham atha pīne śilātale\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 64
  },
  "03146064c": {
    "text": {
      "ascii": "sa dadarza mahAbAhur vAnarAdhipatiM sthitam\n",
      "ud": "स ददर्श महाबाहुर्वानराधिपतिं स्थितम्\n",
      "ur": "sa dadarśa mahābāhur vānarādhipatiṁ sthitam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 64
  },
  "03146065a": {
    "text": {
      "ascii": "vidyutsaMghAtaduSprekSyaM vidyutsaMghAtapiGgalam\n",
      "ud": "विद्युत्संघातदुष्प्रेक्ष्यं विद्युत्संघातपिङ्गलम्\n",
      "ur": "vidyutsaṁghātaduṣprekṣyaṁ vidyutsaṁghātapiṅgalam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 65
  },
  "03146065c": {
    "text": {
      "ascii": "vidyutsaMghAtasadRzaM vidyutsaMghAtacaJcalam\n",
      "ud": "विद्युत्संघातसदृशं विद्युत्संघातचञ्चलम्\n",
      "ur": "vidyutsaṁghātasadr̥śaṁ vidyutsaṁghātacañcalam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 65
  },
  "03146066a": {
    "text": {
      "ascii": "bAhusvastikavinyastapInahrasvazirodharam\n",
      "ud": "बाहुस्वस्तिकविन्यस्तपीनह्रस्वशिरोधरम्\n",
      "ur": "bāhusvastikavinyastapīnahrasvaśirodharam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 66
  },
  "03146066c": {
    "text": {
      "ascii": "skandhabhUyiSThakAyatvAt tanumadhyakaTItaTam\n",
      "ud": "स्कन्धभूयिष्ठकायत्वात्तनुमध्यकटीतटम्\n",
      "ur": "skandhabhūyiṣṭhakāyatvāt tanumadhyakaṭītaṭam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 66
  },
  "03146067a": {
    "text": {
      "ascii": "kiM cic cAbhugnazIrSeNa dIrgharomAJcitena ca\n",
      "ud": "किंचिच्चाभुग्नशीर्षेण दीर्घरोमाञ्चितेन च\n",
      "ur": "kiṁ cic cābhugnaśīrṣeṇa dīrgharomāñcitena ca\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 67
  },
  "03146067c": {
    "text": {
      "ascii": "lAGgUlenordhvagatinA dhvajeneva virAjitam\n",
      "ud": "लाङ्गूलेनोर्ध्वगतिना ध्वजेनेव विराजितम्\n",
      "ur": "lāṅgūlenordhvagatinā dhvajeneva virājitam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 67
  },
  "03146068a": {
    "text": {
      "ascii": "raktoSThaM tAmrajihvAsyaM raktakarNaM caladbhruvam\n",
      "ud": "रक्तोष्ठं ताम्रजिह्वास्यं रक्तकर्णं चलद्भ्रुवम्\n",
      "ur": "raktoṣṭhaṁ tāmrajihvāsyaṁ raktakarṇaṁ caladbhruvam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 68
  },
  "03146068c": {
    "text": {
      "ascii": "vadanaM vRttadaMSTrAgraM razmivantam ivoDupam\n",
      "ud": "वदनं वृत्तदंष्ट्राग्रं रश्मिवन्तमिवोडुपम्\n",
      "ur": "vadanaṁ vr̥ttadaṁṣṭrāgraṁ raśmivantam ivoḍupam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 68
  },
  "03146069a": {
    "text": {
      "ascii": "vadanAbhyantaragataiH zuklabhAsair alaMkRtam\n",
      "ud": "वदनाभ्यन्तरगतैः शुक्लभासैरलंकृतम्\n",
      "ur": "vadanābhyantaragataiḥ śuklabhāsair alaṁkr̥tam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 69
  },
  "03146069c": {
    "text": {
      "ascii": "kesarotkarasaMmizram azokAnAm ivotkaram\n",
      "ud": "केसरोत्करसंमिश्रमशोकानामिवोत्करम्\n",
      "ur": "kesarotkarasaṁmiśram aśokānām ivotkaram\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 69
  },
  "03146070a": {
    "text": {
      "ascii": "hiraNmayInAM madhyasthaM kadalInAM mahAdyutim\n",
      "ud": "हिरण्मयीनां मध्यस्थं कदलीनां महाद्युतिम्\n",
      "ur": "hiraṇmayīnāṁ madhyasthaṁ kadalīnāṁ mahādyutim\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 70
  },
  "03146070c": {
    "text": {
      "ascii": "dIpyamAnaM svavapuSA arciSmantam ivAnalam\n",
      "ud": "दीप्यमानं स्ववपुषा अर्चिष्मन्तमिवानलम्\n",
      "ur": "dīpyamānaṁ svavapuṣā arciṣmantam ivānalam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 70
  },
  "03146071a": {
    "text": {
      "ascii": "nirIkSantam avitrastaM locanair madhupiGgalaiH\n",
      "ud": "निरीक्षन्तमवित्रस्तं लोचनैर्मधुपिङ्गलैः\n",
      "ur": "nirīkṣantam avitrastaṁ locanair madhupiṅgalaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 71
  },
  "03146071c": {
    "text": {
      "ascii": "taM vAnaravaraM vIram atikAyaM mahAbalam\n",
      "ud": "तं वानरवरं वीरमतिकायं महाबलम्\n",
      "ur": "taṁ vānaravaraṁ vīram atikāyaṁ mahābalam\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 71
  },
  "03146072a": {
    "text": {
      "ascii": "athopasRtya tarasA bhImo bhImaparAkramaH\n",
      "ud": "अथोपसृत्य तरसा भीमो भीमपराक्रमः\n",
      "ur": "athopasr̥tya tarasā bhīmo bhīmaparākramaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 72
  },
  "03146072c": {
    "text": {
      "ascii": "siMhanAdaM samakarod bodhayiSyan kapiM tadA\n",
      "ud": "सिंहनादं समकरोद्बोधयिष्यन्कपिं तदा\n",
      "ur": "siṁhanādaṁ samakarod bodhayiṣyan kapiṁ tadā\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 72
  },
  "03146073a": {
    "text": {
      "ascii": "tena zabdena bhImasya vitresur mRgapakSiNaH\n",
      "ud": "तेन शब्देन भीमस्य वित्रेसुर्मृगपक्षिणः\n",
      "ur": "tena śabdena bhīmasya vitresur mr̥gapakṣiṇaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 73
  },
  "03146073c": {
    "text": {
      "ascii": "hanUmAMz ca mahAsattva ISad unmIlya locane\n",
      "ud": "हनूमांश्च महासत्त्व ईषदुन्मील्य लोचने\n",
      "ur": "hanūmāṁś ca mahāsattva īṣad unmīlya locane\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 73
  },
  "03146073e": {
    "text": {
      "ascii": "avaikSad atha sAvajJaM locanair madhupiGgalaiH\n",
      "ud": "अवैक्षदथ सावज्ञं लोचनैर्मधुपिङ्गलैः\n",
      "ur": "avaikṣad atha sāvajñaṁ locanair madhupiṅgalaiḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 73
  },
  "03146074a": {
    "text": {
      "ascii": "smitenAbhASya kaunteyaM vAnaro naram abravIt\n",
      "ud": "स्मितेनाभाष्य कौन्तेयं वानरो नरमब्रवीत्\n",
      "ur": "smitenābhāṣya kaunteyaṁ vānaro naram abravīt\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 74
  },
  "03146074c": {
    "text": {
      "ascii": "kimarthaM sarujas te 'haM sukhasuptaH prabodhitaH\n",
      "ud": "किमर्थं सरुजस्तेऽहं सुखसुप्तः प्रबोधितः\n",
      "ur": "kimarthaṁ sarujas te ’haṁ sukhasuptaḥ prabodhitaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 74
  },
  "03146075a": {
    "text": {
      "ascii": "nanu nAma tvayA kAryA dayA bhUteSu jAnatA\n",
      "ud": "ननु नाम त्वया कार्या दया भूतेषु जानता\n",
      "ur": "nanu nāma tvayā kāryā dayā bhūteṣu jānatā\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 75
  },
  "03146075c": {
    "text": {
      "ascii": "vayaM dharmaM na jAnImas tiryagyoniM samAzritAH\n",
      "ud": "वयं धर्मं न जानीमस्तिर्यग्योनिं समाश्रिताः\n",
      "ur": "vayaṁ dharmaṁ na jānīmas tiryagyoniṁ samāśritāḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 75
  },
  "03146076a": {
    "text": {
      "ascii": "manuSyA buddhisaMpannA dayAM kurvanti jantuSu\n",
      "ud": "मनुष्या बुद्धिसंपन्ना दयां कुर्वन्ति जन्तुषु\n",
      "ur": "manuṣyā buddhisaṁpannā dayāṁ kurvanti jantuṣu\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 76
  },
  "03146076c": {
    "text": {
      "ascii": "krUreSu karmasu kathaM dehavAkcittadUSiSu\n",
      "ud": "क्रूरेषु कर्मसु कथं देहवाक्चित्तदूषिषु\n",
      "ur": "krūreṣu karmasu kathaṁ dehavākcittadūṣiṣu\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 76
  },
  "03146076e": {
    "text": {
      "ascii": "dharmaghAtiSu sajjante buddhimanto bhavadvidhAH\n",
      "ud": "धर्मघातिषु सज्जन्ते बुद्धिमन्तो भवद्विधाः\n",
      "ur": "dharmaghātiṣu sajjante buddhimanto bhavadvidhāḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 76
  },
  "03146077a": {
    "text": {
      "ascii": "na tvaM dharmaM vijAnAsi vRddhA nopAsitAs tvayA\n",
      "ud": "न त्वं धर्मं विजानासि वृद्धा नोपासितास्त्वया\n",
      "ur": "na tvaṁ dharmaṁ vijānāsi vr̥ddhā nopāsitās tvayā\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 77
  },
  "03146077c": {
    "text": {
      "ascii": "alpabuddhitayA vanyAn utsAdayasi yan mRgAn\n",
      "ud": "अल्पबुद्धितया वन्यानुत्सादयसि यन्मृगान्\n",
      "ur": "alpabuddhitayā vanyān utsādayasi yan mr̥gān\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 77
  },
  "03146078a": {
    "text": {
      "ascii": "brUhi kas tvaM kimarthaM vA vanaM tvam idam AgataH\n",
      "ud": "ब्रूहि कस्त्वं किमर्थं वा वनं त्वमिदमागतः\n",
      "ur": "brūhi kas tvaṁ kimarthaṁ vā vanaṁ tvam idam āgataḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 78
  },
  "03146078c": {
    "text": {
      "ascii": "varjitaM mAnuSair bhAvais tathaiva puruSair api\n",
      "ud": "वर्जितं मानुषैर्भावैस्तथैव पुरुषैरपि\n",
      "ur": "varjitaṁ mānuṣair bhāvais tathaiva puruṣair api\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 78
  },
  "03146079a": {
    "text": {
      "ascii": "ataH paramagamyo 'yaM parvataH sudurAruhaH\n",
      "ud": "अतः परमगम्योऽयं पर्वतः सुदुरारुहः\n",
      "ur": "ataḥ paramagamyo ’yaṁ parvataḥ sudurāruhaḥ\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 79
  },
  "03146079c": {
    "text": {
      "ascii": "vinA siddhagatiM vIra gatir atra na vidyate\n",
      "ud": "विना सिद्धगतिं वीर गतिरत्र न विद्यते\n",
      "ur": "vinā siddhagatiṁ vīra gatir atra na vidyate\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 79
  },
  "03146080a": {
    "text": {
      "ascii": "kAruNyAt sauhRdAc caiva vAraye tvAM mahAbala\n",
      "ud": "कारुण्यात्सौहृदाच्चैव वारये त्वां महाबल\n",
      "ur": "kāruṇyāt sauhr̥dāc caiva vāraye tvāṁ mahābala\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 80
  },
  "03146080c": {
    "text": {
      "ascii": "nAtaH paraM tvayA zakyaM gantum Azvasihi prabho\n",
      "ud": "नातः परं त्वया शक्यं गन्तुमाश्वसिहि प्रभो\n",
      "ur": "nātaḥ paraṁ tvayā śakyaṁ gantum āśvasihi prabho\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 80
  },
  "03146081a": {
    "text": {
      "ascii": "imAny amRtakalpAni mUlAni ca phalAni ca\n",
      "ud": "इमान्यमृतकल्पानि मूलानि च फलानि च\n",
      "ur": "imāny amr̥takalpāni mūlāni ca phalāni ca\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 81
  },
  "03146081c": {
    "text": {
      "ascii": "bhakSayitvA nivartasva grAhyaM yadi vaco mama\n",
      "ud": "भक्षयित्वा निवर्तस्व ग्राह्यं यदि वचो मम\n",
      "ur": "bhakṣayitvā nivartasva grāhyaṁ yadi vaco mama\n"
    },
    "book": 3,
    "chapter": 146,
    "verse": 81
  }
}
{
  "03230001": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 1
  },
  "03230001a": {
    "text": {
      "ascii": "tatas te sahitAH sarve duryodhanam upAgaman\n",
      "ud": "ततस्ते सहिताः सर्वे दुर्योधनमुपागमन्\n",
      "ur": "tatas te sahitāḥ sarve duryodhanam upāgaman\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 1
  },
  "03230001c": {
    "text": {
      "ascii": "abruvaMz ca mahArAja yad UcuH kauravaM prati\n",
      "ud": "अब्रुवंश्च महाराज यदूचुः कौरवं प्रति\n",
      "ur": "abruvaṁś ca mahārāja yad ūcuḥ kauravaṁ prati\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 1
  },
  "03230002a": {
    "text": {
      "ascii": "gandharvair vArite sainye dhArtarASTraH pratApavAn\n",
      "ud": "गन्धर्वैर्वारिते सैन्ये धार्तराष्ट्रः प्रतापवान्\n",
      "ur": "gandharvair vārite sainye dhārtarāṣṭraḥ pratāpavān\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 2
  },
  "03230002c": {
    "text": {
      "ascii": "amarSapUrNaH sainyAni pratyabhASata bhArata\n",
      "ud": "अमर्षपूर्णः सैन्यानि प्रत्यभाषत भारत\n",
      "ur": "amarṣapūrṇaḥ sainyāni pratyabhāṣata bhārata\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 2
  },
  "03230003a": {
    "text": {
      "ascii": "zAsatainAn adharmajJAn mama vipriyakAriNaH\n",
      "ud": "शासतैनानधर्मज्ञान्मम विप्रियकारिणः\n",
      "ur": "śāsatainān adharmajñān mama vipriyakāriṇaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 3
  },
  "03230003c": {
    "text": {
      "ascii": "yadi prakrIDito devaiH sarvaiH saha zatakratuH\n",
      "ud": "यदि प्रक्रीडितो देवैः सर्वैः सह शतक्रतुः\n",
      "ur": "yadi prakrīḍito devaiḥ sarvaiḥ saha śatakratuḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 3
  },
  "03230004a": {
    "text": {
      "ascii": "duryodhanavacaH zrutvA dhArtarASTrA mahAbalAH\n",
      "ud": "दुर्योधनवचः श्रुत्वा धार्तराष्ट्रा महाबलाः\n",
      "ur": "duryodhanavacaḥ śrutvā dhārtarāṣṭrā mahābalāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 4
  },
  "03230004c": {
    "text": {
      "ascii": "sarva evAbhisaMnaddhA yodhAz cApi sahasrazaH\n",
      "ud": "सर्व एवाभिसंनद्धा योधाश्चापि सहस्रशः\n",
      "ur": "sarva evābhisaṁnaddhā yodhāś cāpi sahasraśaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 4
  },
  "03230005a": {
    "text": {
      "ascii": "tataH pramathya gandharvAMs tad vanaM vivizur balAt\n",
      "ud": "ततः प्रमथ्य गन्धर्वांस्तद्वनं विविशुर्बलात्\n",
      "ur": "tataḥ pramathya gandharvāṁs tad vanaṁ viviśur balāt\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 5
  },
  "03230005c": {
    "text": {
      "ascii": "siMhanAdena mahatA pUrayanto dizo daza\n",
      "ud": "सिंहनादेन महता पूरयन्तो दिशो दश\n",
      "ur": "siṁhanādena mahatā pūrayanto diśo daśa\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 5
  },
  "03230006a": {
    "text": {
      "ascii": "tato 'parair avAryanta gandharvaiH kurusainikAH\n",
      "ud": "ततोऽपरैरवार्यन्त गन्धर्वैः कुरुसैनिकाः\n",
      "ur": "tato ’parair avāryanta gandharvaiḥ kurusainikāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 6
  },
  "03230006c": {
    "text": {
      "ascii": "te vAryamANA gandharvaiH sAmnaiva vasudhAdhipa\n",
      "ud": "ते वार्यमाणा गन्धर्वैः साम्नैव वसुधाधिप\n",
      "ur": "te vāryamāṇā gandharvaiḥ sāmnaiva vasudhādhipa\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 6
  },
  "03230006e": {
    "text": {
      "ascii": "tAn anAdRtya gandharvAMs tad vanaM vivizur mahat\n",
      "ud": "ताननादृत्य गन्धर्वांस्तद्वनं विविशुर्महत्\n",
      "ur": "tān anādr̥tya gandharvāṁs tad vanaṁ viviśur mahat\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 6
  },
  "03230007a": {
    "text": {
      "ascii": "yadA vAcA na tiSThanti dhArtarASTrAH sarAjakAH\n",
      "ud": "यदा वाचा न तिष्ठन्ति धार्तराष्ट्राः सराजकाः\n",
      "ur": "yadā vācā na tiṣṭhanti dhārtarāṣṭrāḥ sarājakāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 7
  },
  "03230007c": {
    "text": {
      "ascii": "tatas te khecarAH sarve citrasene nyavedayan\n",
      "ud": "ततस्ते खेचराः सर्वे चित्रसेने न्यवेदयन्\n",
      "ur": "tatas te khecarāḥ sarve citrasene nyavedayan\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 7
  },
  "03230008a": {
    "text": {
      "ascii": "gandharvarAjas tAn sarvAn abravIt kauravAn prati\n",
      "ud": "गन्धर्वराजस्तान्सर्वानब्रवीत्कौरवान्प्रति\n",
      "ur": "gandharvarājas tān sarvān abravīt kauravān prati\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 8
  },
  "03230008c": {
    "text": {
      "ascii": "anAryAJ zAsatety evaM citraseno 'tyamarSaNaH\n",
      "ud": "अनार्याञ्शासतेत्येवं चित्रसेनोऽत्यमर्षणः\n",
      "ur": "anāryāñ śāsatety evaṁ citraseno ’tyamarṣaṇaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 8
  },
  "03230009a": {
    "text": {
      "ascii": "anujJAtAs tu gandharvAz citrasenena bhArata\n",
      "ud": "अनुज्ञातास्तु गन्धर्वाश्चित्रसेनेन भारत\n",
      "ur": "anujñātās tu gandharvāś citrasenena bhārata\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 9
  },
  "03230009c": {
    "text": {
      "ascii": "pragRhItAyudhAH sarve dhArtarASTrAn abhidravan\n",
      "ud": "प्रगृहीतायुधाः सर्वे धार्तराष्ट्रानभिद्रवन्\n",
      "ur": "pragr̥hītāyudhāḥ sarve dhārtarāṣṭrān abhidravan\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 9
  },
  "03230010a": {
    "text": {
      "ascii": "tAn dRSTvA patataH zIghrAn gandharvAn udyatAyudhAn\n",
      "ud": "तान्दृष्ट्वा पततः शीघ्रान्गन्धर्वानुद्यतायुधान्\n",
      "ur": "tān dr̥ṣṭvā patataḥ śīghrān gandharvān udyatāyudhān\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 10
  },
  "03230010c": {
    "text": {
      "ascii": "sarve te prAdravan saMkhye dhArtarASTrasya pazyataH\n",
      "ud": "सर्वे ते प्राद्रवन्संख्ये धार्तराष्ट्रस्य पश्यतः\n",
      "ur": "sarve te prādravan saṁkhye dhārtarāṣṭrasya paśyataḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 10
  },
  "03230011a": {
    "text": {
      "ascii": "tAn dRSTvA dravataH sarvAn dhArtarASTrAn parAGmukhAn\n",
      "ud": "तान्दृष्ट्वा द्रवतः सर्वान्धार्तराष्ट्रान्पराङ्मुखान्\n",
      "ur": "tān dr̥ṣṭvā dravataḥ sarvān dhārtarāṣṭrān parāṅmukhān\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 11
  },
  "03230011c": {
    "text": {
      "ascii": "vaikartanas tadA vIro nAsIt tatra parAGmukhaH\n",
      "ud": "वैकर्तनस्तदा वीरो नासीत्तत्र पराङ्मुखः\n",
      "ur": "vaikartanas tadā vīro nāsīt tatra parāṅmukhaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 11
  },
  "03230012a": {
    "text": {
      "ascii": "ApatantIM tu saMprekSya gandharvANAM mahAcamUm\n",
      "ud": "आपतन्तीं तु संप्रेक्ष्य गन्धर्वाणां महाचमूम्\n",
      "ur": "āpatantīṁ tu saṁprekṣya gandharvāṇāṁ mahācamūm\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 12
  },
  "03230012c": {
    "text": {
      "ascii": "mahatA zaravarSeNa rAdheyaH pratyavArayat\n",
      "ud": "महता शरवर्षेण राधेयः प्रत्यवारयत्\n",
      "ur": "mahatā śaravarṣeṇa rādheyaḥ pratyavārayat\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 12
  },
  "03230013a": {
    "text": {
      "ascii": "kSuraprair vizikhair bhallair vatsadantais tathAyasaiH\n",
      "ud": "क्षुरप्रैर्विशिखैर्भल्लैर्वत्सदन्तैस्तथायसैः\n",
      "ur": "kṣuraprair viśikhair bhallair vatsadantais tathāyasaiḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 13
  },
  "03230013c": {
    "text": {
      "ascii": "gandharvAJ zatazo 'bhyaghna\u0026l laghutvAt sUtanandanaH\n",
      "ud": "गन्धर्वाञ्शतशोऽभ्यघ्नँल्लघुत्वात्सूतनन्दनः\n",
      "ur": "gandharvāñ śataśo ’bhyaghnam̐l laghutvāt sūtanandanaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 13
  },
  "03230014a": {
    "text": {
      "ascii": "pAtayann uttamAGgAni gandharvANAM mahArathaH\n",
      "ud": "पातयन्नुत्तमाङ्गानि गन्धर्वाणां महारथः\n",
      "ur": "pātayann uttamāṅgāni gandharvāṇāṁ mahārathaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 14
  },
  "03230014c": {
    "text": {
      "ascii": "kSaNena vyadhamat sarvAM citrasenasya vAhinIm\n",
      "ud": "क्षणेन व्यधमत्सर्वां चित्रसेनस्य वाहिनीम्\n",
      "ur": "kṣaṇena vyadhamat sarvāṁ citrasenasya vāhinīm\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 14
  },
  "03230015a": {
    "text": {
      "ascii": "te vadhyamAnA gandharvAH sUtaputreNa dhImatA\n",
      "ud": "ते वध्यमाना गन्धर्वाः सूतपुत्रेण धीमता\n",
      "ur": "te vadhyamānā gandharvāḥ sūtaputreṇa dhīmatā\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 15
  },
  "03230015c": {
    "text": {
      "ascii": "bhUya evAbhyavartanta zatazo 'tha sahasrazaH\n",
      "ud": "भूय एवाभ्यवर्तन्त शतशोऽथ सहस्रशः\n",
      "ur": "bhūya evābhyavartanta śataśo ’tha sahasraśaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 15
  },
  "03230016a": {
    "text": {
      "ascii": "gandharvabhUtA pRthivI kSaNena samapadyata\n",
      "ud": "गन्धर्वभूता पृथिवी क्षणेन समपद्यत\n",
      "ur": "gandharvabhūtā pr̥thivī kṣaṇena samapadyata\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 16
  },
  "03230016c": {
    "text": {
      "ascii": "Apatadbhir mahAvegaiz citrasenasya sainikaiH\n",
      "ud": "आपतद्भिर्महावेगैश्चित्रसेनस्य सैनिकैः\n",
      "ur": "āpatadbhir mahāvegaiś citrasenasya sainikaiḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 16
  },
  "03230017a": {
    "text": {
      "ascii": "atha duryodhano rAjA zakuniz cApi saubalaH\n",
      "ud": "अथ दुर्योधनो राजा शकुनिश्चापि सौबलः\n",
      "ur": "atha duryodhano rājā śakuniś cāpi saubalaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 17
  },
  "03230017c": {
    "text": {
      "ascii": "duHzAsano vikarNaz ca ye cAnye dhRtarASTrajAH\n",
      "ud": "दुःशासनो विकर्णश्च ये चान्ये धृतराष्ट्रजाः\n",
      "ur": "duḥśāsano vikarṇaś ca ye cānye dhr̥tarāṣṭrajāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 17
  },
  "03230017e": {
    "text": {
      "ascii": "nyahanaMs tat tadA sainyaM rathair garuDanisvanaiH\n",
      "ud": "न्यहनंस्तत्तदा सैन्यं रथैर्गरुडनिस्वनैः\n",
      "ur": "nyahanaṁs tat tadā sainyaṁ rathair garuḍanisvanaiḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 17
  },
  "03230018a": {
    "text": {
      "ascii": "bhUyaz ca yodhayAm AsuH kRtvA karNam athAgrataH\n",
      "ud": "भूयश्च योधयामासुः कृत्वा कर्णमथाग्रतः\n",
      "ur": "bhūyaś ca yodhayām āsuḥ kr̥tvā karṇam athāgrataḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 18
  },
  "03230018c": {
    "text": {
      "ascii": "mahatA rathaghoSeNa hayacAreNa cApy uta\n",
      "ud": "महता रथघोषेण हयचारेण चाप्युत\n",
      "ur": "mahatā rathaghoṣeṇa hayacāreṇa cāpy uta\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 18
  },
  "03230018e": {
    "text": {
      "ascii": "vaikartanaM parIpsanto gandharvAn samavArayan\n",
      "ud": "वैकर्तनं परीप्सन्तो गन्धर्वान्समवारयन्\n",
      "ur": "vaikartanaṁ parīpsanto gandharvān samavārayan\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 18
  },
  "03230019a": {
    "text": {
      "ascii": "tataH saMnyapatan sarve gandharvAH kauravaiH saha\n",
      "ud": "ततः संन्यपतन्सर्वे गन्धर्वाः कौरवैः सह\n",
      "ur": "tataḥ saṁnyapatan sarve gandharvāḥ kauravaiḥ saha\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 19
  },
  "03230019c": {
    "text": {
      "ascii": "tadA sutumulaM yuddham abhaval lomaharSaNam\n",
      "ud": "तदा सुतुमुलं युद्धमभवल्लोमहर्षणम्\n",
      "ur": "tadā sutumulaṁ yuddham abhaval lomaharṣaṇam\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 19
  },
  "03230020a": {
    "text": {
      "ascii": "tatas te mRdavo 'bhUvan gandharvAH zarapIDitAH\n",
      "ud": "ततस्ते मृदवोऽभूवन्गन्धर्वाः शरपीडिताः\n",
      "ur": "tatas te mr̥davo ’bhūvan gandharvāḥ śarapīḍitāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 20
  },
  "03230020c": {
    "text": {
      "ascii": "uccukruzuz ca kauravyA gandharvAn prekSya pIDitAn\n",
      "ud": "उच्चुक्रुशुश्च कौरव्या गन्धर्वान्प्रेक्ष्य पीडितान्\n",
      "ur": "uccukruśuś ca kauravyā gandharvān prekṣya pīḍitān\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 20
  },
  "03230021a": {
    "text": {
      "ascii": "gandharvAMs trAsitAn dRSTvA citraseno 'tyamarSaNaH\n",
      "ud": "गन्धर्वांस्त्रासितान्दृष्ट्वा चित्रसेनोऽत्यमर्षणः\n",
      "ur": "gandharvāṁs trāsitān dr̥ṣṭvā citraseno ’tyamarṣaṇaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 21
  },
  "03230021c": {
    "text": {
      "ascii": "utpapAtAsanAt kruddho vadhe teSAM samAhitaH\n",
      "ud": "उत्पपातासनात्क्रुद्धो वधे तेषां समाहितः\n",
      "ur": "utpapātāsanāt kruddho vadhe teṣāṁ samāhitaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 21
  },
  "03230022a": {
    "text": {
      "ascii": "tato mAyAstram AsthAya yuyudhe citramArgavit\n",
      "ud": "ततो मायास्त्रमास्थाय युयुधे चित्रमार्गवित्\n",
      "ur": "tato māyāstram āsthāya yuyudhe citramārgavit\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 22
  },
  "03230022c": {
    "text": {
      "ascii": "tayAmuhyanta kauravyAz citrasenasya mAyayA\n",
      "ud": "तयामुह्यन्त कौरव्याश्चित्रसेनस्य मायया\n",
      "ur": "tayāmuhyanta kauravyāś citrasenasya māyayā\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 22
  },
  "03230023a": {
    "text": {
      "ascii": "ekaiko hi tadA yodho dhArtarASTrasya bhArata\n",
      "ud": "एकैको हि तदा योधो धार्तराष्ट्रस्य भारत\n",
      "ur": "ekaiko hi tadā yodho dhārtarāṣṭrasya bhārata\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 23
  },
  "03230023c": {
    "text": {
      "ascii": "paryavartata gandharvair dazabhir dazabhiH saha\n",
      "ud": "पर्यवर्तत गन्धर्वैर्दशभिर्दशभिः सह\n",
      "ur": "paryavartata gandharvair daśabhir daśabhiḥ saha\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 23
  },
  "03230024a": {
    "text": {
      "ascii": "tataH saMpIDyamAnAs te balena mahatA tadA\n",
      "ud": "ततः संपीड्यमानास्ते बलेन महता तदा\n",
      "ur": "tataḥ saṁpīḍyamānās te balena mahatā tadā\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 24
  },
  "03230024c": {
    "text": {
      "ascii": "prAdravanta raNe bhItA yatra rAjA yudhiSThiraH\n",
      "ud": "प्राद्रवन्त रणे भीता यत्र राजा युधिष्ठिरः\n",
      "ur": "prādravanta raṇe bhītā yatra rājā yudhiṣṭhiraḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 24
  },
  "03230025a": {
    "text": {
      "ascii": "bhajyamAneSv anIkeSu dhArtarASTreSu sarvazaH\n",
      "ud": "भज्यमानेष्वनीकेषु धार्तराष्ट्रेषु सर्वशः\n",
      "ur": "bhajyamāneṣv anīkeṣu dhārtarāṣṭreṣu sarvaśaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 25
  },
  "03230025c": {
    "text": {
      "ascii": "karNo vaikartano rAjaMs tasthau girir ivAcalaH\n",
      "ud": "कर्णो वैकर्तनो राजंस्तस्थौ गिरिरिवाचलः\n",
      "ur": "karṇo vaikartano rājaṁs tasthau girir ivācalaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 25
  },
  "03230026a": {
    "text": {
      "ascii": "duryodhanaz ca karNaz ca zakuniz cApi saubalaH\n",
      "ud": "दुर्योधनश्च कर्णश्च शकुनिश्चापि सौबलः\n",
      "ur": "duryodhanaś ca karṇaś ca śakuniś cāpi saubalaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 26
  },
  "03230026c": {
    "text": {
      "ascii": "gandharvAn yodhayAM cakruH samare bhRzavikSatAH\n",
      "ud": "गन्धर्वान्योधयां चक्रुः समरे भृशविक्षताः\n",
      "ur": "gandharvān yodhayāṁ cakruḥ samare bhr̥śavikṣatāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 26
  },
  "03230027a": {
    "text": {
      "ascii": "sarva eva tu gandharvAH zatazo 'tha sahasrazaH\n",
      "ud": "सर्व एव तु गन्धर्वाः शतशोऽथ सहस्रशः\n",
      "ur": "sarva eva tu gandharvāḥ śataśo ’tha sahasraśaḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 27
  },
  "03230027c": {
    "text": {
      "ascii": "jighAMsamAnAH sahitAH karNam abhyadravan raNe\n",
      "ud": "जिघांसमानाः सहिताः कर्णमभ्यद्रवन्रणे\n",
      "ur": "jighāṁsamānāḥ sahitāḥ karṇam abhyadravan raṇe\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 27
  },
  "03230028a": {
    "text": {
      "ascii": "asibhiH paTTizaiH zUlair gadAbhiz ca mahAbalAH\n",
      "ud": "असिभिः पट्टिशैः शूलैर्गदाभिश्च महाबलाः\n",
      "ur": "asibhiḥ paṭṭiśaiḥ śūlair gadābhiś ca mahābalāḥ\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 28
  },
  "03230028c": {
    "text": {
      "ascii": "sUtaputraM jighAMsantaH samantAt paryavArayan\n",
      "ud": "सूतपुत्रं जिघांसन्तः समन्तात्पर्यवारयन्\n",
      "ur": "sūtaputraṁ jighāṁsantaḥ samantāt paryavārayan\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 28
  },
  "03230029a": {
    "text": {
      "ascii": "anye 'sya yugam acchindan dhvajam anye nyapAtayan\n",
      "ud": "अन्येऽस्य युगमच्छिन्दन्ध्वजमन्ये न्यपातयन्\n",
      "ur": "anye ’sya yugam acchindan dhvajam anye nyapātayan\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 29
  },
  "03230029c": {
    "text": {
      "ascii": "ISAm anye hayAn anye sUtam anye nyapAtayan\n",
      "ud": "ईषामन्ये हयानन्ये सूतमन्ये न्यपातयन्\n",
      "ur": "īṣām anye hayān anye sūtam anye nyapātayan\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 29
  },
  "03230030a": {
    "text": {
      "ascii": "anye chatraM varUthaM ca bandhuraM ca tathApare\n",
      "ud": "अन्ये छत्रं वरूथं च बन्धुरं च तथापरे\n",
      "ur": "anye chatraṁ varūthaṁ ca bandhuraṁ ca tathāpare\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 30
  },
  "03230030c": {
    "text": {
      "ascii": "gandharvA bahusAhasrAH khaNDazo 'bhyahanan ratham\n",
      "ud": "गन्धर्वा बहुसाहस्राः खण्डशोऽभ्यहनन्रथम्\n",
      "ur": "gandharvā bahusāhasrāḥ khaṇḍaśo ’bhyahanan ratham\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 30
  },
  "03230031a": {
    "text": {
      "ascii": "tato rathAd avaplutya sUtaputro 'sicarmabhRt\n",
      "ud": "ततो रथादवप्लुत्य सूतपुत्रोऽसिचर्मभृत्\n",
      "ur": "tato rathād avaplutya sūtaputro ’sicarmabhr̥t\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 31
  },
  "03230031c": {
    "text": {
      "ascii": "vikarNaratham AsthAya mokSAyAzvAn acodayat\n",
      "ud": "विकर्णरथमास्थाय मोक्षायाश्वानचोदयत्\n",
      "ur": "vikarṇaratham āsthāya mokṣāyāśvān acodayat\n"
    },
    "book": 3,
    "chapter": 230,
    "verse": 31
  }

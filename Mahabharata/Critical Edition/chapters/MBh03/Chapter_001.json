    "book": 3,
    "chapter": 1,
    "verse": 5
  },
  "03001005c": {
    "text": {
      "ascii": "vyatIyur brAhmaNazreSTha zUrANAm arighAtinAm\n",
      "ud": "व्यतीयुर्ब्राह्मणश्रेष्ठ शूराणामरिघातिनाम्\n",
      "ur": "vyatīyur brāhmaṇaśreṣṭha śūrāṇām arighātinām\n"
    },
    "book": 3,
    "chapter": 1,
    "verse": 5
  },
  "03001006a": {
    "text": {
      "ascii": "kathaM ca rAjaputrI sA pravarA sarvayoSitAm\n",
      "ud": "कथं च राजपुत्री सा प्रवरा सर्वयोषिताम्\n",
      "ur": "kathaṁ ca rājaputrī sā pravarā sarvayoṣitām\n"
    },
    "book": 3,
    "chapter": 1,
    "verse": 6
  },
  "03001006c": {
    "text": {
      "ascii": "pativratA mahAbhAgA satataM satyavAdinI\n",
      "ud": "पतिव्रता महाभागा सततं सत्यवादिनी\n",
      "ur": "pativratā mahābhāgā satataṁ satyavādinī\n"
    },
    "book": 3,
    "chapter": 1,
    "verse": 6
  },
  "03001006e": {
    "text": {
      "ascii": "vanavAsam aduHkhArhA dAruNaM pratyapadyata\n",
      "ud": "वनवासमदुःखार्हा दारुणं प्रत्यपद्यत\n",
      "ur": "vanavāsam aduḥkhārhā dāruṇaṁ pratyapadyata\n"
    },
    "book": 3,
    "chapter": 1,
    "verse": 6
  },
  "03001007a": {
    "text": {
      "ascii": "etad AcakSva me sarvaM vistareNa tapodhana\n",
      "ud": "एतदाचक्ष्व मे सर्वं विस्तरेण तपोधन\n",
      "ur": "etad ācakṣva me sarvaṁ vistareṇa tapodhana\n"
    },
    "book": 3,
    "chapter": 1,
    "verse": 7
  },
  "03001007c": {
    "text": {
      "ascii": "zrotum icchAmi caritaM bhUridraviNatejasAm\n",
      "ud": "श्रोतुमिच्छामि चरितं भूरिद्रविणतेजसाम्\n",

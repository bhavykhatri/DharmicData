    "chapter": 1,
    "verse": 11
  },
  "03001011e": {
    "text": {
      "ascii": "Ucur vigatasaMtrAsAH samAgamya parasparam\n",
      "ud": "ऊचुर्विगतसंत्रासाः समागम्य परस्परम्\n",

{
  "03248001": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 1
  },
  "03248001a": {
    "text": {
      "ascii": "tasmin bahumRge 'raNye ramamANA mahArathAH\n",
      "ud": "तस्मिन्बहुमृगेऽरण्ये रममाणा महारथाः\n",
      "ur": "tasmin bahumr̥ge ’raṇye ramamāṇā mahārathāḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 1
  },
  "03248001c": {
    "text": {
      "ascii": "kAmyake bharatazreSThA vijahrus te yathAmarAH\n",
      "ud": "काम्यके भरतश्रेष्ठा विजह्रुस्ते यथामराः\n",
      "ur": "kāmyake bharataśreṣṭhā vijahrus te yathāmarāḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 1
  },
  "03248002a": {
    "text": {
      "ascii": "prekSamANA bahuvidhAn vanoddezAn samantataH\n",
      "ud": "प्रेक्षमाणा बहुविधान्वनोद्देशान्समन्ततः\n",
      "ur": "prekṣamāṇā bahuvidhān vanoddeśān samantataḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 2
  },
  "03248002c": {
    "text": {
      "ascii": "yathartukAlaramyAz ca vanarAjIH supuSpitAH\n",
      "ud": "यथर्तुकालरम्याश्च वनराजीः सुपुष्पिताः\n",
      "ur": "yathartukālaramyāś ca vanarājīḥ supuṣpitāḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 2
  },
  "03248003a": {
    "text": {
      "ascii": "pANDavA mRgayAzIlAz carantas tan mahAvanam\n",
      "ud": "पाण्डवा मृगयाशीलाश्चरन्तस्तन्महावनम्\n",
      "ur": "pāṇḍavā mr̥gayāśīlāś carantas tan mahāvanam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 3
  },
  "03248003c": {
    "text": {
      "ascii": "vijahrur indrapratimAH kaM cit kAlam ariMdamAH\n",
      "ud": "विजह्रुरिन्द्रप्रतिमाः कंचित्कालमरिंदमाः\n",
      "ur": "vijahrur indrapratimāḥ kaṁ cit kālam ariṁdamāḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 3
  },
  "03248004a": {
    "text": {
      "ascii": "tatas te yaugapadyena yayuH sarve caturdizam\n",
      "ud": "ततस्ते यौगपद्येन ययुः सर्वे चतुर्दिशम्\n",
      "ur": "tatas te yaugapadyena yayuḥ sarve caturdiśam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 4
  },
  "03248004c": {
    "text": {
      "ascii": "mRgayAM puruSavyAghrA brAhmaNArthe paraMtapAH\n",
      "ud": "मृगयां पुरुषव्याघ्रा ब्राह्मणार्थे परंतपाः\n",
      "ur": "mr̥gayāṁ puruṣavyāghrā brāhmaṇārthe paraṁtapāḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 4
  },
  "03248005a": {
    "text": {
      "ascii": "draupadIm Azrame nyasya tRNabindor anujJayA\n",
      "ud": "द्रौपदीमाश्रमे न्यस्य तृणबिन्दोरनुज्ञया\n",
      "ur": "draupadīm āśrame nyasya tr̥ṇabindor anujñayā\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 5
  },
  "03248005c": {
    "text": {
      "ascii": "maharSer dIptatapaso dhaumyasya ca purodhasaH\n",
      "ud": "महर्षेर्दीप्ततपसो धौम्यस्य च पुरोधसः\n",
      "ur": "maharṣer dīptatapaso dhaumyasya ca purodhasaḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 5
  },
  "03248006a": {
    "text": {
      "ascii": "tatas tu rAjA sindhUnAM vArddhakSatrir mahAyazAH\n",
      "ud": "ततस्तु राजा सिन्धूनां वार्द्धक्षत्रिर्महायशाः\n",
      "ur": "tatas tu rājā sindhūnāṁ vārddhakṣatrir mahāyaśāḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 6
  },
  "03248006c": {
    "text": {
      "ascii": "vivAhakAmaH zAlveyAn prayAtaH so 'bhavat tadA\n",
      "ud": "विवाहकामः शाल्वेयान्प्रयातः सोऽभवत्तदा\n",
      "ur": "vivāhakāmaḥ śālveyān prayātaḥ so ’bhavat tadā\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 6
  },
  "03248007a": {
    "text": {
      "ascii": "mahatA paribarheNa rAjayogyena saMvRtaH\n",
      "ud": "महता परिबर्हेण राजयोग्येन संवृतः\n",
      "ur": "mahatā paribarheṇa rājayogyena saṁvr̥taḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 7
  },
  "03248007c": {
    "text": {
      "ascii": "rAjabhir bahubhiH sArdham upAyAt kAmyakaM ca saH\n",
      "ud": "राजभिर्बहुभिः सार्धमुपायात्काम्यकं च सः\n",
      "ur": "rājabhir bahubhiḥ sārdham upāyāt kāmyakaṁ ca saḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 7
  },
  "03248008a": {
    "text": {
      "ascii": "tatrApazyat priyAM bhAryAM pANDavAnAM yazasvinIm\n",
      "ud": "तत्रापश्यत्प्रियां भार्यां पाण्डवानां यशस्विनीम्\n",
      "ur": "tatrāpaśyat priyāṁ bhāryāṁ pāṇḍavānāṁ yaśasvinīm\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 8
  },
  "03248008c": {
    "text": {
      "ascii": "tiSThantIm AzramadvAri draupadIM nirjane vane\n",
      "ud": "तिष्ठन्तीमाश्रमद्वारि द्रौपदीं निर्जने वने\n",
      "ur": "tiṣṭhantīm āśramadvāri draupadīṁ nirjane vane\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 8
  },
  "03248009a": {
    "text": {
      "ascii": "vibhrAjamAnAM vapuSA bibhratIM rUpam uttamam\n",
      "ud": "विभ्राजमानां वपुषा बिभ्रतीं रूपमुत्तमम्\n",
      "ur": "vibhrājamānāṁ vapuṣā bibhratīṁ rūpam uttamam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 9
  },
  "03248009c": {
    "text": {
      "ascii": "bhrAjayantIM vanoddezaM nIlAbhram iva vidyutam\n",
      "ud": "भ्राजयन्तीं वनोद्देशं नीलाभ्रमिव विद्युतम्\n",
      "ur": "bhrājayantīṁ vanoddeśaṁ nīlābhram iva vidyutam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 9
  },
  "03248010a": {
    "text": {
      "ascii": "apsarA devakanyA vA mAyA vA devanirmitA\n",
      "ud": "अप्सरा देवकन्या वा माया वा देवनिर्मिता\n",
      "ur": "apsarā devakanyā vā māyā vā devanirmitā\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 10
  },
  "03248010c": {
    "text": {
      "ascii": "iti kRtvAJjaliM sarve dadRzus tAm aninditAm\n",
      "ud": "इति कृत्वाञ्जलिं सर्वे ददृशुस्तामनिन्दिताम्\n",
      "ur": "iti kr̥tvāñjaliṁ sarve dadr̥śus tām aninditām\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 10
  },
  "03248011a": {
    "text": {
      "ascii": "tataH sa rAjA sindhUnAM vArddhakSatrir jayadrathaH\n",
      "ud": "ततः स राजा सिन्धूनां वार्द्धक्षत्रिर्जयद्रथः\n",
      "ur": "tataḥ sa rājā sindhūnāṁ vārddhakṣatrir jayadrathaḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 11
  },
  "03248011c": {
    "text": {
      "ascii": "vismitas tAm anindyAGgIM dRSTvAsId dhRSTamAnasaH\n",
      "ud": "विस्मितस्तामनिन्द्याङ्गीं दृष्ट्वासीद्धृष्टमानसः\n",
      "ur": "vismitas tām anindyāṅgīṁ dr̥ṣṭvāsīd dhr̥ṣṭamānasaḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 11
  },
  "03248012a": {
    "text": {
      "ascii": "sa koTikAzyaM rAjAnam abravIt kAmamohitaH\n",
      "ud": "स कोटिकाश्यं राजानमब्रवीत्काममोहितः\n",
      "ur": "sa koṭikāśyaṁ rājānam abravīt kāmamohitaḥ\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 12
  },
  "03248012c": {
    "text": {
      "ascii": "kasya tv eSAnavadyAGgI yadi vApi na mAnuSI\n",
      "ud": "कस्य त्वेषानवद्याङ्गी यदि वापि न मानुषी\n",
      "ur": "kasya tv eṣānavadyāṅgī yadi vāpi na mānuṣī\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 12
  },
  "03248013a": {
    "text": {
      "ascii": "vivAhArtho na me kaz cid imAM dRSTvAtisundarIm\n",
      "ud": "विवाहार्थो न मे कश्चिदिमां दृष्ट्वातिसुन्दरीम्\n",
      "ur": "vivāhārtho na me kaś cid imāṁ dr̥ṣṭvātisundarīm\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 13
  },
  "03248013c": {
    "text": {
      "ascii": "etAm evAham AdAya gamiSyAmi svam Alayam\n",
      "ud": "एतामेवाहमादाय गमिष्यामि स्वमालयम्\n",
      "ur": "etām evāham ādāya gamiṣyāmi svam ālayam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 13
  },
  "03248014a": {
    "text": {
      "ascii": "gaccha jAnIhi saumyainAM kasya kA ca kuto 'pi vA\n",
      "ud": "गच्छ जानीहि सौम्यैनां कस्य का च कुतोऽपि वा\n",
      "ur": "gaccha jānīhi saumyaināṁ kasya kā ca kuto ’pi vā\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 14
  },
  "03248014c": {
    "text": {
      "ascii": "kimartham AgatA subhrUr idaM kaNTakitaM vanam\n",
      "ud": "किमर्थमागता सुभ्रूरिदं कण्टकितं वनम्\n",
      "ur": "kimartham āgatā subhrūr idaṁ kaṇṭakitaṁ vanam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 14
  },
  "03248015a": {
    "text": {
      "ascii": "api nAma varArohA mAm eSA lokasundarI\n",
      "ud": "अपि नाम वरारोहा मामेषा लोकसुन्दरी\n",
      "ur": "api nāma varārohā mām eṣā lokasundarī\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 15
  },
  "03248015c": {
    "text": {
      "ascii": "bhajed adyAyatApAGgI sudatI tanumadhyamA\n",
      "ud": "भजेदद्यायतापाङ्गी सुदती तनुमध्यमा\n",
      "ur": "bhajed adyāyatāpāṅgī sudatī tanumadhyamā\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 15
  },
  "03248016a": {
    "text": {
      "ascii": "apy ahaM kRtakAmaH syAm imAM prApya varastriyam\n",
      "ud": "अप्यहं कृतकामः स्यामिमां प्राप्य वरस्त्रियम्\n",
      "ur": "apy ahaṁ kr̥takāmaḥ syām imāṁ prāpya varastriyam\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 16
  },
  "03248016c": {
    "text": {
      "ascii": "gaccha jAnIhi ko nv asyA nAtha ity eva koTika\n",
      "ud": "गच्छ जानीहि को न्वस्या नाथ इत्येव कोटिक\n",
      "ur": "gaccha jānīhi ko nv asyā nātha ity eva koṭika\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 16
  },
  "03248017a": {
    "text": {
      "ascii": "sa koTikAzyas tac chrutvA rathAt praskandya kuNDalI\n",
      "ud": "स कोटिकाश्यस्तच्छ्रुत्वा रथात्प्रस्कन्द्य कुण्डली\n",
      "ur": "sa koṭikāśyas tac chrutvā rathāt praskandya kuṇḍalī\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 17
  },
  "03248017c": {
    "text": {
      "ascii": "upetya papraccha tadA kroSTA vyAghravadhUm iva\n",
      "ud": "उपेत्य पप्रच्छ तदा क्रोष्टा व्याघ्रवधूमिव\n",
      "ur": "upetya papraccha tadā kroṣṭā vyāghravadhūm iva\n"
    },
    "book": 3,
    "chapter": 248,
    "verse": 17
  }
}
{
  "03208001": {
    "text": {
      "ascii": "mArkaNDeya uvAca\n",
      "ud": "मार्कण्डेय उवाच\n",
      "ur": "mārkaṇḍeya uvāca\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 1
  },
  "03208001a": {
    "text": {
      "ascii": "brahmaNo yas tRtIyas tu putraH kurukulodvaha\n",
      "ud": "ब्रह्मणो यस्तृतीयस्तु पुत्रः कुरुकुलोद्वह\n",
      "ur": "brahmaṇo yas tr̥tīyas tu putraḥ kurukulodvaha\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 1
  },
  "03208001c": {
    "text": {
      "ascii": "tasyApavasutA bhAryA prajAs tasyApi me zRNu\n",
      "ud": "तस्यापवसुता भार्या प्रजास्तस्यापि मे शृणु\n",
      "ur": "tasyāpavasutā bhāryā prajās tasyāpi me śr̥ṇu\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 1
  },
  "03208002a": {
    "text": {
      "ascii": "bRhajjyotir bRhatkIrtir bRhadbrahmA bRhanmanAH\n",
      "ud": "बृहज्ज्योतिर्बृहत्कीर्तिर्बृहद्ब्रह्मा बृहन्मनाः\n",
      "ur": "br̥hajjyotir br̥hatkīrtir br̥hadbrahmā br̥hanmanāḥ\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 2
  },
  "03208002c": {
    "text": {
      "ascii": "bRhanmantro bRhadbhAsas tathA rAjan bRhaspatiH\n",
      "ud": "बृहन्मन्त्रो बृहद्भासस्तथा राजन्बृहस्पतिः\n",
      "ur": "br̥hanmantro br̥hadbhāsas tathā rājan br̥haspatiḥ\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 2
  },
  "03208003a": {
    "text": {
      "ascii": "prajAsu tAsu sarvAsu rUpeNApratimAbhavat\n",
      "ud": "प्रजासु तासु सर्वासु रूपेणाप्रतिमाभवत्\n",
      "ur": "prajāsu tāsu sarvāsu rūpeṇāpratimābhavat\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 3
  },
  "03208003c": {
    "text": {
      "ascii": "devI bhAnumatI nAma prathamAGgirasaH sutA\n",
      "ud": "देवी भानुमती नाम प्रथमाङ्गिरसः सुता\n",
      "ur": "devī bhānumatī nāma prathamāṅgirasaḥ sutā\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 3
  },
  "03208004a": {
    "text": {
      "ascii": "bhUtAnAm eva sarveSAM yasyAM rAgas tadAbhavat\n",
      "ud": "भूतानामेव सर्वेषां यस्यां रागस्तदाभवत्\n",
      "ur": "bhūtānām eva sarveṣāṁ yasyāṁ rāgas tadābhavat\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 4
  },
  "03208004c": {
    "text": {
      "ascii": "rAgAd rAgeti yAm Ahur dvitIyAGgirasaH sutA\n",
      "ud": "रागाद्रागेति यामाहुर्द्वितीयाङ्गिरसः सुता\n",
      "ur": "rāgād rāgeti yām āhur dvitīyāṅgirasaḥ sutā\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 4
  },
  "03208005a": {
    "text": {
      "ascii": "yAM kapardisutAm Ahur dRzyAdRzyeti dehinaH\n",
      "ud": "यां कपर्दिसुतामाहुर्दृश्यादृश्येति देहिनः\n",
      "ur": "yāṁ kapardisutām āhur dr̥śyādr̥śyeti dehinaḥ\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 5
  },
  "03208005c": {
    "text": {
      "ascii": "tanutvAt sA sinIvAlI tRtIyAGgirasaH sutA\n",
      "ud": "तनुत्वात्सा सिनीवाली तृतीयाङ्गिरसः सुता\n",
      "ur": "tanutvāt sā sinīvālī tr̥tīyāṅgirasaḥ sutā\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 5
  },
  "03208006a": {
    "text": {
      "ascii": "pazyaty arciSmatI bhAbhir havirbhiz ca haviSmatI\n",
      "ud": "पश्यत्यर्चिष्मती भाभिर्हविर्भिश्च हविष्मती\n",
      "ur": "paśyaty arciṣmatī bhābhir havirbhiś ca haviṣmatī\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 6
  },
  "03208006c": {
    "text": {
      "ascii": "SaSThIm aGgirasaH kanyAM puNyAm Ahur haviSmatIm\n",
      "ud": "षष्ठीमङ्गिरसः कन्यां पुण्यामाहुर्हविष्मतीम्\n",
      "ur": "ṣaṣṭhīm aṅgirasaḥ kanyāṁ puṇyām āhur haviṣmatīm\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 6
  },
  "03208007a": {
    "text": {
      "ascii": "mahAmakheSv AGgirasI dIptimatsu mahAmatI\n",
      "ud": "महामखेष्वाङ्गिरसी दीप्तिमत्सु महामती\n",
      "ur": "mahāmakheṣv āṅgirasī dīptimatsu mahāmatī\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 7
  },
  "03208007c": {
    "text": {
      "ascii": "mahAmatIti vikhyAtA saptamI kathyate sutA\n",
      "ud": "महामतीति विख्याता सप्तमी कथ्यते सुता\n",
      "ur": "mahāmatīti vikhyātā saptamī kathyate sutā\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 7
  },
  "03208008a": {
    "text": {
      "ascii": "yAM tu dRSTvA bhagavatIM janaH kuhukuhAyate\n",
      "ud": "यां तु दृष्ट्वा भगवतीं जनः कुहुकुहायते\n",
      "ur": "yāṁ tu dr̥ṣṭvā bhagavatīṁ janaḥ kuhukuhāyate\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 8
  },
  "03208008c": {
    "text": {
      "ascii": "ekAnaMzeti yAm AhuH kuhUm aGgirasaH sutAm\n",
      "ud": "एकानंशेति यामाहुः कुहूमङ्गिरसः सुताम्\n",
      "ur": "ekānaṁśeti yām āhuḥ kuhūm aṅgirasaḥ sutām\n"
    },
    "book": 3,
    "chapter": 208,
    "verse": 8
  }
}
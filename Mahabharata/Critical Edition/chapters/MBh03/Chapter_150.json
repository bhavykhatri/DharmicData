    "book": 3,
    "chapter": 150,
    "verse": 5
  },
  "03150006a": {
    "text": {
      "ascii": "mamApi saphalaM cakSuH smAritaz cAsmi rAghavam\n",
      "ud": "ममापि सफलं चक्षुः स्मारितश्चास्मि राघवम्\n",
      "ur": "mamāpi saphalaṁ cakṣuḥ smāritaś cāsmi rāghavam\n"

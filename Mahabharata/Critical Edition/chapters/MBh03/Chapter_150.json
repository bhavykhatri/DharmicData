    "book": 3,
    "chapter": 150,
    "verse": 4
  },
  "03150004c": {
    "text": {
      "ascii": "ihasthaz ca kuruzreSTha na nivedyo 'smi kasya cit\n",
      "ud": "इहस्थश्च कुरुश्रेष्ठ न निवेद्योऽस्मि कस्यचित्\n",
      "ur": "ihasthaś ca kuruśreṣṭha na nivedyo ’smi kasya cit\n"
    },
    "book": 3,
    "chapter": 150,
    "verse": 4
  },
  "03150005a": {
    "text": {
      "ascii": "dhanadasyAlayAc cApi visRSTAnAM mahAbala\n",
      "ud": "धनदस्यालयाच्चापि विसृष्टानां महाबल\n",
      "ur": "dhanadasyālayāc cāpi visr̥ṣṭānāṁ mahābala\n"

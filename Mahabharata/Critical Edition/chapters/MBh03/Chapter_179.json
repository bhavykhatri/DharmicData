{
  "03179001": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 1
  },
  "03179001a": {
    "text": {
      "ascii": "nidAghAntakaraH kAlaH sarvabhUtasukhAvahaH\n",
      "ud": "निदाघान्तकरः कालः सर्वभूतसुखावहः\n",
      "ur": "nidāghāntakaraḥ kālaḥ sarvabhūtasukhāvahaḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 1
  },
  "03179001c": {
    "text": {
      "ascii": "tatraiva vasatAM teSAM prAvRT samabhipadyata\n",
      "ud": "तत्रैव वसतां तेषां प्रावृट्समभिपद्यत\n",
      "ur": "tatraiva vasatāṁ teṣāṁ prāvr̥ṭ samabhipadyata\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 1
  },
  "03179002a": {
    "text": {
      "ascii": "chAdayanto mahAghoSAH khaM dizaz ca balAhakAH\n",
      "ud": "छादयन्तो महाघोषाः खं दिशश्च बलाहकाः\n",
      "ur": "chādayanto mahāghoṣāḥ khaṁ diśaś ca balāhakāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 2
  },
  "03179002c": {
    "text": {
      "ascii": "pravavarSur divArAtram asitAH satataM tadA\n",
      "ud": "प्रववर्षुर्दिवारात्रमसिताः सततं तदा\n",
      "ur": "pravavarṣur divārātram asitāḥ satataṁ tadā\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 2
  },
  "03179003a": {
    "text": {
      "ascii": "tapAtyayaniketAz ca zatazo 'tha sahasrazaH\n",
      "ud": "तपात्ययनिकेताश्च शतशोऽथ सहस्रशः\n",
      "ur": "tapātyayaniketāś ca śataśo ’tha sahasraśaḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 3
  },
  "03179003c": {
    "text": {
      "ascii": "apetArkaprabhAjAlAH savidyudvimalaprabhAH\n",
      "ud": "अपेतार्कप्रभाजालाः सविद्युद्विमलप्रभाः\n",
      "ur": "apetārkaprabhājālāḥ savidyudvimalaprabhāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 3
  },
  "03179004a": {
    "text": {
      "ascii": "virUDhazaSpA pRthivI mattadaMzasarIsRpA\n",
      "ud": "विरूढशष्पा पृथिवी मत्तदंशसरीसृपा\n",
      "ur": "virūḍhaśaṣpā pr̥thivī mattadaṁśasarīsr̥pā\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 4
  },
  "03179004c": {
    "text": {
      "ascii": "babhUva payasA siktA zAntadhUmarajo 'ruNA\n",
      "ud": "बभूव पयसा सिक्ता शान्तधूमरजोऽरुणा\n",
      "ur": "babhūva payasā siktā śāntadhūmarajo ’ruṇā\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 4
  },
  "03179005a": {
    "text": {
      "ascii": "na sma prajJAyate kiM cid ambhasA samavastRte\n",
      "ud": "न स्म प्रज्ञायते किंचिदम्भसा समवस्तृते\n",
      "ur": "na sma prajñāyate kiṁ cid ambhasā samavastr̥te\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 5
  },
  "03179005c": {
    "text": {
      "ascii": "samaM vA viSamaM vApi nadyo vA sthAvarANi vA\n",
      "ud": "समं वा विषमं वापि नद्यो वा स्थावराणि वा\n",
      "ur": "samaṁ vā viṣamaṁ vāpi nadyo vā sthāvarāṇi vā\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 5
  },
  "03179006a": {
    "text": {
      "ascii": "kSubdhatoyA mahAghoSAH zvasamAnA ivAzugAH\n",
      "ud": "क्षुब्धतोया महाघोषाः श्वसमाना इवाशुगाः\n",
      "ur": "kṣubdhatoyā mahāghoṣāḥ śvasamānā ivāśugāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 6
  },
  "03179006c": {
    "text": {
      "ascii": "sindhavaH zobhayAM cakruH kAnanAni tapAtyaye\n",
      "ud": "सिन्धवः शोभयां चक्रुः काननानि तपात्यये\n",
      "ur": "sindhavaḥ śobhayāṁ cakruḥ kānanāni tapātyaye\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 6
  },
  "03179007a": {
    "text": {
      "ascii": "nadatAM kAnanAnteSu zrUyante vividhAH svanAH\n",
      "ud": "नदतां काननान्तेषु श्रूयन्ते विविधाः स्वनाः\n",
      "ur": "nadatāṁ kānanānteṣu śrūyante vividhāḥ svanāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 7
  },
  "03179007c": {
    "text": {
      "ascii": "vRSTibhis tADyamAnAnAM varAhamRgapakSiNAm\n",
      "ud": "वृष्टिभिस्ताड्यमानानां वराहमृगपक्षिणाम्\n",
      "ur": "vr̥ṣṭibhis tāḍyamānānāṁ varāhamr̥gapakṣiṇām\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 7
  },
  "03179008a": {
    "text": {
      "ascii": "stokakAH zikhinaz caiva puMskokilagaNaiH saha\n",
      "ud": "स्तोककाः शिखिनश्चैव पुंस्कोकिलगणैः सह\n",
      "ur": "stokakāḥ śikhinaś caiva puṁskokilagaṇaiḥ saha\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 8
  },
  "03179008c": {
    "text": {
      "ascii": "mattAH paripatanti sma dardurAz caiva darpitAH\n",
      "ud": "मत्ताः परिपतन्ति स्म दर्दुराश्चैव दर्पिताः\n",
      "ur": "mattāḥ paripatanti sma dardurāś caiva darpitāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 8
  },
  "03179009a": {
    "text": {
      "ascii": "tathA bahuvidhAkArA prAvRN meghAnunAditA\n",
      "ud": "तथा बहुविधाकारा प्रावृण्मेघानुनादिता\n",
      "ur": "tathā bahuvidhākārā prāvr̥ṇ meghānunāditā\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 9
  },
  "03179009c": {
    "text": {
      "ascii": "abhyatItA zivA teSAM caratAM marudhanvasu\n",
      "ud": "अभ्यतीता शिवा तेषां चरतां मरुधन्वसु\n",
      "ur": "abhyatītā śivā teṣāṁ caratāṁ marudhanvasu\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 9
  },
  "03179010a": {
    "text": {
      "ascii": "krauJcahaMsagaNAkIrNA zarat praNihitAbhavat\n",
      "ud": "क्रौञ्चहंसगणाकीर्णा शरत्प्रणिहिताभवत्\n",
      "ur": "krauñcahaṁsagaṇākīrṇā śarat praṇihitābhavat\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 10
  },
  "03179010c": {
    "text": {
      "ascii": "rUDhakakSavanaprasthA prasannajalanimnagA\n",
      "ud": "रूढकक्षवनप्रस्था प्रसन्नजलनिम्नगा\n",
      "ur": "rūḍhakakṣavanaprasthā prasannajalanimnagā\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 10
  },
  "03179011a": {
    "text": {
      "ascii": "vimalAkAzanakSatrA zarat teSAM zivAbhavat\n",
      "ud": "विमलाकाशनक्षत्रा शरत्तेषां शिवाभवत्\n",
      "ur": "vimalākāśanakṣatrā śarat teṣāṁ śivābhavat\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 11
  },
  "03179011c": {
    "text": {
      "ascii": "mRgadvijasamAkIrNA pANDavAnAM mahAtmanAm\n",
      "ud": "मृगद्विजसमाकीर्णा पाण्डवानां महात्मनाम्\n",
      "ur": "mr̥gadvijasamākīrṇā pāṇḍavānāṁ mahātmanām\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 11
  },
  "03179012a": {
    "text": {
      "ascii": "pazyantaH zAntarajasaH kSapA jaladazItalAH\n",
      "ud": "पश्यन्तः शान्तरजसः क्षपा जलदशीतलाः\n",
      "ur": "paśyantaḥ śāntarajasaḥ kṣapā jaladaśītalāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 12
  },
  "03179012c": {
    "text": {
      "ascii": "grahanakSatrasaMghaiz ca somena ca virAjitAH\n",
      "ud": "ग्रहनक्षत्रसंघैश्च सोमेन च विराजिताः\n",
      "ur": "grahanakṣatrasaṁghaiś ca somena ca virājitāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 12
  },
  "03179013a": {
    "text": {
      "ascii": "kumudaiH puNDarIkaiz ca zItavAridharAH zivAH\n",
      "ud": "कुमुदैः पुण्डरीकैश्च शीतवारिधराः शिवाः\n",
      "ur": "kumudaiḥ puṇḍarīkaiś ca śītavāridharāḥ śivāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 13
  },
  "03179013c": {
    "text": {
      "ascii": "nadIH puSkariNIz caiva dadRzuH samalaMkRtAH\n",
      "ud": "नदीः पुष्करिणीश्चैव ददृशुः समलंकृताः\n",
      "ur": "nadīḥ puṣkariṇīś caiva dadr̥śuḥ samalaṁkr̥tāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 13
  },
  "03179014a": {
    "text": {
      "ascii": "AkAzanIkAzataTAM nIpanIvArasaMkulAm\n",
      "ud": "आकाशनीकाशतटां नीपनीवारसंकुलाम्\n",
      "ur": "ākāśanīkāśataṭāṁ nīpanīvārasaṁkulām\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 14
  },
  "03179014c": {
    "text": {
      "ascii": "babhUva caratAM harSaH puNyatIrthAM sarasvatIm\n",
      "ud": "बभूव चरतां हर्षः पुण्यतीर्थां सरस्वतीम्\n",
      "ur": "babhūva caratāṁ harṣaḥ puṇyatīrthāṁ sarasvatīm\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 14
  },
  "03179015a": {
    "text": {
      "ascii": "te vai mumudire vIrAH prasannasalilAM zivAm\n",
      "ud": "ते वै मुमुदिरे वीराः प्रसन्नसलिलां शिवाम्\n",
      "ur": "te vai mumudire vīrāḥ prasannasalilāṁ śivām\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 15
  },
  "03179015c": {
    "text": {
      "ascii": "pazyanto dRDhadhanvAnaH paripUrNAM sarasvatIm\n",
      "ud": "पश्यन्तो दृढधन्वानः परिपूर्णां सरस्वतीम्\n",
      "ur": "paśyanto dr̥ḍhadhanvānaḥ paripūrṇāṁ sarasvatīm\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 15
  },
  "03179016a": {
    "text": {
      "ascii": "teSAM puNyatamA rAtriH parvasaMdhau sma zAradI\n",
      "ud": "तेषां पुण्यतमा रात्रिः पर्वसंधौ स्म शारदी\n",
      "ur": "teṣāṁ puṇyatamā rātriḥ parvasaṁdhau sma śāradī\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 16
  },
  "03179016c": {
    "text": {
      "ascii": "tatraiva vasatAm AsIt kArttikI janamejaya\n",
      "ud": "तत्रैव वसतामासीत्कार्त्तिकी जनमेजय\n",
      "ur": "tatraiva vasatām āsīt kārttikī janamejaya\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 16
  },
  "03179017a": {
    "text": {
      "ascii": "puNyakRdbhir mahAsattvais tApasaiH saha pANDavAH\n",
      "ud": "पुण्यकृद्भिर्महासत्त्वैस्तापसैः सह पाण्डवाः\n",
      "ur": "puṇyakr̥dbhir mahāsattvais tāpasaiḥ saha pāṇḍavāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 17
  },
  "03179017c": {
    "text": {
      "ascii": "tat sarvaM bharatazreSThAH samUhur yogam uttamam\n",
      "ud": "तत्सर्वं भरतश्रेष्ठाः समूहुर्योगमुत्तमम्\n",
      "ur": "tat sarvaṁ bharataśreṣṭhāḥ samūhur yogam uttamam\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 17
  },
  "03179018a": {
    "text": {
      "ascii": "tamisrAbhyudaye tasmin dhaumyena saha pANDavAH\n",
      "ud": "तमिस्राभ्युदये तस्मिन्धौम्येन सह पाण्डवाः\n",
      "ur": "tamisrābhyudaye tasmin dhaumyena saha pāṇḍavāḥ\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 18
  },
  "03179018c": {
    "text": {
      "ascii": "sUtaiH paurogavaiz caiva kAmyakaM prayayur vanam\n",
      "ud": "सूतैः पौरोगवैश्चैव काम्यकं प्रययुर्वनम्\n",
      "ur": "sūtaiḥ paurogavaiś caiva kāmyakaṁ prayayur vanam\n"
    },
    "book": 3,
    "chapter": 179,
    "verse": 18
  }
}
    "book": 3,
    "chapter": 274,
    "verse": 13
  },
  "03274013e": {
    "text": {
      "ascii": "zatazaH puruSavyAghra rathodAreNa jaghnivAn\n",
      "ud": "शतशः पुरुषव्याघ्र रथोदारेण जघ्निवान्\n",

    },
    "book": 3,
    "chapter": 274,
    "verse": 12
  },
  "03274013": {
    "text": {
      "ascii": "mAtalir uvAca\n",
      "ud": "मातलिरुवाच\n",
      "ur": "mātalir uvāca\n"

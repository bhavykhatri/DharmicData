    "book": 3,
    "chapter": 274,
    "verse": 1
  },
  "03274001a": {
    "text": {
      "ascii": "tataH kruddho dazagrIvaH priye putre nipAtite\n",
      "ud": "ततः क्रुद्धो दशग्रीवः प्रिये पुत्रे निपातिते\n",
      "ur": "tataḥ kruddho daśagrīvaḥ priye putre nipātite\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 1
  },
  "03274001c": {
    "text": {
      "ascii": "niryayau ratham AsthAya hemaratnavibhUSitam\n",
      "ud": "निर्ययौ रथमास्थाय हेमरत्नविभूषितम्\n",
      "ur": "niryayau ratham āsthāya hemaratnavibhūṣitam\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 1
  },
  "03274002a": {
    "text": {
      "ascii": "saMvRto rAkSasair ghorair vividhAyudhapANibhiH\n",
      "ud": "संवृतो राक्षसैर्घोरैर्विविधायुधपाणिभिः\n",
      "ur": "saṁvr̥to rākṣasair ghorair vividhāyudhapāṇibhiḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 2
  },
  "03274002c": {
    "text": {
      "ascii": "abhidudrAva rAmaM sa pothayan hariyUthapAn\n",
      "ud": "अभिदुद्राव रामं स पोथयन्हरियूथपान्\n",
      "ur": "abhidudrāva rāmaṁ sa pothayan hariyūthapān\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 2
  },
  "03274003a": {
    "text": {
      "ascii": "tam AdravantaM saMkruddhaM maindanIlanalAGgadAH\n",
      "ud": "तमाद्रवन्तं संक्रुद्धं मैन्दनीलनलाङ्गदाः\n",
      "ur": "tam ādravantaṁ saṁkruddhaṁ maindanīlanalāṅgadāḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 3
  },
  "03274003c": {
    "text": {
      "ascii": "hanUmAJ jAmbavAMz caiva sasainyAH paryavArayan\n",
      "ud": "हनूमाञ्जाम्बवांश्चैव ससैन्याः पर्यवारयन्\n",
      "ur": "hanūmāñ jāmbavāṁś caiva sasainyāḥ paryavārayan\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 3
  },
  "03274004a": {
    "text": {
      "ascii": "te dazagrIvasainyaM tad RkSavAnarayUthapAH\n",
      "ud": "ते दशग्रीवसैन्यं तदृक्षवानरयूथपाः\n",
      "ur": "te daśagrīvasainyaṁ tad r̥kṣavānarayūthapāḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 4
  },
  "03274004c": {
    "text": {
      "ascii": "drumair vidhvaMsayAM cakrur dazagrIvasya pazyataH\n",
      "ud": "द्रुमैर्विध्वंसयां चक्रुर्दशग्रीवस्य पश्यतः\n",
      "ur": "drumair vidhvaṁsayāṁ cakrur daśagrīvasya paśyataḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 4
  },
  "03274005a": {
    "text": {
      "ascii": "tataH svasainyam Alokya vadhyamAnam arAtibhiH\n",
      "ud": "ततः स्वसैन्यमालोक्य वध्यमानमरातिभिः\n",
      "ur": "tataḥ svasainyam ālokya vadhyamānam arātibhiḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 5
  },
  "03274005c": {
    "text": {
      "ascii": "mAyAvI vyadadhAn mAyAM rAvaNo rAkSasezvaraH\n",
      "ud": "मायावी व्यदधान्मायां रावणो राक्षसेश्वरः\n",
      "ur": "māyāvī vyadadhān māyāṁ rāvaṇo rākṣaseśvaraḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 5
  },
  "03274006a": {
    "text": {
      "ascii": "tasya dehAd viniSkrAntAH zatazo 'tha sahasrazaH\n",
      "ud": "तस्य देहाद्विनिष्क्रान्ताः शतशोऽथ सहस्रशः\n",
      "ur": "tasya dehād viniṣkrāntāḥ śataśo ’tha sahasraśaḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 6
  },
  "03274006c": {
    "text": {
      "ascii": "rAkSasAH pratyadRzyanta zarazaktyRSTipANayaH\n",
      "ud": "राक्षसाः प्रत्यदृश्यन्त शरशक्त्यृष्टिपाणयः\n",
      "ur": "rākṣasāḥ pratyadr̥śyanta śaraśaktyr̥ṣṭipāṇayaḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 6
  },
  "03274007a": {
    "text": {
      "ascii": "tAn rAmo jaghnivAn sarvAn divyenAstreNa rAkSasAn\n",
      "ud": "तान्रामो जघ्निवान्सर्वान्दिव्येनास्त्रेण राक्षसान्\n",
      "ur": "tān rāmo jaghnivān sarvān divyenāstreṇa rākṣasān\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 7
  },
  "03274007c": {
    "text": {
      "ascii": "atha bhUyo 'pi mAyAM sa vyadadhAd rAkSasAdhipaH\n",
      "ud": "अथ भूयोऽपि मायां स व्यदधाद्राक्षसाधिपः\n",
      "ur": "atha bhūyo ’pi māyāṁ sa vyadadhād rākṣasādhipaḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 7
  },
  "03274008a": {
    "text": {
      "ascii": "kRtvA rAmasya rUpANi lakSmaNasya ca bhArata\n",
      "ud": "कृत्वा रामस्य रूपाणि लक्ष्मणस्य च भारत\n",
      "ur": "kr̥tvā rāmasya rūpāṇi lakṣmaṇasya ca bhārata\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 8
  },
  "03274008c": {
    "text": {
      "ascii": "abhidudrAva rAmaM ca lakSmaNaM ca dazAnanaH\n",
      "ud": "अभिदुद्राव रामं च लक्ष्मणं च दशाननः\n",
      "ur": "abhidudrāva rāmaṁ ca lakṣmaṇaṁ ca daśānanaḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 8
  },
  "03274009a": {
    "text": {
      "ascii": "tatas te rAmam archanto lakSmaNaM ca kSapAcarAH\n",
      "ud": "ततस्ते राममर्छन्तो लक्ष्मणं च क्षपाचराः\n",
      "ur": "tatas te rāmam archanto lakṣmaṇaṁ ca kṣapācarāḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 9
  },
  "03274009c": {
    "text": {
      "ascii": "abhipetus tadA rAjan pragRhItoccakArmukAH\n",
      "ud": "अभिपेतुस्तदा राजन्प्रगृहीतोच्चकार्मुकाः\n",
      "ur": "abhipetus tadā rājan pragr̥hītoccakārmukāḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 9
  },
  "03274010a": {
    "text": {
      "ascii": "tAM dRSTvA rAkSasendrasya mAyAm ikSvAkunandanaH\n",
      "ud": "तां दृष्ट्वा राक्षसेन्द्रस्य मायामिक्ष्वाकुनन्दनः\n",
      "ur": "tāṁ dr̥ṣṭvā rākṣasendrasya māyām ikṣvākunandanaḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 10
  },
  "03274010c": {
    "text": {
      "ascii": "uvAca rAmaM saumitrir asaMbhrAnto bRhad vacaH\n",
      "ud": "उवाच रामं सौमित्रिरसंभ्रान्तो बृहद्वचः\n",
      "ur": "uvāca rāmaṁ saumitrir asaṁbhrānto br̥had vacaḥ\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 10
  },
  "03274011a": {
    "text": {
      "ascii": "jahImAn rAkSasAn pApAn AtmanaH pratirUpakAn\n",
      "ud": "जहीमान्राक्षसान्पापानात्मनः प्रतिरूपकान्\n",
      "ur": "jahīmān rākṣasān pāpān ātmanaḥ pratirūpakān\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 11
  },
  "03274011c": {
    "text": {
      "ascii": "jaghAna rAmas tAMz cAnyAn AtmanaH pratirUpakAn\n",
      "ud": "जघान रामस्तांश्चान्यानात्मनः प्रतिरूपकान्\n",
      "ur": "jaghāna rāmas tāṁś cānyān ātmanaḥ pratirūpakān\n"
    },
    "book": 3,
    "chapter": 274,
    "verse": 11
  },
  "03274012a": {
    "text": {
      "ascii": "tato haryazvayuktena rathenAdityavarcasA\n",
      "ud": "ततो हर्यश्वयुक्तेन रथेनादित्यवर्चसा\n",
      "ur": "tato haryaśvayuktena rathenādityavarcasā\n"

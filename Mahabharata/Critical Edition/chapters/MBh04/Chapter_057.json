{
  "04057001": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 1
  },
  "04057001a": {
    "text": {
      "ascii": "atha saMgamya sarve tu kauravANAM mahArathAH\n",
      "ud": "अथ संगम्य सर्वे तु कौरवाणां महारथाः\n",
      "ur": "atha saṁgamya sarve tu kauravāṇāṁ mahārathāḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 1
  },
  "04057001c": {
    "text": {
      "ascii": "arjunaM sahitA yattAH pratyayudhyanta bhArata\n",
      "ud": "अर्जुनं सहिता यत्ताः प्रत्ययुध्यन्त भारत\n",
      "ur": "arjunaṁ sahitā yattāḥ pratyayudhyanta bhārata\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 1
  },
  "04057002a": {
    "text": {
      "ascii": "sa sAyakamayair jAlaiH sarvatas tAn mahArathAn\n",
      "ud": "स सायकमयैर्जालैः सर्वतस्तान्महारथान्\n",
      "ur": "sa sāyakamayair jālaiḥ sarvatas tān mahārathān\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 2
  },
  "04057002c": {
    "text": {
      "ascii": "prAcchAdayad ameyAtmA nIhAra iva parvatAn\n",
      "ud": "प्राच्छादयदमेयात्मा नीहार इव पर्वतान्\n",
      "ur": "prācchādayad ameyātmā nīhāra iva parvatān\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 2
  },
  "04057003a": {
    "text": {
      "ascii": "nadadbhiz ca mahAnAgair heSamANaiz ca vAjibhiH\n",
      "ud": "नदद्भिश्च महानागैर्हेषमाणैश्च वाजिभिः\n",
      "ur": "nadadbhiś ca mahānāgair heṣamāṇaiś ca vājibhiḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 3
  },
  "04057003c": {
    "text": {
      "ascii": "bherIzaGkhaninAdaiz ca sa zabdas tumulo 'bhavat\n",
      "ud": "भेरीशङ्खनिनादैश्च स शब्दस्तुमुलोऽभवत्\n",
      "ur": "bherīśaṅkhaninādaiś ca sa śabdas tumulo ’bhavat\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 3
  },
  "04057004a": {
    "text": {
      "ascii": "narAzvakAyAn nirbhidya lohAni kavacAni ca\n",
      "ud": "नराश्वकायान्निर्भिद्य लोहानि कवचानि च\n",
      "ur": "narāśvakāyān nirbhidya lohāni kavacāni ca\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 4
  },
  "04057004c": {
    "text": {
      "ascii": "pArthasya zarajAlAni viniSpetuH sahasrazaH\n",
      "ud": "पार्थस्य शरजालानि विनिष्पेतुः सहस्रशः\n",
      "ur": "pārthasya śarajālāni viniṣpetuḥ sahasraśaḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 4
  },
  "04057005a": {
    "text": {
      "ascii": "tvaramANaH zarAn asyan pANDavaH sa babhau raNe\n",
      "ud": "त्वरमाणः शरानस्यन्पाण्डवः स बभौ रणे\n",
      "ur": "tvaramāṇaḥ śarān asyan pāṇḍavaḥ sa babhau raṇe\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 5
  },
  "04057005c": {
    "text": {
      "ascii": "madhyaMdinagato 'rciSmAJ zaradIva divAkaraH\n",
      "ud": "मध्यंदिनगतोऽर्चिष्माञ्शरदीव दिवाकरः\n",
      "ur": "madhyaṁdinagato ’rciṣmāñ śaradīva divākaraḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 5
  },
  "04057006a": {
    "text": {
      "ascii": "upaplavanta vitrastA rathebhyo rathinas tadA\n",
      "ud": "उपप्लवन्त वित्रस्ता रथेभ्यो रथिनस्तदा\n",
      "ur": "upaplavanta vitrastā rathebhyo rathinas tadā\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 6
  },
  "04057006c": {
    "text": {
      "ascii": "sAdinaz cAzvapRSThebhyo bhUmau cApi padAtayaH\n",
      "ud": "सादिनश्चाश्वपृष्ठेभ्यो भूमौ चापि पदातयः\n",
      "ur": "sādinaś cāśvapr̥ṣṭhebhyo bhūmau cāpi padātayaḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 6
  },
  "04057007a": {
    "text": {
      "ascii": "zaraiH saMtADyamAnAnAM kavacAnAM mahAtmanAm\n",
      "ud": "शरैः संताड्यमानानां कवचानां महात्मनाम्\n",
      "ur": "śaraiḥ saṁtāḍyamānānāṁ kavacānāṁ mahātmanām\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 7
  },
  "04057007c": {
    "text": {
      "ascii": "tAmrarAjatalohAnAM prAdurAsIn mahAsvanaH\n",
      "ud": "ताम्रराजतलोहानां प्रादुरासीन्महास्वनः\n",
      "ur": "tāmrarājatalohānāṁ prādurāsīn mahāsvanaḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 7
  },
  "04057008a": {
    "text": {
      "ascii": "channam AyodhanaM sarvaM zarIrair gatacetasAm\n",
      "ud": "छन्नमायोधनं सर्वं शरीरैर्गतचेतसाम्\n",
      "ur": "channam āyodhanaṁ sarvaṁ śarīrair gatacetasām\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 8
  },
  "04057008c": {
    "text": {
      "ascii": "gajAzvasAdibhis tatra zitabANAttajIvitaiH\n",
      "ud": "गजाश्वसादिभिस्तत्र शितबाणात्तजीवितैः\n",
      "ur": "gajāśvasādibhis tatra śitabāṇāttajīvitaiḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 8
  },
  "04057009a": {
    "text": {
      "ascii": "rathopasthAbhipatitair AstRtA mAnavair mahI\n",
      "ud": "रथोपस्थाभिपतितैरास्तृता मानवैर्मही\n",
      "ur": "rathopasthābhipatitair āstr̥tā mānavair mahī\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 9
  },
  "04057009c": {
    "text": {
      "ascii": "pranRtyad iva saMgrAme cApahasto dhanaMjayaH\n",
      "ud": "प्रनृत्यदिव संग्रामे चापहस्तो धनंजयः\n",
      "ur": "pranr̥tyad iva saṁgrāme cāpahasto dhanaṁjayaḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 9
  },
  "04057010a": {
    "text": {
      "ascii": "zrutvA gANDIvanirghoSaM visphUrjitam ivAzaneH\n",
      "ud": "श्रुत्वा गाण्डीवनिर्घोषं विस्फूर्जितमिवाशनेः\n",
      "ur": "śrutvā gāṇḍīvanirghoṣaṁ visphūrjitam ivāśaneḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 10
  },
  "04057010c": {
    "text": {
      "ascii": "trastAni sarvabhUtAni vyagacchanta mahAhavAt\n",
      "ud": "त्रस्तानि सर्वभूतानि व्यगच्छन्त महाहवात्\n",
      "ur": "trastāni sarvabhūtāni vyagacchanta mahāhavāt\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 10
  },
  "04057011a": {
    "text": {
      "ascii": "kuNDaloSNISadhArINi jAtarUpasrajAni ca\n",
      "ud": "कुण्डलोष्णीषधारीणि जातरूपस्रजानि च\n",
      "ur": "kuṇḍaloṣṇīṣadhārīṇi jātarūpasrajāni ca\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 11
  },
  "04057011c": {
    "text": {
      "ascii": "patitAni sma dRzyante zirAMsi raNamUrdhani\n",
      "ud": "पतितानि स्म दृश्यन्ते शिरांसि रणमूर्धनि\n",
      "ur": "patitāni sma dr̥śyante śirāṁsi raṇamūrdhani\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 11
  },
  "04057012a": {
    "text": {
      "ascii": "vizikhonmathitair gAtrair bAhubhiz ca sakArmukaiH\n",
      "ud": "विशिखोन्मथितैर्गात्रैर्बाहुभिश्च सकार्मुकैः\n",
      "ur": "viśikhonmathitair gātrair bāhubhiś ca sakārmukaiḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 12
  },
  "04057012c": {
    "text": {
      "ascii": "sahastAbharaNaiz cAnyaiH pracchannA bhAti medinI\n",
      "ud": "सहस्ताभरणैश्चान्यैः प्रच्छन्ना भाति मेदिनी\n",
      "ur": "sahastābharaṇaiś cānyaiḥ pracchannā bhāti medinī\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 12
  },
  "04057013a": {
    "text": {
      "ascii": "zirasAM pAtyamAnAnAm antarA nizitaiH zaraiH\n",
      "ud": "शिरसां पात्यमानानामन्तरा निशितैः शरैः\n",
      "ur": "śirasāṁ pātyamānānām antarā niśitaiḥ śaraiḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 13
  },
  "04057013c": {
    "text": {
      "ascii": "azmavRSTir ivAkAzAd abhavad bharatarSabha\n",
      "ud": "अश्मवृष्टिरिवाकाशादभवद्भरतर्षभ\n",
      "ur": "aśmavr̥ṣṭir ivākāśād abhavad bharatarṣabha\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 13
  },
  "04057014a": {
    "text": {
      "ascii": "darzayitvA tathAtmAnaM raudraM rudraparAkramaH\n",
      "ud": "दर्शयित्वा तथात्मानं रौद्रं रुद्रपराक्रमः\n",
      "ur": "darśayitvā tathātmānaṁ raudraṁ rudraparākramaḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 14
  },
  "04057014c": {
    "text": {
      "ascii": "avaruddhaz caran pArtho dazavarSANi trINi ca\n",
      "ud": "अवरुद्धश्चरन्पार्थो दशवर्षाणि त्रीणि च\n",
      "ur": "avaruddhaś caran pārtho daśavarṣāṇi trīṇi ca\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 14
  },
  "04057014e": {
    "text": {
      "ascii": "krodhAgnim utsRjad ghoraM dhArtarASTreSu pANDavaH\n",
      "ud": "क्रोधाग्निमुत्सृजद्घोरं धार्तराष्ट्रेषु पाण्डवः\n",
      "ur": "krodhāgnim utsr̥jad ghoraṁ dhārtarāṣṭreṣu pāṇḍavaḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 14
  },
  "04057015a": {
    "text": {
      "ascii": "tasya tad dahataH sainyaM dRSTvA caiva parAkramam\n",
      "ud": "तस्य तद्दहतः सैन्यं दृष्ट्वा चैव पराक्रमम्\n",
      "ur": "tasya tad dahataḥ sainyaṁ dr̥ṣṭvā caiva parākramam\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 15
  },
  "04057015c": {
    "text": {
      "ascii": "sarve zAntiparA yodhA dhArtarASTrasya pazyataH\n",
      "ud": "सर्वे शान्तिपरा योधा धार्तराष्ट्रस्य पश्यतः\n",
      "ur": "sarve śāntiparā yodhā dhārtarāṣṭrasya paśyataḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 15
  },
  "04057016a": {
    "text": {
      "ascii": "vitrAsayitvA tat sainyaM drAvayitvA mahArathAn\n",
      "ud": "वित्रासयित्वा तत्सैन्यं द्रावयित्वा महारथान्\n",
      "ur": "vitrāsayitvā tat sainyaṁ drāvayitvā mahārathān\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 16
  },
  "04057016c": {
    "text": {
      "ascii": "arjuno jayatAM zreSThaH paryavartata bhArata\n",
      "ud": "अर्जुनो जयतां श्रेष्ठः पर्यवर्तत भारत\n",
      "ur": "arjuno jayatāṁ śreṣṭhaḥ paryavartata bhārata\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 16
  },
  "04057017a": {
    "text": {
      "ascii": "prAvartayan nadIM ghorAM zoNitaughataraGgiNIm\n",
      "ud": "प्रावर्तयन्नदीं घोरां शोणितौघतरङ्गिणीम्\n",
      "ur": "prāvartayan nadīṁ ghorāṁ śoṇitaughataraṅgiṇīm\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 17
  },
  "04057017c": {
    "text": {
      "ascii": "asthizaivalasaMbAdhAM yugAnte kAlanirmitAm\n",
      "ud": "अस्थिशैवलसंबाधां युगान्ते कालनिर्मिताम्\n",
      "ur": "asthiśaivalasaṁbādhāṁ yugānte kālanirmitām\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 17
  },
  "04057018a": {
    "text": {
      "ascii": "zaracApaplavAM ghorAM mAMsazoNitakardamAm\n",
      "ud": "शरचापप्लवां घोरां मांसशोणितकर्दमाम्\n",
      "ur": "śaracāpaplavāṁ ghorāṁ māṁsaśoṇitakardamām\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 18
  },
  "04057018c": {
    "text": {
      "ascii": "mahArathamahAdvIpAM zaGkhadundubhinisvanAm\n",
      "ud": "महारथमहाद्वीपां शङ्खदुन्दुभिनिस्वनाम्\n",
      "ur": "mahārathamahādvīpāṁ śaṅkhadundubhinisvanām\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 18
  },
  "04057018e": {
    "text": {
      "ascii": "cakAra mahatIM pArtho nadIm uttarazoNitAm\n",
      "ud": "चकार महतीं पार्थो नदीमुत्तरशोणिताम्\n",
      "ur": "cakāra mahatīṁ pārtho nadīm uttaraśoṇitām\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 18
  },
  "04057019a": {
    "text": {
      "ascii": "AdadAnasya hi zarAn saMdhAya ca vimuJcataH\n",
      "ud": "आददानस्य हि शरान्संधाय च विमुञ्चतः\n",
      "ur": "ādadānasya hi śarān saṁdhāya ca vimuñcataḥ\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 19
  },
  "04057019c": {
    "text": {
      "ascii": "vikarSataz ca gANDIvaM na kiM cid dRzyate 'ntaram\n",
      "ud": "विकर्षतश्च गाण्डीवं न किंचिद्दृश्यतेऽन्तरम्\n",
      "ur": "vikarṣataś ca gāṇḍīvaṁ na kiṁ cid dr̥śyate ’ntaram\n"
    },
    "book": 4,
    "chapter": 57,
    "verse": 19
  }
}
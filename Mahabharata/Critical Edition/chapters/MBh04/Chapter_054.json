{
  "04054001": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 1
  },
  "04054001a": {
    "text": {
      "ascii": "taM pArthaH pratijagrAha vAyuvegam ivoddhatam\n",
      "ud": "तं पार्थः प्रतिजग्राह वायुवेगमिवोद्धतम्\n",
      "ur": "taṁ pārthaḥ pratijagrāha vāyuvegam ivoddhatam\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 1
  },
  "04054001c": {
    "text": {
      "ascii": "zarajAlena mahatA varSamANam ivAmbudam\n",
      "ud": "शरजालेन महता वर्षमाणमिवाम्बुदम्\n",
      "ur": "śarajālena mahatā varṣamāṇam ivāmbudam\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 1
  },
  "04054002a": {
    "text": {
      "ascii": "tayor devAsurasamaH saMnipAto mahAn abhUt\n",
      "ud": "तयोर्देवासुरसमः संनिपातो महानभूत्\n",
      "ur": "tayor devāsurasamaḥ saṁnipāto mahān abhūt\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 2
  },
  "04054002c": {
    "text": {
      "ascii": "kiratoH zarajAlAni vRtravAsavayor iva\n",
      "ud": "किरतोः शरजालानि वृत्रवासवयोरिव\n",
      "ur": "kiratoḥ śarajālāni vr̥travāsavayor iva\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 2
  },
  "04054003a": {
    "text": {
      "ascii": "na sma sUryas tadA bhAti na ca vAti samIraNaH\n",
      "ud": "न स्म सूर्यस्तदा भाति न च वाति समीरणः\n",
      "ur": "na sma sūryas tadā bhāti na ca vāti samīraṇaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 3
  },
  "04054003c": {
    "text": {
      "ascii": "zaragADhe kRte vyomni chAyAbhUte samantataH\n",
      "ud": "शरगाढे कृते व्योम्नि छायाभूते समन्ततः\n",
      "ur": "śaragāḍhe kr̥te vyomni chāyābhūte samantataḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 3
  },
  "04054004a": {
    "text": {
      "ascii": "mahAMz caTacaTAzabdo yodhayor hanyamAnayoH\n",
      "ud": "महांश्चटचटाशब्दो योधयोर्हन्यमानयोः\n",
      "ur": "mahāṁś caṭacaṭāśabdo yodhayor hanyamānayoḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 4
  },
  "04054004c": {
    "text": {
      "ascii": "dahyatAm iva veNUnAm AsIt parapuraMjaya\n",
      "ud": "दह्यतामिव वेणूनामासीत्परपुरंजय\n",
      "ur": "dahyatām iva veṇūnām āsīt parapuraṁjaya\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 4
  },
  "04054005a": {
    "text": {
      "ascii": "hayAn asyArjunaH sarvAn kRtavAn alpajIvitAn\n",
      "ud": "हयानस्यार्जुनः सर्वान्कृतवानल्पजीवितान्\n",
      "ur": "hayān asyārjunaḥ sarvān kr̥tavān alpajīvitān\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 5
  },
  "04054005c": {
    "text": {
      "ascii": "sa rAjan na prajAnAti dizaM kAM cana mohitaH\n",
      "ud": "स राजन्न प्रजानाति दिशं कांचन मोहितः\n",
      "ur": "sa rājan na prajānāti diśaṁ kāṁ cana mohitaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 5
  },
  "04054006a": {
    "text": {
      "ascii": "tato drauNir mahAvIryaH pArthasya vicariSyataH\n",
      "ud": "ततो द्रौणिर्महावीर्यः पार्थस्य विचरिष्यतः\n",
      "ur": "tato drauṇir mahāvīryaḥ pārthasya vicariṣyataḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 6
  },
  "04054006c": {
    "text": {
      "ascii": "vivaraM sUkSmam Alokya jyAM ciccheda kSureNa ha\n",
      "ud": "विवरं सूक्ष्ममालोक्य ज्यां चिच्छेद क्षुरेण ह\n",
      "ur": "vivaraṁ sūkṣmam ālokya jyāṁ ciccheda kṣureṇa ha\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 6
  },
  "04054006e": {
    "text": {
      "ascii": "tad asyApUjayan devAH karma dRSTvAtimAnuSam\n",
      "ud": "तदस्यापूजयन्देवाः कर्म दृष्ट्वातिमानुषम्\n",
      "ur": "tad asyāpūjayan devāḥ karma dr̥ṣṭvātimānuṣam\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 6
  },
  "04054007a": {
    "text": {
      "ascii": "tato drauNir dhanUMSy aSTau vyapakramya nararSabham\n",
      "ud": "ततो द्रौणिर्धनूंष्यष्टौ व्यपक्रम्य नरर्षभम्\n",
      "ur": "tato drauṇir dhanūṁṣy aṣṭau vyapakramya nararṣabham\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 7
  },
  "04054007c": {
    "text": {
      "ascii": "punar abhyAhanat pArthaM hRdaye kaGkapatribhiH\n",
      "ud": "पुनरभ्याहनत्पार्थं हृदये कङ्कपत्रिभिः\n",
      "ur": "punar abhyāhanat pārthaṁ hr̥daye kaṅkapatribhiḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 7
  },
  "04054008a": {
    "text": {
      "ascii": "tataH pArtho mahAbAhuH prahasya svanavat tadA\n",
      "ud": "ततः पार्थो महाबाहुः प्रहस्य स्वनवत्तदा\n",
      "ur": "tataḥ pārtho mahābāhuḥ prahasya svanavat tadā\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 8
  },
  "04054008c": {
    "text": {
      "ascii": "yojayAm Asa navayA maurvyA gANDIvam ojasA\n",
      "ud": "योजयामास नवया मौर्व्या गाण्डीवमोजसा\n",
      "ur": "yojayām āsa navayā maurvyā gāṇḍīvam ojasā\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 8
  },
  "04054009a": {
    "text": {
      "ascii": "tato 'rdhacandram AvRtya tena pArthaH samAgamat\n",
      "ud": "ततोऽर्धचन्द्रमावृत्य तेन पार्थः समागमत्\n",
      "ur": "tato ’rdhacandram āvr̥tya tena pārthaḥ samāgamat\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 9
  },
  "04054009c": {
    "text": {
      "ascii": "vAraNeneva mattena matto vAraNayUthapaH\n",
      "ud": "वारणेनेव मत्तेन मत्तो वारणयूथपः\n",
      "ur": "vāraṇeneva mattena matto vāraṇayūthapaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 9
  },
  "04054010a": {
    "text": {
      "ascii": "tataH pravavRte yuddhaM pRthivyAm ekavIrayoH\n",
      "ud": "ततः प्रववृते युद्धं पृथिव्यामेकवीरयोः\n",
      "ur": "tataḥ pravavr̥te yuddhaṁ pr̥thivyām ekavīrayoḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 10
  },
  "04054010c": {
    "text": {
      "ascii": "raNamadhye dvayor eva sumahal lomaharSaNam\n",
      "ud": "रणमध्ये द्वयोरेव सुमहल्लोमहर्षणम्\n",
      "ur": "raṇamadhye dvayor eva sumahal lomaharṣaṇam\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 10
  },
  "04054011a": {
    "text": {
      "ascii": "tau vIrau kuravaH sarve dadRzur vismayAnvitAH\n",
      "ud": "तौ वीरौ कुरवः सर्वे ददृशुर्विस्मयान्विताः\n",
      "ur": "tau vīrau kuravaḥ sarve dadr̥śur vismayānvitāḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 11
  },
  "04054011c": {
    "text": {
      "ascii": "yudhyamAnau mahAtmAnau yUthapAv iva saMgatau\n",
      "ud": "युध्यमानौ महात्मानौ यूथपाविव संगतौ\n",
      "ur": "yudhyamānau mahātmānau yūthapāv iva saṁgatau\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 11
  },
  "04054012a": {
    "text": {
      "ascii": "tau samAjaghnatur vIrAv anyonyaM puruSarSabhau\n",
      "ud": "तौ समाजघ्नतुर्वीरावन्योन्यं पुरुषर्षभौ\n",
      "ur": "tau samājaghnatur vīrāv anyonyaṁ puruṣarṣabhau\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 12
  },
  "04054012c": {
    "text": {
      "ascii": "zarair AzIviSAkArair jvaladbhir iva pannagaiH\n",
      "ud": "शरैराशीविषाकारैर्ज्वलद्भिरिव पन्नगैः\n",
      "ur": "śarair āśīviṣākārair jvaladbhir iva pannagaiḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 12
  },
  "04054013a": {
    "text": {
      "ascii": "akSayyAv iSudhI divyau pANDavasya mahAtmanaH\n",
      "ud": "अक्षय्याविषुधी दिव्यौ पाण्डवस्य महात्मनः\n",
      "ur": "akṣayyāv iṣudhī divyau pāṇḍavasya mahātmanaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 13
  },
  "04054013c": {
    "text": {
      "ascii": "tena pArtho raNe zUras tasthau girir ivAcalaH\n",
      "ud": "तेन पार्थो रणे शूरस्तस्थौ गिरिरिवाचलः\n",
      "ur": "tena pārtho raṇe śūras tasthau girir ivācalaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 13
  },
  "04054014a": {
    "text": {
      "ascii": "azvatthAmnaH punar bANAH kSipram abhyasyato raNe\n",
      "ud": "अश्वत्थाम्नः पुनर्बाणाः क्षिप्रमभ्यस्यतो रणे\n",
      "ur": "aśvatthāmnaḥ punar bāṇāḥ kṣipram abhyasyato raṇe\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 14
  },
  "04054014c": {
    "text": {
      "ascii": "jagmuH parikSayaM zIghram abhUt tenAdhiko 'rjunaH\n",
      "ud": "जग्मुः परिक्षयं शीघ्रमभूत्तेनाधिकोऽर्जुनः\n",
      "ur": "jagmuḥ parikṣayaṁ śīghram abhūt tenādhiko ’rjunaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 14
  },
  "04054015a": {
    "text": {
      "ascii": "tataH karNo mahac cApaM vikRSyAbhyadhikaM ruSA\n",
      "ud": "ततः कर्णो महच्चापं विकृष्याभ्यधिकं रुषा\n",
      "ur": "tataḥ karṇo mahac cāpaṁ vikr̥ṣyābhyadhikaṁ ruṣā\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 15
  },
  "04054015c": {
    "text": {
      "ascii": "avAkSipat tataH zabdo hAhAkAro mahAn abhUt\n",
      "ud": "अवाक्षिपत्ततः शब्दो हाहाकारो महानभूत्\n",
      "ur": "avākṣipat tataḥ śabdo hāhākāro mahān abhūt\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 15
  },
  "04054016a": {
    "text": {
      "ascii": "tatra cakSur dadhe pArtho yatra visphAryate dhanuH\n",
      "ud": "तत्र चक्षुर्दधे पार्थो यत्र विस्फार्यते धनुः\n",
      "ur": "tatra cakṣur dadhe pārtho yatra visphāryate dhanuḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 16
  },
  "04054016c": {
    "text": {
      "ascii": "dadarza tatra rAdheyaM tasya kopo 'tyavIvRdhat\n",
      "ud": "ददर्श तत्र राधेयं तस्य कोपोऽत्यवीवृधत्\n",
      "ur": "dadarśa tatra rādheyaṁ tasya kopo ’tyavīvr̥dhat\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 16
  },
  "04054017a": {
    "text": {
      "ascii": "sa roSavazam ApannaH karNam eva jighAMsayA\n",
      "ud": "स रोषवशमापन्नः कर्णमेव जिघांसया\n",
      "ur": "sa roṣavaśam āpannaḥ karṇam eva jighāṁsayā\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 17
  },
  "04054017c": {
    "text": {
      "ascii": "avaikSata vivRttAbhyAM netrAbhyAM kurupuMgavaH\n",
      "ud": "अवैक्षत विवृत्ताभ्यां नेत्राभ्यां कुरुपुंगवः\n",
      "ur": "avaikṣata vivr̥ttābhyāṁ netrābhyāṁ kurupuṁgavaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 17
  },
  "04054018a": {
    "text": {
      "ascii": "tathA tu vimukhe pArthe droNaputrasya sAyakAn\n",
      "ud": "तथा तु विमुखे पार्थे द्रोणपुत्रस्य सायकान्\n",
      "ur": "tathā tu vimukhe pārthe droṇaputrasya sāyakān\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 18
  },
  "04054018c": {
    "text": {
      "ascii": "tvaritAH puruSA rAjann upAjahruH sahasrazaH\n",
      "ud": "त्वरिताः पुरुषा राजन्नुपाजह्रुः सहस्रशः\n",
      "ur": "tvaritāḥ puruṣā rājann upājahruḥ sahasraśaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 18
  },
  "04054019a": {
    "text": {
      "ascii": "utsRjya ca mahAbAhur droNaputraM dhanaMjayaH\n",
      "ud": "उत्सृज्य च महाबाहुर्द्रोणपुत्रं धनंजयः\n",
      "ur": "utsr̥jya ca mahābāhur droṇaputraṁ dhanaṁjayaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 19
  },
  "04054019c": {
    "text": {
      "ascii": "abhidudrAva sahasA karNam eva sapatnajit\n",
      "ud": "अभिदुद्राव सहसा कर्णमेव सपत्नजित्\n",
      "ur": "abhidudrāva sahasā karṇam eva sapatnajit\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 19
  },
  "04054020a": {
    "text": {
      "ascii": "tam abhidrutya kaunteyaH krodhasaMraktalocanaH\n",
      "ud": "तमभिद्रुत्य कौन्तेयः क्रोधसंरक्तलोचनः\n",
      "ur": "tam abhidrutya kaunteyaḥ krodhasaṁraktalocanaḥ\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 20
  },
  "04054020c": {
    "text": {
      "ascii": "kAmayan dvairathe yuddham idaM vacanam abravIt\n",
      "ud": "कामयन्द्वैरथे युद्धमिदं वचनमब्रवीत्\n",
      "ur": "kāmayan dvairathe yuddham idaṁ vacanam abravīt\n"
    },
    "book": 4,
    "chapter": 54,
    "verse": 20
  }
}
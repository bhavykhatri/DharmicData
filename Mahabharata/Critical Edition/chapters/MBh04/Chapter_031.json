    "book": 4,
    "chapter": 31,
    "verse": 18
  },
  "04031019a": {
    "text": {
      "ascii": "tau vyAvaharatAM tatra mahAtmAnau mahAbalau\n",
      "ud": "तौ व्यावहरतां तत्र महात्मानौ महाबलौ\n",
      "ur": "tau vyāvaharatāṁ tatra mahātmānau mahābalau\n"
    },
    "book": 4,
    "chapter": 31,
    "verse": 19
  },
  "04031019c": {
    "text": {
      "ascii": "anyonyam abhigarjantau goSThe govRSabhAv iva\n",
      "ud": "अन्योन्यमभिगर्जन्तौ गोष्ठे गोवृषभाविव\n",
      "ur": "anyonyam abhigarjantau goṣṭhe govr̥ṣabhāv iva\n"
    },
    "book": 4,
    "chapter": 31,
    "verse": 19
  },
  "04031020a": {
    "text": {
      "ascii": "tato rathAbhyAM rathinau vyatiyAya samantataH\n",
      "ud": "ततो रथाभ्यां रथिनौ व्यतियाय समन्ततः\n",

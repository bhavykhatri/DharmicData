    "book": 1,
    "chapter": 8,
    "verse": 13
  },
  "01008014a": {
    "text": {
      "ascii": "tataH katipayAhasya vivAhe samupasthite\n",
      "ud": "ततः कतिपयाहस्य विवाहे समुपस्थिते\n",
      "ur": "tataḥ katipayāhasya vivāhe samupasthite\n"

{
  "01022001": {
    "text": {
      "ascii": "sUta uvAca\n",
      "ud": "सूत उवाच\n",
      "ur": "sūta uvāca\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 1
  },
  "01022001a": {
    "text": {
      "ascii": "evaM stutas tadA kadrvA bhagavAn harivAhanaH\n",
      "ud": "एवं स्तुतस्तदा कद्र्वा भगवान्हरिवाहनः\n",
      "ur": "evaṁ stutas tadā kadrvā bhagavān harivāhanaḥ\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 1
  },
  "01022001c": {
    "text": {
      "ascii": "nIlajImUtasaMghAtair vyoma sarvaM samAvRNot\n",
      "ud": "नीलजीमूतसंघातैर्व्योम सर्वं समावृणोत्\n",
      "ur": "nīlajīmūtasaṁghātair vyoma sarvaṁ samāvr̥ṇot\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 1
  },
  "01022002a": {
    "text": {
      "ascii": "te meghA mumucus toyaM prabhUtaM vidyudujjvalAH\n",
      "ud": "ते मेघा मुमुचुस्तोयं प्रभूतं विद्युदुज्ज्वलाः\n",
      "ur": "te meghā mumucus toyaṁ prabhūtaṁ vidyudujjvalāḥ\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 2
  },
  "01022002c": {
    "text": {
      "ascii": "parasparam ivAtyarthaM garjantaH satataM divi\n",
      "ud": "परस्परमिवात्यर्थं गर्जन्तः सततं दिवि\n",
      "ur": "parasparam ivātyarthaṁ garjantaḥ satataṁ divi\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 2
  },
  "01022003a": {
    "text": {
      "ascii": "saMghAtitam ivAkAzaM jaladaiH sumahAdbhutaiH\n",
      "ud": "संघातितमिवाकाशं जलदैः सुमहाद्भुतैः\n",
      "ur": "saṁghātitam ivākāśaṁ jaladaiḥ sumahādbhutaiḥ\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 3
  },
  "01022003c": {
    "text": {
      "ascii": "sRjadbhir atulaM toyam ajasraM sumahAravaiH\n",
      "ud": "सृजद्भिरतुलं तोयमजस्रं सुमहारवैः\n",
      "ur": "sr̥jadbhir atulaṁ toyam ajasraṁ sumahāravaiḥ\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 3
  },
  "01022004a": {
    "text": {
      "ascii": "saMpranRttam ivAkAzaM dhArormibhir anekazaH\n",
      "ud": "संप्रनृत्तमिवाकाशं धारोर्मिभिरनेकशः\n",
      "ur": "saṁpranr̥ttam ivākāśaṁ dhārormibhir anekaśaḥ\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 4
  },
  "01022004c": {
    "text": {
      "ascii": "meghastanitanirghoSam ambaraM samapadyata\n",
      "ud": "मेघस्तनितनिर्घोषमम्बरं समपद्यत\n",
      "ur": "meghastanitanirghoṣam ambaraṁ samapadyata\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 4
  },
  "01022005a": {
    "text": {
      "ascii": "nAgAnAm uttamo harSas tadA varSati vAsave\n",
      "ud": "नागानामुत्तमो हर्षस्तदा वर्षति वासवे\n",
      "ur": "nāgānām uttamo harṣas tadā varṣati vāsave\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 5
  },
  "01022005c": {
    "text": {
      "ascii": "ApUryata mahI cApi salilena samantataH\n",
      "ud": "आपूर्यत मही चापि सलिलेन समन्ततः\n",
      "ur": "āpūryata mahī cāpi salilena samantataḥ\n"
    },
    "book": 1,
    "chapter": 22,
    "verse": 5
  }
}
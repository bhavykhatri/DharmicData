{
  "01089001": {
    "text": {
      "ascii": "janamejaya uvAca\n",
      "ud": "जनमेजय उवाच\n",
      "ur": "janamejaya uvāca\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 1
  },
  "01089001a": {
    "text": {
      "ascii": "bhagavaJ zrotum icchAmi pUror vaMzakarAn nRpAn\n",
      "ud": "भगवञ्श्रोतुमिच्छामि पूरोर्वंशकरान्नृपान्\n",
      "ur": "bhagavañ śrotum icchāmi pūror vaṁśakarān nr̥pān\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 1
  },
  "01089001c": {
    "text": {
      "ascii": "yadvIryA yAdRzAz caiva yAvanto yatparAkramAH\n",
      "ud": "यद्वीर्या यादृशाश्चैव यावन्तो यत्पराक्रमाः\n",
      "ur": "yadvīryā yādr̥śāś caiva yāvanto yatparākramāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 1
  },
  "01089002a": {
    "text": {
      "ascii": "na hy asmiJ zIlahIno vA nirvIryo vA narAdhipaH\n",
      "ud": "न ह्यस्मिञ्शीलहीनो वा निर्वीर्यो वा नराधिपः\n",
      "ur": "na hy asmiñ śīlahīno vā nirvīryo vā narādhipaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 2
  },
  "01089002c": {
    "text": {
      "ascii": "prajAvirahito vApi bhUtapUrvaH kadA cana\n",
      "ud": "प्रजाविरहितो वापि भूतपूर्वः कदाचन\n",
      "ur": "prajāvirahito vāpi bhūtapūrvaḥ kadā cana\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 2
  },
  "01089003a": {
    "text": {
      "ascii": "teSAM prathitavRttAnAM rAjJAM vijJAnazAlinAm\n",
      "ud": "तेषां प्रथितवृत्तानां राज्ञां विज्ञानशालिनाम्\n",
      "ur": "teṣāṁ prathitavr̥ttānāṁ rājñāṁ vijñānaśālinām\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 3
  },
  "01089003c": {
    "text": {
      "ascii": "caritaM zrotum icchAmi vistareNa tapodhana\n",
      "ud": "चरितं श्रोतुमिच्छामि विस्तरेण तपोधन\n",
      "ur": "caritaṁ śrotum icchāmi vistareṇa tapodhana\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 3
  },
  "01089004": {
    "text": {
      "ascii": "vaizaMpAyana uvAca\n",
      "ud": "वैशंपायन उवाच\n",
      "ur": "vaiśaṁpāyana uvāca\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 4
  },
  "01089004a": {
    "text": {
      "ascii": "hanta te kathayiSyAmi yan mAM tvaM paripRcchasi\n",
      "ud": "हन्त ते कथयिष्यामि यन्मां त्वं परिपृच्छसि\n",
      "ur": "hanta te kathayiṣyāmi yan māṁ tvaṁ paripr̥cchasi\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 4
  },
  "01089004c": {
    "text": {
      "ascii": "pUror vaMzadharAn vIrAJ zakrapratimatejasaH\n",
      "ud": "पूरोर्वंशधरान्वीराञ्शक्रप्रतिमतेजसः\n",
      "ur": "pūror vaṁśadharān vīrāñ śakrapratimatejasaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 4
  },
  "01089005a": {
    "text": {
      "ascii": "pravIrezvararaudrAzvAs trayaH putrA mahArathAH\n",
      "ud": "प्रवीरेश्वररौद्राश्वास्त्रयः पुत्रा महारथाः\n",
      "ur": "pravīreśvararaudrāśvās trayaḥ putrā mahārathāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 5
  },
  "01089005c": {
    "text": {
      "ascii": "pUroH pauSTyAm ajAyanta pravIras tatra vaMzakRt\n",
      "ud": "पूरोः पौष्ट्यामजायन्त प्रवीरस्तत्र वंशकृत्\n",
      "ur": "pūroḥ pauṣṭyām ajāyanta pravīras tatra vaṁśakr̥t\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 5
  },
  "01089006a": {
    "text": {
      "ascii": "manasyur abhavat tasmAc chUraH zyenIsutaH prabhuH\n",
      "ud": "मनस्युरभवत्तस्माच्छूरः श्येनीसुतः प्रभुः\n",
      "ur": "manasyur abhavat tasmāc chūraḥ śyenīsutaḥ prabhuḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 6
  },
  "01089006c": {
    "text": {
      "ascii": "pRthivyAz caturantAyA goptA rAjIvalocanaH\n",
      "ud": "पृथिव्याश्चतुरन्ताया गोप्ता राजीवलोचनः\n",
      "ur": "pr̥thivyāś caturantāyā goptā rājīvalocanaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 6
  },
  "01089007a": {
    "text": {
      "ascii": "subhrUH saMhanano vAgmI sauvIrItanayAs trayaH\n",
      "ud": "सुभ्रूः संहननो वाग्मी सौवीरीतनयास्त्रयः\n",
      "ur": "subhrūḥ saṁhanano vāgmī sauvīrītanayās trayaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 7
  },
  "01089007c": {
    "text": {
      "ascii": "manasyor abhavan putrAH zUrAH sarve mahArathAH\n",
      "ud": "मनस्योरभवन्पुत्राः शूराः सर्वे महारथाः\n",
      "ur": "manasyor abhavan putrāḥ śūrāḥ sarve mahārathāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 7
  },
  "01089008a": {
    "text": {
      "ascii": "raudrAzvasya maheSvAsA dazApsarasi sUnavaH\n",
      "ud": "रौद्राश्वस्य महेष्वासा दशाप्सरसि सूनवः\n",
      "ur": "raudrāśvasya maheṣvāsā daśāpsarasi sūnavaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 8
  },
  "01089008c": {
    "text": {
      "ascii": "yajvAno jajJire zUrAH prajAvanto bahuzrutAH\n",
      "ud": "यज्वानो जज्ञिरे शूराः प्रजावन्तो बहुश्रुताः\n",
      "ur": "yajvāno jajñire śūrāḥ prajāvanto bahuśrutāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 8
  },
  "01089008e": {
    "text": {
      "ascii": "sarve sarvAstravidvAMsaH sarve dharmaparAyaNAH\n",
      "ud": "सर्वे सर्वास्त्रविद्वांसः सर्वे धर्मपरायणाः\n",
      "ur": "sarve sarvāstravidvāṁsaḥ sarve dharmaparāyaṇāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 8
  },
  "01089009a": {
    "text": {
      "ascii": "Rcepur atha kakSepuH kRkaNepuz ca vIryavAn\n",
      "ud": "ऋचेपुरथ कक्षेपुः कृकणेपुश्च वीर्यवान्\n",
      "ur": "r̥cepur atha kakṣepuḥ kr̥kaṇepuś ca vīryavān\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 9
  },
  "01089009c": {
    "text": {
      "ascii": "sthaNDilepur vanepuz ca sthalepuz ca mahArathaH\n",
      "ud": "स्थण्डिलेपुर्वनेपुश्च स्थलेपुश्च महारथः\n",
      "ur": "sthaṇḍilepur vanepuś ca sthalepuś ca mahārathaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 9
  },
  "01089010a": {
    "text": {
      "ascii": "tejepur balavAn dhImAn satyepuz cendravikramaH\n",
      "ud": "तेजेपुर्बलवान्धीमान्सत्येपुश्चेन्द्रविक्रमः\n",
      "ur": "tejepur balavān dhīmān satyepuś cendravikramaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 10
  },
  "01089010c": {
    "text": {
      "ascii": "dharmepuH saMnatepuz ca dazamo devavikramaH\n",
      "ud": "धर्मेपुः संनतेपुश्च दशमो देवविक्रमः\n",
      "ur": "dharmepuḥ saṁnatepuś ca daśamo devavikramaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 10
  },
  "01089010e": {
    "text": {
      "ascii": "anAdhRSTisutAs tAta rAjasUyAzvamedhinaH\n",
      "ud": "अनाधृष्टिसुतास्तात राजसूयाश्वमेधिनः\n",
      "ur": "anādhr̥ṣṭisutās tāta rājasūyāśvamedhinaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 10
  },
  "01089011a": {
    "text": {
      "ascii": "matinAras tato rAjA vidvAMz carceputo 'bhavat\n",
      "ud": "मतिनारस्ततो राजा विद्वांश्चर्चेपुतोऽभवत्\n",
      "ur": "matināras tato rājā vidvāṁś carceputo ’bhavat\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 11
  },
  "01089011c": {
    "text": {
      "ascii": "matinArasutA rAjaMz catvAro 'mitavikramAH\n",
      "ud": "मतिनारसुता राजंश्चत्वारोऽमितविक्रमाः\n",
      "ur": "matinārasutā rājaṁś catvāro ’mitavikramāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 11
  },
  "01089011e": {
    "text": {
      "ascii": "taMsur mahAn atiratho druhyuz cApratimadyutiH\n",
      "ud": "तंसुर्महानतिरथो द्रुह्युश्चाप्रतिमद्युतिः\n",
      "ur": "taṁsur mahān atiratho druhyuś cāpratimadyutiḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 11
  },
  "01089012a": {
    "text": {
      "ascii": "teSAM taMsur mahAvIryaH pauravaM vaMzam udvahan\n",
      "ud": "तेषां तंसुर्महावीर्यः पौरवं वंशमुद्वहन्\n",
      "ur": "teṣāṁ taṁsur mahāvīryaḥ pauravaṁ vaṁśam udvahan\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 12
  },
  "01089012c": {
    "text": {
      "ascii": "AjahAra yazo dIptaM jigAya ca vasuMdharAm\n",
      "ud": "आजहार यशो दीप्तं जिगाय च वसुंधराम्\n",
      "ur": "ājahāra yaśo dīptaṁ jigāya ca vasuṁdharām\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 12
  },
  "01089013a": {
    "text": {
      "ascii": "ilinaM tu sutaM taMsur janayAm Asa vIryavAn\n",
      "ud": "इलिनं तु सुतं तंसुर्जनयामास वीर्यवान्\n",
      "ur": "ilinaṁ tu sutaṁ taṁsur janayām āsa vīryavān\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 13
  },
  "01089013c": {
    "text": {
      "ascii": "so 'pi kRtsnAm imAM bhUmiM vijigye jayatAM varaH\n",
      "ud": "सोऽपि कृत्स्नामिमां भूमिं विजिग्ये जयतां वरः\n",
      "ur": "so ’pi kr̥tsnām imāṁ bhūmiṁ vijigye jayatāṁ varaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 13
  },
  "01089014a": {
    "text": {
      "ascii": "rathaMtaryAM sutAn paJca paJcabhUtopamAMs tataH\n",
      "ud": "रथंतर्यां सुतान्पञ्च पञ्चभूतोपमांस्ततः\n",
      "ur": "rathaṁtaryāṁ sutān pañca pañcabhūtopamāṁs tataḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 14
  },
  "01089014c": {
    "text": {
      "ascii": "ilino janayAm Asa duHSantaprabhRtIn nRpa\n",
      "ud": "इलिनो जनयामास दुःषन्तप्रभृतीन्नृप\n",
      "ur": "ilino janayām āsa duḥṣantaprabhr̥tīn nr̥pa\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 14
  },
  "01089015a": {
    "text": {
      "ascii": "duHSantaM zUrabhImau ca prapUrvaM vasum eva ca\n",
      "ud": "दुःषन्तं शूरभीमौ च प्रपूर्वं वसुमेव च\n",
      "ur": "duḥṣantaṁ śūrabhīmau ca prapūrvaṁ vasum eva ca\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 15
  },
  "01089015c": {
    "text": {
      "ascii": "teSAM jyeSTho 'bhavad rAjA duHSanto janamejaya\n",
      "ud": "तेषां ज्येष्ठोऽभवद्राजा दुःषन्तो जनमेजय\n",
      "ur": "teṣāṁ jyeṣṭho ’bhavad rājā duḥṣanto janamejaya\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 15
  },
  "01089016a": {
    "text": {
      "ascii": "duHSantAd bharato jajJe vidvAJ zAkuntalo nRpaH\n",
      "ud": "दुःषन्ताद्भरतो जज्ञे विद्वाञ्शाकुन्तलो नृपः\n",
      "ur": "duḥṣantād bharato jajñe vidvāñ śākuntalo nr̥paḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 16
  },
  "01089016c": {
    "text": {
      "ascii": "tasmAd bharatavaMzasya vipratasthe mahad yazaH\n",
      "ud": "तस्माद्भरतवंशस्य विप्रतस्थे महद्यशः\n",
      "ur": "tasmād bharatavaṁśasya vipratasthe mahad yaśaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 16
  },
  "01089017a": {
    "text": {
      "ascii": "bharatas tisRSu strISu nava putrAn ajIjanat\n",
      "ud": "भरतस्तिसृषु स्त्रीषु नव पुत्रानजीजनत्\n",
      "ur": "bharatas tisr̥ṣu strīṣu nava putrān ajījanat\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 17
  },
  "01089017c": {
    "text": {
      "ascii": "nAbhyanandanta tAn rAjA nAnurUpA mamety uta\n",
      "ud": "नाभ्यनन्दन्त तान्राजा नानुरूपा ममेत्युत\n",
      "ur": "nābhyanandanta tān rājā nānurūpā mamety uta\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 17
  },
  "01089018a": {
    "text": {
      "ascii": "tato mahadbhiH kratubhir IjAno bharatas tadA\n",
      "ud": "ततो महद्भिः क्रतुभिरीजानो भरतस्तदा\n",
      "ur": "tato mahadbhiḥ kratubhir ījāno bharatas tadā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 18
  },
  "01089018c": {
    "text": {
      "ascii": "lebhe putraM bharadvAjAd bhumanyuM nAma bhArata\n",
      "ud": "लेभे पुत्रं भरद्वाजाद्भुमन्युं नाम भारत\n",
      "ur": "lebhe putraṁ bharadvājād bhumanyuṁ nāma bhārata\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 18
  },
  "01089019a": {
    "text": {
      "ascii": "tataH putriNam AtmAnaM jJAtvA pauravanandanaH\n",
      "ud": "ततः पुत्रिणमात्मानं ज्ञात्वा पौरवनन्दनः\n",
      "ur": "tataḥ putriṇam ātmānaṁ jñātvā pauravanandanaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 19
  },
  "01089019c": {
    "text": {
      "ascii": "bhumanyuM bharatazreSTha yauvarAjye 'bhyaSecayat\n",
      "ud": "भुमन्युं भरतश्रेष्ठ यौवराज्येऽभ्यषेचयत्\n",
      "ur": "bhumanyuṁ bharataśreṣṭha yauvarājye ’bhyaṣecayat\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 19
  },
  "01089020a": {
    "text": {
      "ascii": "tatas tasya mahIndrasya vitathaH putrako 'bhavat\n",
      "ud": "ततस्तस्य महीन्द्रस्य वितथः पुत्रकोऽभवत्\n",
      "ur": "tatas tasya mahīndrasya vitathaḥ putrako ’bhavat\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 20
  },
  "01089020c": {
    "text": {
      "ascii": "tataH sa vitatho nAma bhumanyor abhavat sutaH\n",
      "ud": "ततः स वितथो नाम भुमन्योरभवत्सुतः\n",
      "ur": "tataḥ sa vitatho nāma bhumanyor abhavat sutaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 20
  },
  "01089021a": {
    "text": {
      "ascii": "suhotraz ca suhotA ca suhaviH suyajus tathA\n",
      "ud": "सुहोत्रश्च सुहोता च सुहविः सुयजुस्तथा\n",
      "ur": "suhotraś ca suhotā ca suhaviḥ suyajus tathā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 21
  },
  "01089021c": {
    "text": {
      "ascii": "puSkariNyAm RcIkasya bhumanyor abhavan sutAH\n",
      "ud": "पुष्करिण्यामृचीकस्य भुमन्योरभवन्सुताः\n",
      "ur": "puṣkariṇyām r̥cīkasya bhumanyor abhavan sutāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 21
  },
  "01089022a": {
    "text": {
      "ascii": "teSAM jyeSThaH suhotras tu rAjyam Apa mahIkSitAm\n",
      "ud": "तेषां ज्येष्ठः सुहोत्रस्तु राज्यमाप महीक्षिताम्\n",
      "ur": "teṣāṁ jyeṣṭhaḥ suhotras tu rājyam āpa mahīkṣitām\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 22
  },
  "01089022c": {
    "text": {
      "ascii": "rAjasUyAzvamedhAdyaiH so 'yajad bahubhiH savaiH\n",
      "ud": "राजसूयाश्वमेधाद्यैः सोऽयजद्बहुभिः सवैः\n",
      "ur": "rājasūyāśvamedhādyaiḥ so ’yajad bahubhiḥ savaiḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 22
  },
  "01089023a": {
    "text": {
      "ascii": "suhotraH pRthivIM sarvAM bubhuje sAgarAmbarAm\n",
      "ud": "सुहोत्रः पृथिवीं सर्वां बुभुजे सागराम्बराम्\n",
      "ur": "suhotraḥ pr̥thivīṁ sarvāṁ bubhuje sāgarāmbarām\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 23
  },
  "01089023c": {
    "text": {
      "ascii": "pUrNAM hastigavAzvasya bahuratnasamAkulAm\n",
      "ud": "पूर्णां हस्तिगवाश्वस्य बहुरत्नसमाकुलाम्\n",
      "ur": "pūrṇāṁ hastigavāśvasya bahuratnasamākulām\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 23
  },
  "01089024a": {
    "text": {
      "ascii": "mamajjeva mahI tasya bhUribhArAvapIDitA\n",
      "ud": "ममज्जेव मही तस्य भूरिभारावपीडिता\n",
      "ur": "mamajjeva mahī tasya bhūribhārāvapīḍitā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 24
  },
  "01089024c": {
    "text": {
      "ascii": "hastyazvarathasaMpUrNA manuSyakalilA bhRzam\n",
      "ud": "हस्त्यश्वरथसंपूर्णा मनुष्यकलिला भृशम्\n",
      "ur": "hastyaśvarathasaṁpūrṇā manuṣyakalilā bhr̥śam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 24
  },
  "01089025a": {
    "text": {
      "ascii": "suhotre rAjani tadA dharmataH zAsati prajAH\n",
      "ud": "सुहोत्रे राजनि तदा धर्मतः शासति प्रजाः\n",
      "ur": "suhotre rājani tadā dharmataḥ śāsati prajāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 25
  },
  "01089025c": {
    "text": {
      "ascii": "caityayUpAGkitA cAsId bhUmiH zatasahasrazaH\n",
      "ud": "चैत्ययूपाङ्किता चासीद्भूमिः शतसहस्रशः\n",
      "ur": "caityayūpāṅkitā cāsīd bhūmiḥ śatasahasraśaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 25
  },
  "01089025e": {
    "text": {
      "ascii": "pravRddhajanasasyA ca sahadevA vyarocata\n",
      "ud": "प्रवृद्धजनसस्या च सहदेवा व्यरोचत\n",
      "ur": "pravr̥ddhajanasasyā ca sahadevā vyarocata\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 25
  },
  "01089026a": {
    "text": {
      "ascii": "aikSvAkI janayAm Asa suhotrAt pRthivIpateH\n",
      "ud": "ऐक्ष्वाकी जनयामास सुहोत्रात्पृथिवीपतेः\n",
      "ur": "aikṣvākī janayām āsa suhotrāt pr̥thivīpateḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 26
  },
  "01089026c": {
    "text": {
      "ascii": "ajamIDhaM sumIDhaM ca purumIDhaM ca bhArata\n",
      "ud": "अजमीढं सुमीढं च पुरुमीढं च भारत\n",
      "ur": "ajamīḍhaṁ sumīḍhaṁ ca purumīḍhaṁ ca bhārata\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 26
  },
  "01089027a": {
    "text": {
      "ascii": "ajamIDho varas teSAM tasmin vaMzaH pratiSThitaH\n",
      "ud": "अजमीढो वरस्तेषां तस्मिन्वंशः प्रतिष्ठितः\n",
      "ur": "ajamīḍho varas teṣāṁ tasmin vaṁśaḥ pratiṣṭhitaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 27
  },
  "01089027c": {
    "text": {
      "ascii": "SaT putrAn so 'py ajanayat tisRSu strISu bhArata\n",
      "ud": "षट्पुत्रान्सोऽप्यजनयत्तिसृषु स्त्रीषु भारत\n",
      "ur": "ṣaṭ putrān so ’py ajanayat tisr̥ṣu strīṣu bhārata\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 27
  },
  "01089028a": {
    "text": {
      "ascii": "RkSaM dhUminy atho nIlI duHSantaparameSThinau\n",
      "ud": "ऋक्षं धूमिन्यथो नीली दुःषन्तपरमेष्ठिनौ\n",
      "ur": "r̥kṣaṁ dhūminy atho nīlī duḥṣantaparameṣṭhinau\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 28
  },
  "01089028c": {
    "text": {
      "ascii": "keziny ajanayaj jahnum ubhau ca janarUpiNau\n",
      "ud": "केशिन्यजनयज्जह्नुमुभौ च जनरूपिणौ\n",
      "ur": "keśiny ajanayaj jahnum ubhau ca janarūpiṇau\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 28
  },
  "01089029a": {
    "text": {
      "ascii": "tatheme sarvapAJcAlA duHSantaparameSThinoH\n",
      "ud": "तथेमे सर्वपाञ्चाला दुःषन्तपरमेष्ठिनोः\n",
      "ur": "tatheme sarvapāñcālā duḥṣantaparameṣṭhinoḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 29
  },
  "01089029c": {
    "text": {
      "ascii": "anvayAH kuzikA rAjaJ jahnor amitatejasaH\n",
      "ud": "अन्वयाः कुशिका राजञ्जह्नोरमिततेजसः\n",
      "ur": "anvayāḥ kuśikā rājañ jahnor amitatejasaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 29
  },
  "01089030a": {
    "text": {
      "ascii": "janarUpiNayor jyeSTham RkSam Ahur janAdhipam\n",
      "ud": "जनरूपिणयोर्ज्येष्ठमृक्षमाहुर्जनाधिपम्\n",
      "ur": "janarūpiṇayor jyeṣṭham r̥kṣam āhur janādhipam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 30
  },
  "01089030c": {
    "text": {
      "ascii": "RkSAt saMvaraNo jajJe rAjan vaMzakaras tava\n",
      "ud": "ऋक्षात्संवरणो जज्ञे राजन्वंशकरस्तव\n",
      "ur": "r̥kṣāt saṁvaraṇo jajñe rājan vaṁśakaras tava\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 30
  },
  "01089031a": {
    "text": {
      "ascii": "ArkSe saMvaraNe rAjan prazAsati vasuMdharAm\n",
      "ud": "आर्क्षे संवरणे राजन्प्रशासति वसुंधराम्\n",
      "ur": "ārkṣe saṁvaraṇe rājan praśāsati vasuṁdharām\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 31
  },
  "01089031c": {
    "text": {
      "ascii": "saMkSayaH sumahAn AsIt prajAnAm iti zuzrumaH\n",
      "ud": "संक्षयः सुमहानासीत्प्रजानामिति शुश्रुमः\n",
      "ur": "saṁkṣayaḥ sumahān āsīt prajānām iti śuśrumaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 31
  },
  "01089032a": {
    "text": {
      "ascii": "vyazIryata tato rASTraM kSayair nAnAvidhais tathA\n",
      "ud": "व्यशीर्यत ततो राष्ट्रं क्षयैर्नानाविधैस्तथा\n",
      "ur": "vyaśīryata tato rāṣṭraṁ kṣayair nānāvidhais tathā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 32
  },
  "01089032c": {
    "text": {
      "ascii": "kSunmRtyubhyAm anAvRSTyA vyAdhibhiz ca samAhatam\n",
      "ud": "क्षुन्मृत्युभ्यामनावृष्ट्या व्याधिभिश्च समाहतम्\n",
      "ur": "kṣunmr̥tyubhyām anāvr̥ṣṭyā vyādhibhiś ca samāhatam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 32
  },
  "01089032e": {
    "text": {
      "ascii": "abhyaghnan bhAratAMz caiva sapatnAnAM balAni ca\n",
      "ud": "अभ्यघ्नन्भारतांश्चैव सपत्नानां बलानि च\n",
      "ur": "abhyaghnan bhāratāṁś caiva sapatnānāṁ balāni ca\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 32
  },
  "01089033a": {
    "text": {
      "ascii": "cAlayan vasudhAM caiva balena caturaGgiNA\n",
      "ud": "चालयन्वसुधां चैव बलेन चतुरङ्गिणा\n",
      "ur": "cālayan vasudhāṁ caiva balena caturaṅgiṇā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 33
  },
  "01089033c": {
    "text": {
      "ascii": "abhyayAt taM ca pAJcAlyo vijitya tarasA mahIm\n",
      "ud": "अभ्ययात्तं च पाञ्चाल्यो विजित्य तरसा महीम्\n",
      "ur": "abhyayāt taṁ ca pāñcālyo vijitya tarasā mahīm\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 33
  },
  "01089033e": {
    "text": {
      "ascii": "akSauhiNIbhir dazabhiH sa enaM samare 'jayat\n",
      "ud": "अक्षौहिणीभिर्दशभिः स एनं समरेऽजयत्\n",
      "ur": "akṣauhiṇībhir daśabhiḥ sa enaṁ samare ’jayat\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 33
  },
  "01089034a": {
    "text": {
      "ascii": "tataH sadAraH sAmAtyaH saputraH sasuhRjjanaH\n",
      "ud": "ततः सदारः सामात्यः सपुत्रः ससुहृज्जनः\n",
      "ur": "tataḥ sadāraḥ sāmātyaḥ saputraḥ sasuhr̥jjanaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 34
  },
  "01089034c": {
    "text": {
      "ascii": "rAjA saMvaraNas tasmAt palAyata mahAbhayAt\n",
      "ud": "राजा संवरणस्तस्मात्पलायत महाभयात्\n",
      "ur": "rājā saṁvaraṇas tasmāt palāyata mahābhayāt\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 34
  },
  "01089035a": {
    "text": {
      "ascii": "sindhor nadasya mahato nikuJje nyavasat tadA\n",
      "ud": "सिन्धोर्नदस्य महतो निकुञ्जे न्यवसत्तदा\n",
      "ur": "sindhor nadasya mahato nikuñje nyavasat tadā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 35
  },
  "01089035c": {
    "text": {
      "ascii": "nadIviSayaparyante parvatasya samIpataH\n",
      "ud": "नदीविषयपर्यन्ते पर्वतस्य समीपतः\n",
      "ur": "nadīviṣayaparyante parvatasya samīpataḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 35
  },
  "01089035e": {
    "text": {
      "ascii": "tatrAvasan bahUn kAlAn bhAratA durgamAzritAH\n",
      "ud": "तत्रावसन्बहून्कालान्भारता दुर्गमाश्रिताः\n",
      "ur": "tatrāvasan bahūn kālān bhāratā durgamāśritāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 35
  },
  "01089036a": {
    "text": {
      "ascii": "teSAM nivasatAM tatra sahasraM parivatsarAn\n",
      "ud": "तेषां निवसतां तत्र सहस्रं परिवत्सरान्\n",
      "ur": "teṣāṁ nivasatāṁ tatra sahasraṁ parivatsarān\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 36
  },
  "01089036c": {
    "text": {
      "ascii": "athAbhyagacchad bharatAn vasiSTho bhagavAn RSiH\n",
      "ud": "अथाभ्यगच्छद्भरतान्वसिष्ठो भगवानृषिः\n",
      "ur": "athābhyagacchad bharatān vasiṣṭho bhagavān r̥ṣiḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 36
  },
  "01089037a": {
    "text": {
      "ascii": "tam AgataM prayatnena pratyudgamyAbhivAdya ca\n",
      "ud": "तमागतं प्रयत्नेन प्रत्युद्गम्याभिवाद्य च\n",
      "ur": "tam āgataṁ prayatnena pratyudgamyābhivādya ca\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 37
  },
  "01089037c": {
    "text": {
      "ascii": "arghyam abhyAharaMs tasmai te sarve bhAratAs tadA\n",
      "ud": "अर्घ्यमभ्याहरंस्तस्मै ते सर्वे भारतास्तदा\n",
      "ur": "arghyam abhyāharaṁs tasmai te sarve bhāratās tadā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 37
  },
  "01089037e": {
    "text": {
      "ascii": "nivedya sarvam RSaye satkAreNa suvarcase\n",
      "ud": "निवेद्य सर्वमृषये सत्कारेण सुवर्चसे\n",
      "ur": "nivedya sarvam r̥ṣaye satkāreṇa suvarcase\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 37
  },
  "01089038a": {
    "text": {
      "ascii": "taM samAm aSTamIm uSTaM rAjA vavre svayaM tadA\n",
      "ud": "तं समामष्टमीमुष्टं राजा वव्रे स्वयं तदा\n",
      "ur": "taṁ samām aṣṭamīm uṣṭaṁ rājā vavre svayaṁ tadā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 38
  },
  "01089038c": {
    "text": {
      "ascii": "purohito bhavAn no 'stu rAjyAya prayatAmahe\n",
      "ud": "पुरोहितो भवान्नोऽस्तु राज्याय प्रयतामहे\n",
      "ur": "purohito bhavān no ’stu rājyāya prayatāmahe\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 38
  },
  "01089038e": {
    "text": {
      "ascii": "om ity evaM vasiSTho 'pi bhAratAn pratyapadyata\n",
      "ud": "ओमित्येवं वसिष्ठोऽपि भारतान्प्रत्यपद्यत\n",
      "ur": "om ity evaṁ vasiṣṭho ’pi bhāratān pratyapadyata\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 38
  },
  "01089039a": {
    "text": {
      "ascii": "athAbhyaSiJcat sAmrAjye sarvakSatrasya pauravam\n",
      "ud": "अथाभ्यषिञ्चत्साम्राज्ये सर्वक्षत्रस्य पौरवम्\n",
      "ur": "athābhyaṣiñcat sāmrājye sarvakṣatrasya pauravam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 39
  },
  "01089039c": {
    "text": {
      "ascii": "viSANabhUtaM sarvasyAM pRthivyAm iti naH zrutam\n",
      "ud": "विषाणभूतं सर्वस्यां पृथिव्यामिति नः श्रुतम्\n",
      "ur": "viṣāṇabhūtaṁ sarvasyāṁ pr̥thivyām iti naḥ śrutam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 39
  },
  "01089040a": {
    "text": {
      "ascii": "bharatAdhyuSitaM pUrvaM so 'dhyatiSThat purottamam\n",
      "ud": "भरताध्युषितं पूर्वं सोऽध्यतिष्ठत्पुरोत्तमम्\n",
      "ur": "bharatādhyuṣitaṁ pūrvaṁ so ’dhyatiṣṭhat purottamam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 40
  },
  "01089040c": {
    "text": {
      "ascii": "punar balibhRtaz caiva cakre sarvamahIkSitaH\n",
      "ud": "पुनर्बलिभृतश्चैव चक्रे सर्वमहीक्षितः\n",
      "ur": "punar balibhr̥taś caiva cakre sarvamahīkṣitaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 40
  },
  "01089041a": {
    "text": {
      "ascii": "tataH sa pRthivIM prApya punar Ije mahAbalaH\n",
      "ud": "ततः स पृथिवीं प्राप्य पुनरीजे महाबलः\n",
      "ur": "tataḥ sa pr̥thivīṁ prāpya punar īje mahābalaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 41
  },
  "01089041c": {
    "text": {
      "ascii": "AjamIDho mahAyajJair bahubhir bhUridakSiNaiH\n",
      "ud": "आजमीढो महायज्ञैर्बहुभिर्भूरिदक्षिणैः\n",
      "ur": "ājamīḍho mahāyajñair bahubhir bhūridakṣiṇaiḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 41
  },
  "01089042a": {
    "text": {
      "ascii": "tataH saMvaraNAt saurI suSuve tapatI kurum\n",
      "ud": "ततः संवरणात्सौरी सुषुवे तपती कुरुम्\n",
      "ur": "tataḥ saṁvaraṇāt saurī suṣuve tapatī kurum\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 42
  },
  "01089042c": {
    "text": {
      "ascii": "rAjatve taM prajAH sarvA dharmajJa iti vavrire\n",
      "ud": "राजत्वे तं प्रजाः सर्वा धर्मज्ञ इति वव्रिरे\n",
      "ur": "rājatve taṁ prajāḥ sarvā dharmajña iti vavrire\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 42
  },
  "01089043a": {
    "text": {
      "ascii": "tasya nAmnAbhivikhyAtaM pRthivyAM kurujAGgalam\n",
      "ud": "तस्य नाम्नाभिविख्यातं पृथिव्यां कुरुजाङ्गलम्\n",
      "ur": "tasya nāmnābhivikhyātaṁ pr̥thivyāṁ kurujāṅgalam\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 43
  },
  "01089043c": {
    "text": {
      "ascii": "kurukSetraM sa tapasA puNyaM cakre mahAtapAH\n",
      "ud": "कुरुक्षेत्रं स तपसा पुण्यं चक्रे महातपाः\n",
      "ur": "kurukṣetraṁ sa tapasā puṇyaṁ cakre mahātapāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 43
  },
  "01089044a": {
    "text": {
      "ascii": "azvavantam abhiSvantaM tathA citrarathaM munim\n",
      "ud": "अश्ववन्तमभिष्वन्तं तथा चित्ररथं मुनिम्\n",
      "ur": "aśvavantam abhiṣvantaṁ tathā citrarathaṁ munim\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 44
  },
  "01089044c": {
    "text": {
      "ascii": "janamejayaM ca vikhyAtaM putrAMz cAsyAnuzuzrumaH\n",
      "ud": "जनमेजयं च विख्यातं पुत्रांश्चास्यानुशुश्रुमः\n",
      "ur": "janamejayaṁ ca vikhyātaṁ putrāṁś cāsyānuśuśrumaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 44
  },
  "01089044e": {
    "text": {
      "ascii": "paJcaitAn vAhinI putrAn vyajAyata manasvinI\n",
      "ud": "पञ्चैतान्वाहिनी पुत्रान्व्यजायत मनस्विनी\n",
      "ur": "pañcaitān vāhinī putrān vyajāyata manasvinī\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 44
  },
  "01089045a": {
    "text": {
      "ascii": "abhiSvataH parikSit tu zabalAzvaz ca vIryavAn\n",
      "ud": "अभिष्वतः परिक्षित्तु शबलाश्वश्च वीर्यवान्\n",
      "ur": "abhiṣvataḥ parikṣit tu śabalāśvaś ca vīryavān\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 45
  },
  "01089045c": {
    "text": {
      "ascii": "abhirAjo virAjaz ca zalmalaz ca mahAbalaH\n",
      "ud": "अभिराजो विराजश्च शल्मलश्च महाबलः\n",
      "ur": "abhirājo virājaś ca śalmalaś ca mahābalaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 45
  },
  "01089046a": {
    "text": {
      "ascii": "uccaiHzravA bhadrakAro jitAriz cASTamaH smRtaH\n",
      "ud": "उच्चैःश्रवा भद्रकारो जितारिश्चाष्टमः स्मृतः\n",
      "ur": "uccaiḥśravā bhadrakāro jitāriś cāṣṭamaḥ smr̥taḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 46
  },
  "01089046c": {
    "text": {
      "ascii": "eteSAm anvavAye tu khyAtAs te karmajair guNaiH\n",
      "ud": "एतेषामन्ववाये तु ख्यातास्ते कर्मजैर्गुणैः\n",
      "ur": "eteṣām anvavāye tu khyātās te karmajair guṇaiḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 46
  },
  "01089047a": {
    "text": {
      "ascii": "janamejayAdayaH sapta tathaivAnye mahAbalAH\n",
      "ud": "जनमेजयादयः सप्त तथैवान्ये महाबलाः\n",
      "ur": "janamejayādayaḥ sapta tathaivānye mahābalāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 47
  },
  "01089047c": {
    "text": {
      "ascii": "parikSito 'bhavan putrAH sarve dharmArthakovidAH\n",
      "ud": "परिक्षितोऽभवन्पुत्राः सर्वे धर्मार्थकोविदाः\n",
      "ur": "parikṣito ’bhavan putrāḥ sarve dharmārthakovidāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 47
  },
  "01089048a": {
    "text": {
      "ascii": "kakSasenograsenau ca citrasenaz ca vIryavAn\n",
      "ud": "कक्षसेनोग्रसेनौ च चित्रसेनश्च वीर्यवान्\n",
      "ur": "kakṣasenograsenau ca citrasenaś ca vīryavān\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 48
  },
  "01089048c": {
    "text": {
      "ascii": "indrasenaH suSeNaz ca bhImasenaz ca nAmataH\n",
      "ud": "इन्द्रसेनः सुषेणश्च भीमसेनश्च नामतः\n",
      "ur": "indrasenaḥ suṣeṇaś ca bhīmasenaś ca nāmataḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 48
  },
  "01089049a": {
    "text": {
      "ascii": "janamejayasya tanayA bhuvi khyAtA mahAbalAH\n",
      "ud": "जनमेजयस्य तनया भुवि ख्याता महाबलाः\n",
      "ur": "janamejayasya tanayā bhuvi khyātā mahābalāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 49
  },
  "01089049c": {
    "text": {
      "ascii": "dhRtarASTraH prathamajaH pANDur bAhlIka eva ca\n",
      "ud": "धृतराष्ट्रः प्रथमजः पाण्डुर्बाह्लीक एव च\n",
      "ur": "dhr̥tarāṣṭraḥ prathamajaḥ pāṇḍur bāhlīka eva ca\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 49
  },
  "01089050a": {
    "text": {
      "ascii": "niSadhaz ca mahAtejAs tathA jAmbUnado balI\n",
      "ud": "निषधश्च महातेजास्तथा जाम्बूनदो बली\n",
      "ur": "niṣadhaś ca mahātejās tathā jāmbūnado balī\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 50
  },
  "01089050c": {
    "text": {
      "ascii": "kuNDodaraH padAtiz ca vasAtiz cASTamaH smRtaH\n",
      "ud": "कुण्डोदरः पदातिश्च वसातिश्चाष्टमः स्मृतः\n",
      "ur": "kuṇḍodaraḥ padātiś ca vasātiś cāṣṭamaḥ smr̥taḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 50
  },
  "01089050e": {
    "text": {
      "ascii": "sarve dharmArthakuzalAH sarve bhUtahite ratAH\n",
      "ud": "सर्वे धर्मार्थकुशलाः सर्वे भूतहिते रताः\n",
      "ur": "sarve dharmārthakuśalāḥ sarve bhūtahite ratāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 50
  },
  "01089051a": {
    "text": {
      "ascii": "dhRtarASTro 'tha rAjAsIt tasya putro 'tha kuNDikaH\n",
      "ud": "धृतराष्ट्रोऽथ राजासीत्तस्य पुत्रोऽथ कुण्डिकः\n",
      "ur": "dhr̥tarāṣṭro ’tha rājāsīt tasya putro ’tha kuṇḍikaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 51
  },
  "01089051c": {
    "text": {
      "ascii": "hastI vitarkaH krAthaz ca kuNDalaz cApi paJcamaH\n",
      "ud": "हस्ती वितर्कः क्राथश्च कुण्डलश्चापि पञ्चमः\n",
      "ur": "hastī vitarkaḥ krāthaś ca kuṇḍalaś cāpi pañcamaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 51
  },
  "01089051e": {
    "text": {
      "ascii": "haviHzravAs tathendrAbhaH sumanyuz cAparAjitaH\n",
      "ud": "हविःश्रवास्तथेन्द्राभः सुमन्युश्चापराजितः\n",
      "ur": "haviḥśravās tathendrābhaḥ sumanyuś cāparājitaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 51
  },
  "01089052a": {
    "text": {
      "ascii": "pratIpasya trayaH putrA jajJire bharatarSabha\n",
      "ud": "प्रतीपस्य त्रयः पुत्रा जज्ञिरे भरतर्षभ\n",
      "ur": "pratīpasya trayaḥ putrā jajñire bharatarṣabha\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 52
  },
  "01089052c": {
    "text": {
      "ascii": "devApiH zaMtanuz caiva bAhlIkaz ca mahArathaH\n",
      "ud": "देवापिः शंतनुश्चैव बाह्लीकश्च महारथः\n",
      "ur": "devāpiḥ śaṁtanuś caiva bāhlīkaś ca mahārathaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 52
  },
  "01089053a": {
    "text": {
      "ascii": "devApis tu pravavrAja teSAM dharmaparIpsayA\n",
      "ud": "देवापिस्तु प्रवव्राज तेषां धर्मपरीप्सया\n",
      "ur": "devāpis tu pravavrāja teṣāṁ dharmaparīpsayā\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 53
  },
  "01089053c": {
    "text": {
      "ascii": "zaMtanuz ca mahIM lebhe bAhlIkaz ca mahArathaH\n",
      "ud": "शंतनुश्च महीं लेभे बाह्लीकश्च महारथः\n",
      "ur": "śaṁtanuś ca mahīṁ lebhe bāhlīkaś ca mahārathaḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 53
  },
  "01089054a": {
    "text": {
      "ascii": "bharatasyAnvaye jAtAH sattvavanto mahArathAH\n",
      "ud": "भरतस्यान्वये जाताः सत्त्ववन्तो महारथाः\n",
      "ur": "bharatasyānvaye jātāḥ sattvavanto mahārathāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 54
  },
  "01089054c": {
    "text": {
      "ascii": "devarSikalpA nRpate bahavo rAjasattamAH\n",
      "ud": "देवर्षिकल्पा नृपते बहवो राजसत्तमाः\n",
      "ur": "devarṣikalpā nr̥pate bahavo rājasattamāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 54
  },
  "01089055a": {
    "text": {
      "ascii": "evaMvidhAz cApy apare devakalpA mahArathAH\n",
      "ud": "एवंविधाश्चाप्यपरे देवकल्पा महारथाः\n",
      "ur": "evaṁvidhāś cāpy apare devakalpā mahārathāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 55
  },
  "01089055c": {
    "text": {
      "ascii": "jAtA manor anvavAye ailavaMzavivardhanAH\n",
      "ud": "जाता मनोरन्ववाये ऐलवंशविवर्धनाः\n",
      "ur": "jātā manor anvavāye ailavaṁśavivardhanāḥ\n"
    },
    "book": 1,
    "chapter": 89,
    "verse": 55
  }
}
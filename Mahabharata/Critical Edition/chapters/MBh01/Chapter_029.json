    "book": 1,
    "chapter": 29,
    "verse": 14
  },
  "01029014c": {
    "text": {
      "ascii": "ajaraz cAmaraz ca syAm amRtena vinApy aham\n",
      "ud": "अजरश्चामरश्च स्याममृतेन विनाप्यहम्\n",
      "ur": "ajaraś cāmaraś ca syām amr̥tena vināpy aham\n"

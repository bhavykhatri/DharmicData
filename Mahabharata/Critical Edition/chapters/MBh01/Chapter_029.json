    "book": 1,
    "chapter": 29,
    "verse": 17
  },
  "01029017c": {
    "text": {
      "ascii": "vihaMgamaM surAmitraM harantam amRtaM balAt\n",
      "ud": "विहंगमं सुरामित्रं हरन्तममृतं बलात्\n",
      "ur": "vihaṁgamaṁ surāmitraṁ harantam amr̥taṁ balāt\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 17
  },
  "01029018a": {
    "text": {
      "ascii": "tam uvAcendram Akrande garuDaH patatAM varaH\n",
      "ud": "तमुवाचेन्द्रमाक्रन्दे गरुडः पततां वरः\n",
      "ur": "tam uvācendram ākrande garuḍaḥ patatāṁ varaḥ\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 18
  },
  "01029018c": {
    "text": {
      "ascii": "prahasaJ zlakSNayA vAcA tathA vajrasamAhataH\n",
      "ud": "प्रहसञ्श्लक्ष्णया वाचा तथा वज्रसमाहतः\n",
      "ur": "prahasañ ślakṣṇayā vācā tathā vajrasamāhataḥ\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 18
  },
  "01029019a": {
    "text": {
      "ascii": "RSer mAnaM kariSyAmi vajraM yasyAsthisaMbhavam\n",
      "ud": "ऋषेर्मानं करिष्यामि वज्रं यस्यास्थिसंभवम्\n",
      "ur": "r̥ṣer mānaṁ kariṣyāmi vajraṁ yasyāsthisaṁbhavam\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 19
  },
  "01029019c": {
    "text": {
      "ascii": "vajrasya ca kariSyAmi tava caiva zatakrato\n",
      "ud": "वज्रस्य च करिष्यामि तव चैव शतक्रतो\n",
      "ur": "vajrasya ca kariṣyāmi tava caiva śatakrato\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 19
  },
  "01029020a": {
    "text": {
      "ascii": "eSa patraM tyajAmy ekaM yasyAntaM nopalapsyase\n",
      "ud": "एष पत्रं त्यजाम्येकं यस्यान्तं नोपलप्स्यसे\n",
      "ur": "eṣa patraṁ tyajāmy ekaṁ yasyāntaṁ nopalapsyase\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 20
  },
  "01029020c": {
    "text": {
      "ascii": "na hi vajranipAtena rujA me 'sti kadA cana\n",
      "ud": "न हि वज्रनिपातेन रुजा मेऽस्ति कदाचन\n",
      "ur": "na hi vajranipātena rujā me ’sti kadā cana\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 20
  },
  "01029021a": {
    "text": {
      "ascii": "tatra taM sarvabhUtAni vismitAny abruvaMs tadA\n",
      "ud": "तत्र तं सर्वभूतानि विस्मितान्यब्रुवंस्तदा\n",
      "ur": "tatra taṁ sarvabhūtāni vismitāny abruvaṁs tadā\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 21
  },
  "01029021c": {
    "text": {
      "ascii": "surUpaM patram AlakSya suparNo 'yaM bhavatv iti\n",
      "ud": "सुरूपं पत्रमालक्ष्य सुपर्णोऽयं भवत्विति\n",
      "ur": "surūpaṁ patram ālakṣya suparṇo ’yaṁ bhavatv iti\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 21
  },
  "01029022a": {
    "text": {
      "ascii": "dRSTvA tad adbhutaM cApi sahasrAkSaH puraMdaraH\n",
      "ud": "दृष्ट्वा तदद्भुतं चापि सहस्राक्षः पुरंदरः\n",
      "ur": "dr̥ṣṭvā tad adbhutaṁ cāpi sahasrākṣaḥ puraṁdaraḥ\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 22
  },
  "01029022c": {
    "text": {
      "ascii": "khago mahad idaM bhUtam iti matvAbhyabhASata\n",
      "ud": "खगो महदिदं भूतमिति मत्वाभ्यभाषत\n",
      "ur": "khago mahad idaṁ bhūtam iti matvābhyabhāṣata\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 22
  },
  "01029023a": {
    "text": {
      "ascii": "balaM vijJAtum icchAmi yat te param anuttamam\n",
      "ud": "बलं विज्ञातुमिच्छामि यत्ते परमनुत्तमम्\n",
      "ur": "balaṁ vijñātum icchāmi yat te param anuttamam\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 23
  },
  "01029023c": {
    "text": {
      "ascii": "sakhyaM cAnantam icchAmi tvayA saha khagottama\n",
      "ud": "सख्यं चानन्तमिच्छामि त्वया सह खगोत्तम\n",
      "ur": "sakhyaṁ cānantam icchāmi tvayā saha khagottama\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 23
  }
}
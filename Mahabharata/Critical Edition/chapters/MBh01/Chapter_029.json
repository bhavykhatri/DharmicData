    "book": 1,
    "chapter": 29,
    "verse": 13
  },
  "01029014a": {
    "text": {
      "ascii": "uvAca cainaM bhUyo 'pi nArAyaNam idaM vacaH\n",
      "ud": "उवाच चैनं भूयोऽपि नारायणमिदं वचः\n",

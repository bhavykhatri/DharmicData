    "book": 1,
    "chapter": 29,
    "verse": 4
  },
  "01029005a": {
    "text": {
      "ascii": "adhaz cakrasya caivAtra dIptAnalasamadyutI\n",
      "ud": "अधश्चक्रस्य चैवात्र दीप्तानलसमद्युती\n",
      "ur": "adhaś cakrasya caivātra dīptānalasamadyutī\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 5
  },
  "01029005c": {
    "text": {
      "ascii": "vidyujjihvau mahAghorau dIptAsyau dIptalocanau\n",
      "ud": "विद्युज्जिह्वौ महाघोरौ दीप्तास्यौ दीप्तलोचनौ\n",
      "ur": "vidyujjihvau mahāghorau dīptāsyau dīptalocanau\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 5
  },
  "01029006a": {
    "text": {
      "ascii": "cakSurviSau mahAvIryau nityakruddhau tarasvinau\n",
      "ud": "चक्षुर्विषौ महावीर्यौ नित्यक्रुद्धौ तरस्विनौ\n",
      "ur": "cakṣurviṣau mahāvīryau nityakruddhau tarasvinau\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 6
  },
  "01029006c": {
    "text": {
      "ascii": "rakSArtham evAmRtasya dadarza bhujagottamau\n",
      "ud": "रक्षार्थमेवामृतस्य ददर्श भुजगोत्तमौ\n",
      "ur": "rakṣārtham evāmr̥tasya dadarśa bhujagottamau\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 6
  },
  "01029007a": {
    "text": {
      "ascii": "sadA saMrabdhanayanau sadA cAnimiSekSaNau\n",
      "ud": "सदा संरब्धनयनौ सदा चानिमिषेक्षणौ\n",
      "ur": "sadā saṁrabdhanayanau sadā cānimiṣekṣaṇau\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 7
  },
  "01029007c": {
    "text": {
      "ascii": "tayor eko 'pi yaM pazyet sa tUrNaM bhasmasAd bhavet\n",
      "ud": "तयोरेकोऽपि यं पश्येत्स तूर्णं भस्मसाद्भवेत्\n",
      "ur": "tayor eko ’pi yaṁ paśyet sa tūrṇaṁ bhasmasād bhavet\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 7
  },
  "01029008a": {
    "text": {
      "ascii": "tayoz cakSUMSi rajasA suparNas tUrNam AvRNot\n",
      "ud": "तयोश्चक्षूंषि रजसा सुपर्णस्तूर्णमावृणोत्\n",
      "ur": "tayoś cakṣūṁṣi rajasā suparṇas tūrṇam āvr̥ṇot\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 8
  },
  "01029008c": {
    "text": {
      "ascii": "adRSTarUpas tau cApi sarvataH paryakAlayat\n",
      "ud": "अदृष्टरूपस्तौ चापि सर्वतः पर्यकालयत्\n",
      "ur": "adr̥ṣṭarūpas tau cāpi sarvataḥ paryakālayat\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 8
  },
  "01029009a": {
    "text": {
      "ascii": "tayor aGge samAkramya vainateyo 'ntarikSagaH\n",
      "ud": "तयोरङ्गे समाक्रम्य वैनतेयोऽन्तरिक्षगः\n",
      "ur": "tayor aṅge samākramya vainateyo ’ntarikṣagaḥ\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 9
  },
  "01029009c": {
    "text": {
      "ascii": "Acchinat tarasA madhye somam abhyadravat tataH\n",
      "ud": "आच्छिनत्तरसा मध्ये सोममभ्यद्रवत्ततः\n",
      "ur": "ācchinat tarasā madhye somam abhyadravat tataḥ\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 9
  },
  "01029010a": {
    "text": {
      "ascii": "samutpATyAmRtaM tat tu vainateyas tato balI\n",
      "ud": "समुत्पाट्यामृतं तत्तु वैनतेयस्ततो बली\n",
      "ur": "samutpāṭyāmr̥taṁ tat tu vainateyas tato balī\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 10
  },
  "01029010c": {
    "text": {
      "ascii": "utpapAta javenaiva yantram unmathya vIryavAn\n",
      "ud": "उत्पपात जवेनैव यन्त्रमुन्मथ्य वीर्यवान्\n",
      "ur": "utpapāta javenaiva yantram unmathya vīryavān\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 10
  },
  "01029011a": {
    "text": {
      "ascii": "apItvaivAmRtaM pakSI parigRhyAzu vIryavAn\n",
      "ud": "अपीत्वैवामृतं पक्षी परिगृह्याशु वीर्यवान्\n",
      "ur": "apītvaivāmr̥taṁ pakṣī parigr̥hyāśu vīryavān\n"
    },
    "book": 1,
    "chapter": 29,
    "verse": 11
  },
  "01029011c": {
    "text": {
      "ascii": "agacchad aparizrAnta AvAryArkaprabhAM khagaH\n",
      "ud": "अगच्छदपरिश्रान्त आवार्यार्कप्रभां खगः\n",

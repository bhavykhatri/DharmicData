{
  "01012001": {
    "text": {
      "ascii": "rurur uvAca\n",
      "ud": "रुरुरुवाच\n",
      "ur": "rurur uvāca\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 1
  },
  "01012001a": {
    "text": {
      "ascii": "kathaM hiMsitavAn sarpAn kSatriyo janamejayaH\n",
      "ud": "कथं हिंसितवान्सर्पान्क्षत्रियो जनमेजयः\n",
      "ur": "kathaṁ hiṁsitavān sarpān kṣatriyo janamejayaḥ\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 1
  },
  "01012001c": {
    "text": {
      "ascii": "sarpA vA hiMsitAs tAta kimarthaM dvijasattama\n",
      "ud": "सर्पा वा हिंसितास्तात किमर्थं द्विजसत्तम\n",
      "ur": "sarpā vā hiṁsitās tāta kimarthaṁ dvijasattama\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 1
  },
  "01012002a": {
    "text": {
      "ascii": "kimarthaM mokSitAz caiva pannagAs tena zaMsa me\n",
      "ud": "किमर्थं मोक्षिताश्चैव पन्नगास्तेन शंस मे\n",
      "ur": "kimarthaṁ mokṣitāś caiva pannagās tena śaṁsa me\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 2
  },
  "01012002c": {
    "text": {
      "ascii": "AstIkena tad AcakSva zrotum icchAmy azeSataH\n",
      "ud": "आस्तीकेन तदाचक्ष्व श्रोतुमिच्छाम्यशेषतः\n",
      "ur": "āstīkena tad ācakṣva śrotum icchāmy aśeṣataḥ\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 2
  },
  "01012003": {
    "text": {
      "ascii": "RSir uvAca\n",
      "ud": "ऋषिरुवाच\n",
      "ur": "r̥ṣir uvāca\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 3
  },
  "01012003a": {
    "text": {
      "ascii": "zroSyasi tvaM ruro sarvam AstIkacaritaM mahat\n",
      "ud": "श्रोष्यसि त्वं रुरो सर्वमास्तीकचरितं महत्\n",
      "ur": "śroṣyasi tvaṁ ruro sarvam āstīkacaritaṁ mahat\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 3
  },
  "01012003c": {
    "text": {
      "ascii": "brAhmaNAnAM kathayatAm ity uktvAntaradhIyata\n",
      "ud": "ब्राह्मणानां कथयतामित्युक्त्वान्तरधीयत\n",
      "ur": "brāhmaṇānāṁ kathayatām ity uktvāntaradhīyata\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 3
  },
  "01012004": {
    "text": {
      "ascii": "sUta uvAca\n",
      "ud": "सूत उवाच\n",
      "ur": "sūta uvāca\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 4
  },
  "01012004a": {
    "text": {
      "ascii": "ruruz cApi vanaM sarvaM paryadhAvat samantataH\n",
      "ud": "रुरुश्चापि वनं सर्वं पर्यधावत्समन्ततः\n",
      "ur": "ruruś cāpi vanaṁ sarvaṁ paryadhāvat samantataḥ\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 4
  },
  "01012004c": {
    "text": {
      "ascii": "tam RSiM draSTum anvicchan saMzrAnto nyapatad bhuvi\n",
      "ud": "तमृषिं द्रष्टुमन्विच्छन्संश्रान्तो न्यपतद्भुवि\n",
      "ur": "tam r̥ṣiṁ draṣṭum anvicchan saṁśrānto nyapatad bhuvi\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 4
  },
  "01012005a": {
    "text": {
      "ascii": "labdhasaMjJo ruruz cAyAt tac cAcakhyau pitus tadA\n",
      "ud": "लब्धसंज्ञो रुरुश्चायात्तच्चाचख्यौ पितुस्तदा\n",
      "ur": "labdhasaṁjño ruruś cāyāt tac cācakhyau pitus tadā\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 5
  },
  "01012005c": {
    "text": {
      "ascii": "pitA cAsya tad AkhyAnaM pRSTaH sarvaM nyavedayat\n",
      "ud": "पिता चास्य तदाख्यानं पृष्टः सर्वं न्यवेदयत्\n",
      "ur": "pitā cāsya tad ākhyānaṁ pr̥ṣṭaḥ sarvaṁ nyavedayat\n"
    },
    "book": 1,
    "chapter": 12,
    "verse": 5
  }
}
    "book": 1,
    "chapter": 64,
    "verse": 11
  },
  "01064012a": {
    "text": {
      "ascii": "parasparAzliSTazAkhaiH pAdapaiH kusumAcitaiH\n",
      "ud": "परस्पराश्लिष्टशाखैः पादपैः कुसुमाचितैः\n",

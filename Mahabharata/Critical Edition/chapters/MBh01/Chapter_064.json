    },
    "book": 1,
    "chapter": 64,
    "verse": 22
  },
  "01064023a": {
    "text": {
      "ascii": "nadIm AzramasaMbaddhAM dRSTvAzramapadaM tathA\n",
      "ud": "नदीमाश्रमसंबद्धां दृष्ट्वाश्रमपदं तथा\n",
      "ur": "nadīm āśramasaṁbaddhāṁ dr̥ṣṭvāśramapadaṁ tathā\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 23
  },
  "01064023c": {
    "text": {
      "ascii": "cakArAbhipravezAya matiM sa nRpatis tadA\n",
      "ud": "चकाराभिप्रवेशाय मतिं स नृपतिस्तदा\n",
      "ur": "cakārābhipraveśāya matiṁ sa nr̥patis tadā\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 23
  },
  "01064024a": {
    "text": {
      "ascii": "alaMkRtaM dvIpavatyA mAlinyA ramyatIrayA\n",
      "ud": "अलंकृतं द्वीपवत्या मालिन्या रम्यतीरया\n",
      "ur": "alaṁkr̥taṁ dvīpavatyā mālinyā ramyatīrayā\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 24
  },
  "01064024c": {
    "text": {
      "ascii": "naranArAyaNasthAnaM gaGgayevopazobhitam\n",
      "ud": "नरनारायणस्थानं गङ्गयेवोपशोभितम्\n",
      "ur": "naranārāyaṇasthānaṁ gaṅgayevopaśobhitam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 24
  },
  "01064024e": {
    "text": {
      "ascii": "mattabarhiNasaMghuSTaM praviveza mahad vanam\n",
      "ud": "मत्तबर्हिणसंघुष्टं प्रविवेश महद्वनम्\n",
      "ur": "mattabarhiṇasaṁghuṣṭaṁ praviveśa mahad vanam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 24
  },
  "01064025a": {
    "text": {
      "ascii": "tat sa caitrarathaprakhyaM samupetya narezvaraH\n",
      "ud": "तत्स चैत्ररथप्रख्यं समुपेत्य नरेश्वरः\n",
      "ur": "tat sa caitrarathaprakhyaṁ samupetya nareśvaraḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 25
  },
  "01064025c": {
    "text": {
      "ascii": "atIva guNasaMpannam anirdezyaM ca varcasA\n",
      "ud": "अतीव गुणसंपन्नमनिर्देश्यं च वर्चसा\n",
      "ur": "atīva guṇasaṁpannam anirdeśyaṁ ca varcasā\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 25
  },
  "01064025e": {
    "text": {
      "ascii": "maharSiM kAzyapaM draSTum atha kaNvaM tapodhanam\n",
      "ud": "महर्षिं काश्यपं द्रष्टुमथ कण्वं तपोधनम्\n",
      "ur": "maharṣiṁ kāśyapaṁ draṣṭum atha kaṇvaṁ tapodhanam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 25
  },
  "01064026a": {
    "text": {
      "ascii": "rathinIm azvasaMbAdhAM padAtigaNasaMkulAm\n",
      "ud": "रथिनीमश्वसंबाधां पदातिगणसंकुलाम्\n",
      "ur": "rathinīm aśvasaṁbādhāṁ padātigaṇasaṁkulām\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 26
  },
  "01064026c": {
    "text": {
      "ascii": "avasthApya vanadvAri senAm idam uvAca saH\n",
      "ud": "अवस्थाप्य वनद्वारि सेनामिदमुवाच सः\n",
      "ur": "avasthāpya vanadvāri senām idam uvāca saḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 26
  },
  "01064027a": {
    "text": {
      "ascii": "muniM virajasaM draSTuM gamiSyAmi tapodhanam\n",
      "ud": "मुनिं विरजसं द्रष्टुं गमिष्यामि तपोधनम्\n",
      "ur": "muniṁ virajasaṁ draṣṭuṁ gamiṣyāmi tapodhanam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 27
  },
  "01064027c": {
    "text": {
      "ascii": "kAzyapaM sthIyatAm atra yAvadAgamanaM mama\n",
      "ud": "काश्यपं स्थीयतामत्र यावदागमनं मम\n",
      "ur": "kāśyapaṁ sthīyatām atra yāvadāgamanaṁ mama\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 27
  },
  "01064028a": {
    "text": {
      "ascii": "tad vanaM nandanaprakhyam AsAdya manujezvaraH\n",
      "ud": "तद्वनं नन्दनप्रख्यमासाद्य मनुजेश्वरः\n",
      "ur": "tad vanaṁ nandanaprakhyam āsādya manujeśvaraḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 28
  },
  "01064028c": {
    "text": {
      "ascii": "kSutpipAse jahau rAjA harSaM cAvApa puSkalam\n",
      "ud": "क्षुत्पिपासे जहौ राजा हर्षं चावाप पुष्कलम्\n",
      "ur": "kṣutpipāse jahau rājā harṣaṁ cāvāpa puṣkalam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 28
  },
  "01064029a": {
    "text": {
      "ascii": "sAmAtyo rAjaliGgAni so 'panIya narAdhipaH\n",
      "ud": "सामात्यो राजलिङ्गानि सोऽपनीय नराधिपः\n",
      "ur": "sāmātyo rājaliṅgāni so ’panīya narādhipaḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 29
  },
  "01064029c": {
    "text": {
      "ascii": "purohitasahAyaz ca jagAmAzramam uttamam\n",
      "ud": "पुरोहितसहायश्च जगामाश्रममुत्तमम्\n",
      "ur": "purohitasahāyaś ca jagāmāśramam uttamam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 29
  },
  "01064029e": {
    "text": {
      "ascii": "didRkSus tatra tam RSiM taporAzim athAvyayam\n",
      "ud": "दिदृक्षुस्तत्र तमृषिं तपोराशिमथाव्ययम्\n",
      "ur": "didr̥kṣus tatra tam r̥ṣiṁ taporāśim athāvyayam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 29
  },
  "01064030a": {
    "text": {
      "ascii": "brahmalokapratIkAzam AzramaM so 'bhivIkSya ca\n",
      "ud": "ब्रह्मलोकप्रतीकाशमाश्रमं सोऽभिवीक्ष्य च\n",
      "ur": "brahmalokapratīkāśam āśramaṁ so ’bhivīkṣya ca\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 30
  },
  "01064030c": {
    "text": {
      "ascii": "SaTpadodgItasaMghuSTaM nAnAdvijagaNAyutam\n",
      "ud": "षट्पदोद्गीतसंघुष्टं नानाद्विजगणायुतम्\n",
      "ur": "ṣaṭpadodgītasaṁghuṣṭaṁ nānādvijagaṇāyutam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 30
  },
  "01064031a": {
    "text": {
      "ascii": "Rco bahvRcamukhyaiz ca preryamANAH padakramaiH\n",
      "ud": "ऋचो बह्वृचमुख्यैश्च प्रेर्यमाणाः पदक्रमैः\n",
      "ur": "r̥co bahvr̥camukhyaiś ca preryamāṇāḥ padakramaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 31
  },
  "01064031c": {
    "text": {
      "ascii": "zuzrAva manujavyAghro vitateSv iha karmasu\n",
      "ud": "शुश्राव मनुजव्याघ्रो विततेष्विह कर्मसु\n",
      "ur": "śuśrāva manujavyāghro vitateṣv iha karmasu\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 31
  },
  "01064032a": {
    "text": {
      "ascii": "yajJavidyAGgavidbhiz ca kramadbhiz ca kramAn api\n",
      "ud": "यज्ञविद्याङ्गविद्भिश्च क्रमद्भिश्च क्रमानपि\n",
      "ur": "yajñavidyāṅgavidbhiś ca kramadbhiś ca kramān api\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 32
  },
  "01064032c": {
    "text": {
      "ascii": "amitAtmabhiH suniyataiH zuzubhe sa tadAzramaH\n",
      "ud": "अमितात्मभिः सुनियतैः शुशुभे स तदाश्रमः\n",
      "ur": "amitātmabhiḥ suniyataiḥ śuśubhe sa tadāśramaḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 32
  },
  "01064033a": {
    "text": {
      "ascii": "atharvavedapravarAH pUgayAjJika saMmatAH\n",
      "ud": "अथर्ववेदप्रवराः पूगयाज्ञिक संमताः\n",
      "ur": "atharvavedapravarāḥ pūgayājñika saṁmatāḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 33
  },
  "01064033c": {
    "text": {
      "ascii": "saMhitAm Irayanti sma padakramayutAM tu te\n",
      "ud": "संहितामीरयन्ति स्म पदक्रमयुतां तु ते\n",
      "ur": "saṁhitām īrayanti sma padakramayutāṁ tu te\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 33
  },
  "01064034a": {
    "text": {
      "ascii": "zabdasaMskArasaMyuktaM bruvadbhiz cAparair dvijaiH\n",
      "ud": "शब्दसंस्कारसंयुक्तं ब्रुवद्भिश्चापरैर्द्विजैः\n",
      "ur": "śabdasaṁskārasaṁyuktaṁ bruvadbhiś cāparair dvijaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 34
  },
  "01064034c": {
    "text": {
      "ascii": "nAditaH sa babhau zrImAn brahmaloka ivAzramaH\n",
      "ud": "नादितः स बभौ श्रीमान्ब्रह्मलोक इवाश्रमः\n",
      "ur": "nāditaḥ sa babhau śrīmān brahmaloka ivāśramaḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 34
  },
  "01064035a": {
    "text": {
      "ascii": "yajJasaMskAravidbhiz ca kramazikSA vizAradaiH\n",
      "ud": "यज्ञसंस्कारविद्भिश्च क्रमशिक्षा विशारदैः\n",
      "ur": "yajñasaṁskāravidbhiś ca kramaśikṣā viśāradaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 35
  },
  "01064035c": {
    "text": {
      "ascii": "nyAyatattvArthavijJAnasaMpannair vedapAragaiH\n",
      "ud": "न्यायतत्त्वार्थविज्ञानसंपन्नैर्वेदपारगैः\n",
      "ur": "nyāyatattvārthavijñānasaṁpannair vedapāragaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 35
  },
  "01064036a": {
    "text": {
      "ascii": "nAnAvAkyasamAhArasamavAyavizAradaiH\n",
      "ud": "नानावाक्यसमाहारसमवायविशारदैः\n",
      "ur": "nānāvākyasamāhārasamavāyaviśāradaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 36
  },
  "01064036c": {
    "text": {
      "ascii": "vizeSakAryavidbhiz ca mokSadharmaparAyaNaiH\n",
      "ud": "विशेषकार्यविद्भिश्च मोक्षधर्मपरायणैः\n",
      "ur": "viśeṣakāryavidbhiś ca mokṣadharmaparāyaṇaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 36
  },
  "01064037a": {
    "text": {
      "ascii": "sthApanAkSepasiddhAntaparamArthajJatAM gataiH\n",
      "ud": "स्थापनाक्षेपसिद्धान्तपरमार्थज्ञतां गतैः\n",
      "ur": "sthāpanākṣepasiddhāntaparamārthajñatāṁ gataiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 37
  },
  "01064037c": {
    "text": {
      "ascii": "lokAyatikamukhyaiz ca samantAd anunAditam\n",
      "ud": "लोकायतिकमुख्यैश्च समन्तादनुनादितम्\n",
      "ur": "lokāyatikamukhyaiś ca samantād anunāditam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 37
  },
  "01064038a": {
    "text": {
      "ascii": "tatra tatra ca viprendrAn niyatAn saMzitavratAn\n",
      "ud": "तत्र तत्र च विप्रेन्द्रान्नियतान्संशितव्रतान्\n",
      "ur": "tatra tatra ca viprendrān niyatān saṁśitavratān\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 38
  },
  "01064038c": {
    "text": {
      "ascii": "japahomaparAn siddhAn dadarza paravIrahA\n",
      "ud": "जपहोमपरान्सिद्धान्ददर्श परवीरहा\n",
      "ur": "japahomaparān siddhān dadarśa paravīrahā\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 38
  },
  "01064039a": {
    "text": {
      "ascii": "AsanAni vicitrANi puSpavanti mahIpatiH\n",
      "ud": "आसनानि विचित्राणि पुष्पवन्ति महीपतिः\n",
      "ur": "āsanāni vicitrāṇi puṣpavanti mahīpatiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 39
  },
  "01064039c": {
    "text": {
      "ascii": "prayatnopahitAni sma dRSTvA vismayam Agamat\n",
      "ud": "प्रयत्नोपहितानि स्म दृष्ट्वा विस्मयमागमत्\n",
      "ur": "prayatnopahitāni sma dr̥ṣṭvā vismayam āgamat\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 39
  },
  "01064040a": {
    "text": {
      "ascii": "devatAyatanAnAM ca pUjAM prekSya kRtAM dvijaiH\n",
      "ud": "देवतायतनानां च पूजां प्रेक्ष्य कृतां द्विजैः\n",
      "ur": "devatāyatanānāṁ ca pūjāṁ prekṣya kr̥tāṁ dvijaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 40
  },
  "01064040c": {
    "text": {
      "ascii": "brahmalokastham AtmAnaM mene sa nRpasattamaH\n",
      "ud": "ब्रह्मलोकस्थमात्मानं मेने स नृपसत्तमः\n",
      "ur": "brahmalokastham ātmānaṁ mene sa nr̥pasattamaḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 40
  },
  "01064041a": {
    "text": {
      "ascii": "sa kAzyapatapoguptam AzramapravaraM zubham\n",
      "ud": "स काश्यपतपोगुप्तमाश्रमप्रवरं शुभम्\n",
      "ur": "sa kāśyapatapoguptam āśramapravaraṁ śubham\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 41
  },
  "01064041c": {
    "text": {
      "ascii": "nAtRpyat prekSamANo vai tapodhanagaNair yutam\n",
      "ud": "नातृप्यत्प्रेक्षमाणो वै तपोधनगणैर्युतम्\n",
      "ur": "nātr̥pyat prekṣamāṇo vai tapodhanagaṇair yutam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 41
  },
  "01064042a": {
    "text": {
      "ascii": "sa kAzyapasyAyatanaM mahAvratair; vRtaM samantAd RSibhis tapodhanaiH\n",
      "ud": "स काश्यपस्यायतनं महाव्रतै;र्वृतं समन्तादृषिभिस्तपोधनैः\n",
      "ur": "sa kāśyapasyāyatanaṁ mahāvratair; vr̥taṁ samantād r̥ṣibhis tapodhanaiḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 42
  },
  "01064042c": {
    "text": {
      "ascii": "viveza sAmAtyapurohito 'rihA; viviktam atyarthamanoharaM zivam\n",
      "ud": "विवेश सामात्यपुरोहितोऽरिहा; विविक्तमत्यर्थमनोहरं शिवम्\n",
      "ur": "viveśa sāmātyapurohito ’rihā; viviktam atyarthamanoharaṁ śivam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 42
  }
}
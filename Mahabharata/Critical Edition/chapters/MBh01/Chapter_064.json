    "book": 1,
    "chapter": 64,
    "verse": 12
  },
  "01064013a": {
    "text": {
      "ascii": "sukhazItaH sugandhI ca puSpareNuvaho 'nilaH\n",
      "ud": "सुखशीतः सुगन्धी च पुष्परेणुवहोऽनिलः\n",
      "ur": "sukhaśītaḥ sugandhī ca puṣpareṇuvaho ’nilaḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 13
  },
  "01064013c": {
    "text": {
      "ascii": "parikrAman vane vRkSAn upaitIva riraMsayA\n",
      "ud": "परिक्रामन्वने वृक्षानुपैतीव रिरंसया\n",
      "ur": "parikrāman vane vr̥kṣān upaitīva riraṁsayā\n"

    "book": 1,
    "chapter": 64,
    "verse": 15
  },
  "01064015c": {
    "text": {
      "ascii": "AzramapravaraM ramyaM dadarza ca manoramam\n",
      "ud": "आश्रमप्रवरं रम्यं ददर्श च मनोरमम्\n",
      "ur": "āśramapravaraṁ ramyaṁ dadarśa ca manoramam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 15
  },
  "01064016a": {
    "text": {
      "ascii": "nAnAvRkSasamAkIrNaM saMprajvalitapAvakam\n",
      "ud": "नानावृक्षसमाकीर्णं संप्रज्वलितपावकम्\n",
      "ur": "nānāvr̥kṣasamākīrṇaṁ saṁprajvalitapāvakam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 16
  },
  "01064016c": {
    "text": {
      "ascii": "yatibhir vAlakhilyaiz ca vRtaM munigaNAnvitam\n",
      "ud": "यतिभिर्वालखिल्यैश्च वृतं मुनिगणान्वितम्\n",
      "ur": "yatibhir vālakhilyaiś ca vr̥taṁ munigaṇānvitam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 16
  },
  "01064017a": {
    "text": {
      "ascii": "agnyAgAraiz ca bahubhiH puSpasaMstarasaMstRtam\n",
      "ud": "अग्न्यागारैश्च बहुभिः पुष्पसंस्तरसंस्तृतम्\n",
      "ur": "agnyāgāraiś ca bahubhiḥ puṣpasaṁstarasaṁstr̥tam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 17
  },
  "01064017c": {
    "text": {
      "ascii": "mahAkacchair bRhadbhiz ca vibhrAjitam atIva ca\n",
      "ud": "महाकच्छैर्बृहद्भिश्च विभ्राजितमतीव च\n",
      "ur": "mahākacchair br̥hadbhiś ca vibhrājitam atīva ca\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 17
  },
  "01064018a": {
    "text": {
      "ascii": "mAlinIm abhito rAjan nadIM puNyAM sukhodakAm\n",
      "ud": "मालिनीमभितो राजन्नदीं पुण्यां सुखोदकाम्\n",
      "ur": "mālinīm abhito rājan nadīṁ puṇyāṁ sukhodakām\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 18
  },
  "01064018c": {
    "text": {
      "ascii": "naikapakSigaNAkIrNAM tapovanamanoramAm\n",
      "ud": "नैकपक्षिगणाकीर्णां तपोवनमनोरमाम्\n",
      "ur": "naikapakṣigaṇākīrṇāṁ tapovanamanoramām\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 18
  },
  "01064018e": {
    "text": {
      "ascii": "tatra vyAlamRgAn saumyAn pazyan prItim avApa saH\n",
      "ud": "तत्र व्यालमृगान्सौम्यान्पश्यन्प्रीतिमवाप सः\n",
      "ur": "tatra vyālamr̥gān saumyān paśyan prītim avāpa saḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 18
  },
  "01064019a": {
    "text": {
      "ascii": "taM cApy atirathaH zrImAn AzramaM pratyapadyata\n",
      "ud": "तं चाप्यतिरथः श्रीमानाश्रमं प्रत्यपद्यत\n",
      "ur": "taṁ cāpy atirathaḥ śrīmān āśramaṁ pratyapadyata\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 19
  },
  "01064019c": {
    "text": {
      "ascii": "devalokapratIkAzaM sarvataH sumanoharam\n",
      "ud": "देवलोकप्रतीकाशं सर्वतः सुमनोहरम्\n",
      "ur": "devalokapratīkāśaṁ sarvataḥ sumanoharam\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 19
  },
  "01064020a": {
    "text": {
      "ascii": "nadIm AzramasaMzliSTAM puNyatoyAM dadarza saH\n",
      "ud": "नदीमाश्रमसंश्लिष्टां पुण्यतोयां ददर्श सः\n",
      "ur": "nadīm āśramasaṁśliṣṭāṁ puṇyatoyāṁ dadarśa saḥ\n"
    },
    "book": 1,
    "chapter": 64,
    "verse": 20
  },
  "01064020c": {
    "text": {
      "ascii": "sarvaprANabhRtAM tatra jananIm iva viSThitAm\n",
      "ud": "सर्वप्राणभृतां तत्र जननीमिव विष्ठिताम्\n",

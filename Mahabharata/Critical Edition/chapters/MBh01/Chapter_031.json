{
  "01031001": {
    "text": {
      "ascii": "zaunaka uvAca\n",
      "ud": "शौनक उवाच\n",
      "ur": "śaunaka uvāca\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 1
  },
  "01031001a": {
    "text": {
      "ascii": "bhujaMgamAnAM zApasya mAtrA caiva sutena ca\n",
      "ud": "भुजंगमानां शापस्य मात्रा चैव सुतेन च\n",
      "ur": "bhujaṁgamānāṁ śāpasya mātrā caiva sutena ca\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 1
  },
  "01031001c": {
    "text": {
      "ascii": "vinatAyAs tvayA proktaM kAraNaM sUtanandana\n",
      "ud": "विनतायास्त्वया प्रोक्तं कारणं सूतनन्दन\n",
      "ur": "vinatāyās tvayā proktaṁ kāraṇaṁ sūtanandana\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 1
  },
  "01031002a": {
    "text": {
      "ascii": "varapradAnaM bhartrA ca kadrUvinatayos tathA\n",
      "ud": "वरप्रदानं भर्त्रा च कद्रूविनतयोस्तथा\n",
      "ur": "varapradānaṁ bhartrā ca kadrūvinatayos tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 2
  },
  "01031002c": {
    "text": {
      "ascii": "nAmanI caiva te prokte pakSiNor vainateyayoH\n",
      "ud": "नामनी चैव ते प्रोक्ते पक्षिणोर्वैनतेययोः\n",
      "ur": "nāmanī caiva te prokte pakṣiṇor vainateyayoḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 2
  },
  "01031003a": {
    "text": {
      "ascii": "pannagAnAM tu nAmAni na kIrtayasi sUtaja\n",
      "ud": "पन्नगानां तु नामानि न कीर्तयसि सूतज\n",
      "ur": "pannagānāṁ tu nāmāni na kīrtayasi sūtaja\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 3
  },
  "01031003c": {
    "text": {
      "ascii": "prAdhAnyenApi nAmAni zrotum icchAmahe vayam\n",
      "ud": "प्राधान्येनापि नामानि श्रोतुमिच्छामहे वयम्\n",
      "ur": "prādhānyenāpi nāmāni śrotum icchāmahe vayam\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 3
  },
  "01031004": {
    "text": {
      "ascii": "sUta uvAca\n",
      "ud": "सूत उवाच\n",
      "ur": "sūta uvāca\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 4
  },
  "01031004a": {
    "text": {
      "ascii": "bahutvAn nAmadheyAni bhujagAnAM tapodhana\n",
      "ud": "बहुत्वान्नामधेयानि भुजगानां तपोधन\n",
      "ur": "bahutvān nāmadheyāni bhujagānāṁ tapodhana\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 4
  },
  "01031004c": {
    "text": {
      "ascii": "na kIrtayiSye sarveSAM prAdhAnyena tu me zRNu\n",
      "ud": "न कीर्तयिष्ये सर्वेषां प्राधान्येन तु मे शृणु\n",
      "ur": "na kīrtayiṣye sarveṣāṁ prādhānyena tu me śr̥ṇu\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 4
  },
  "01031005a": {
    "text": {
      "ascii": "zeSaH prathamato jAto vAsukis tadanantaram\n",
      "ud": "शेषः प्रथमतो जातो वासुकिस्तदनन्तरम्\n",
      "ur": "śeṣaḥ prathamato jāto vāsukis tadanantaram\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 5
  },
  "01031005c": {
    "text": {
      "ascii": "airAvatas takSakaz ca karkoTakadhanaMjayau\n",
      "ud": "ऐरावतस्तक्षकश्च कर्कोटकधनंजयौ\n",
      "ur": "airāvatas takṣakaś ca karkoṭakadhanaṁjayau\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 5
  },
  "01031006a": {
    "text": {
      "ascii": "kAliyo maNinAgaz ca nAgaz cApUraNas tathA\n",
      "ud": "कालियो मणिनागश्च नागश्चापूरणस्तथा\n",
      "ur": "kāliyo maṇināgaś ca nāgaś cāpūraṇas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 6
  },
  "01031006c": {
    "text": {
      "ascii": "nAgas tathA piJjaraka elApatro 'tha vAmanaH\n",
      "ud": "नागस्तथा पिञ्जरक एलापत्रोऽथ वामनः\n",
      "ur": "nāgas tathā piñjaraka elāpatro ’tha vāmanaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 6
  },
  "01031007a": {
    "text": {
      "ascii": "nIlAnIlau tathA nAgau kalmASazabalau tathA\n",
      "ud": "नीलानीलौ तथा नागौ कल्माषशबलौ तथा\n",
      "ur": "nīlānīlau tathā nāgau kalmāṣaśabalau tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 7
  },
  "01031007c": {
    "text": {
      "ascii": "Aryakaz cAdikaz caiva nAgaz ca zalapotakaH\n",
      "ud": "आर्यकश्चादिकश्चैव नागश्च शलपोतकः\n",
      "ur": "āryakaś cādikaś caiva nāgaś ca śalapotakaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 7
  },
  "01031008a": {
    "text": {
      "ascii": "sumanomukho dadhimukhas tathA vimalapiNDakaH\n",
      "ud": "सुमनोमुखो दधिमुखस्तथा विमलपिण्डकः\n",
      "ur": "sumanomukho dadhimukhas tathā vimalapiṇḍakaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 8
  },
  "01031008c": {
    "text": {
      "ascii": "AptaH koTanakaz caiva zaGkho vAlazikhas tathA\n",
      "ud": "आप्तः कोटनकश्चैव शङ्खो वालशिखस्तथा\n",
      "ur": "āptaḥ koṭanakaś caiva śaṅkho vālaśikhas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 8
  },
  "01031009a": {
    "text": {
      "ascii": "niSThyUnako hemaguho nahuSaH piGgalas tathA\n",
      "ud": "निष्ठ्यूनको हेमगुहो नहुषः पिङ्गलस्तथा\n",
      "ur": "niṣṭhyūnako hemaguho nahuṣaḥ piṅgalas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 9
  },
  "01031009c": {
    "text": {
      "ascii": "bAhyakarNo hastipadas tathA mudgarapiNDakaH\n",
      "ud": "बाह्यकर्णो हस्तिपदस्तथा मुद्गरपिण्डकः\n",
      "ur": "bāhyakarṇo hastipadas tathā mudgarapiṇḍakaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 9
  },
  "01031010a": {
    "text": {
      "ascii": "kambalAzvatarau cApi nAgaH kAlIyakas tathA\n",
      "ud": "कम्बलाश्वतरौ चापि नागः कालीयकस्तथा\n",
      "ur": "kambalāśvatarau cāpi nāgaḥ kālīyakas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 10
  },
  "01031010c": {
    "text": {
      "ascii": "vRttasaMvartakau nAgau dvau ca padmAv iti zrutau\n",
      "ud": "वृत्तसंवर्तकौ नागौ द्वौ च पद्माविति श्रुतौ\n",
      "ur": "vr̥ttasaṁvartakau nāgau dvau ca padmāv iti śrutau\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 10
  },
  "01031011a": {
    "text": {
      "ascii": "nAgaH zaGkhanakaz caiva tathA ca sphaNDako 'paraH\n",
      "ud": "नागः शङ्खनकश्चैव तथा च स्फण्डकोऽपरः\n",
      "ur": "nāgaḥ śaṅkhanakaś caiva tathā ca sphaṇḍako ’paraḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 11
  },
  "01031011c": {
    "text": {
      "ascii": "kSemakaz ca mahAnAgo nAgaH piNDArakas tathA\n",
      "ud": "क्षेमकश्च महानागो नागः पिण्डारकस्तथा\n",
      "ur": "kṣemakaś ca mahānāgo nāgaḥ piṇḍārakas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 11
  },
  "01031012a": {
    "text": {
      "ascii": "karavIraH puSpadaMSTra eLako bilvapANDukaH\n",
      "ud": "करवीरः पुष्पदंष्ट्र एळको बिल्वपाण्डुकः\n",
      "ur": "karavīraḥ puṣpadaṁṣṭra eḷako bilvapāṇḍukaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 12
  },
  "01031012c": {
    "text": {
      "ascii": "mUSakAdaH zaGkhazirAH pUrNadaMSTro haridrakaH\n",
      "ud": "मूषकादः शङ्खशिराः पूर्णदंष्ट्रो हरिद्रकः\n",
      "ur": "mūṣakādaḥ śaṅkhaśirāḥ pūrṇadaṁṣṭro haridrakaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 12
  },
  "01031013a": {
    "text": {
      "ascii": "aparAjito jyotikaz ca pannagaH zrIvahas tathA\n",
      "ud": "अपराजितो ज्योतिकश्च पन्नगः श्रीवहस्तथा\n",
      "ur": "aparājito jyotikaś ca pannagaḥ śrīvahas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 13
  },
  "01031013c": {
    "text": {
      "ascii": "kauravyo dhRtarASTraz ca puSkaraH zalyakas tathA\n",
      "ud": "कौरव्यो धृतराष्ट्रश्च पुष्करः शल्यकस्तथा\n",
      "ur": "kauravyo dhr̥tarāṣṭraś ca puṣkaraḥ śalyakas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 13
  },
  "01031014a": {
    "text": {
      "ascii": "virajAz ca subAhuz ca zAlipiNDaz ca vIryavAn\n",
      "ud": "विरजाश्च सुबाहुश्च शालिपिण्डश्च वीर्यवान्\n",
      "ur": "virajāś ca subāhuś ca śālipiṇḍaś ca vīryavān\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 14
  },
  "01031014c": {
    "text": {
      "ascii": "hastibhadraH piTharako mukharaH koNavAsanaH\n",
      "ud": "हस्तिभद्रः पिठरको मुखरः कोणवासनः\n",
      "ur": "hastibhadraḥ piṭharako mukharaḥ koṇavāsanaḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 14
  },
  "01031015a": {
    "text": {
      "ascii": "kuJjaraH kuraraz caiva tathA nAgaH prabhAkaraH\n",
      "ud": "कुञ्जरः कुररश्चैव तथा नागः प्रभाकरः\n",
      "ur": "kuñjaraḥ kuraraś caiva tathā nāgaḥ prabhākaraḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 15
  },
  "01031015c": {
    "text": {
      "ascii": "kumudaH kumudAkSaz ca tittirir halikas tathA\n",
      "ud": "कुमुदः कुमुदाक्षश्च तित्तिरिर्हलिकस्तथा\n",
      "ur": "kumudaḥ kumudākṣaś ca tittirir halikas tathā\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 15
  },
  "01031015e": {
    "text": {
      "ascii": "karkarAkarkarau cobhau kuNDodaramahodarau\n",
      "ud": "कर्कराकर्करौ चोभौ कुण्डोदरमहोदरौ\n",
      "ur": "karkarākarkarau cobhau kuṇḍodaramahodarau\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 15
  },
  "01031016a": {
    "text": {
      "ascii": "ete prAdhAnyato nAgAH kIrtitA dvijasattama\n",
      "ud": "एते प्राधान्यतो नागाः कीर्तिता द्विजसत्तम\n",
      "ur": "ete prādhānyato nāgāḥ kīrtitā dvijasattama\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 16
  },
  "01031016c": {
    "text": {
      "ascii": "bahutvAn nAmadheyAnAm itare na prakIrtitAH\n",
      "ud": "बहुत्वान्नामधेयानामितरे न प्रकीर्तिताः\n",
      "ur": "bahutvān nāmadheyānām itare na prakīrtitāḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 16
  },
  "01031017a": {
    "text": {
      "ascii": "eteSAM prasavo yaz ca prasavasya ca saMtatiH\n",
      "ud": "एतेषां प्रसवो यश्च प्रसवस्य च संततिः\n",
      "ur": "eteṣāṁ prasavo yaś ca prasavasya ca saṁtatiḥ\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 17
  },
  "01031017c": {
    "text": {
      "ascii": "asaMkhyeyeti matvA tAn na bravImi dvijottama\n",
      "ud": "असंख्येयेति मत्वा तान्न ब्रवीमि द्विजोत्तम\n",
      "ur": "asaṁkhyeyeti matvā tān na bravīmi dvijottama\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 17
  },
  "01031018a": {
    "text": {
      "ascii": "bahUnIha sahasrANi prayutAny arbudAni ca\n",
      "ud": "बहूनीह सहस्राणि प्रयुतान्यर्बुदानि च\n",
      "ur": "bahūnīha sahasrāṇi prayutāny arbudāni ca\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 18
  },
  "01031018c": {
    "text": {
      "ascii": "azakyAny eva saMkhyAtuM bhujagAnAM tapodhana\n",
      "ud": "अशक्यान्येव संख्यातुं भुजगानां तपोधन\n",
      "ur": "aśakyāny eva saṁkhyātuṁ bhujagānāṁ tapodhana\n"
    },
    "book": 1,
    "chapter": 31,
    "verse": 18
  }
}